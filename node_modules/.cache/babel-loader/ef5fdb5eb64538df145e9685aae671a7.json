A,GAjBA;AAkBA,EAAA,OAAA,EAAA;AACA,IAAA,eADA,6BACA;AACA,WAAA,WAAA,GAAA,IAAA;AACA,UAAA,UAAA,GAAA,YAAA,CAAA,OAAA,CAAA,YAAA,CAAA;AACA,UAAA,eAAA,GAAA,EAAA;;AAEA,UAAA,UAAA,EAAA;AACA,QAAA,eAAA,CAAA,IAAA,CAAA,UAAA;AACA,OAFA,MAEA;AACA,YAAA,UAAA,GAAA,IAAA,CAAA,KAAA,CAAA,YAAA,CAAA,OAAA,CAAA,iBAAA,CAAA,CAAA;AACA,QAAA,UAAA,CAAA,GAAA,CAAA,UAAA,EAAA;AAAA,iBAAA,eAAA,CAAA,IAAA,CAAA,EAAA,CAAA,UAAA,CAAA;AAAA,SAAA;AACA;;AACA,UAAA,EAAA,GAAA,IAAA;AACA,MAAA,gBAAA,CAAA,kBAAA,CAAA,eAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,EAAA,CAAA,WAAA,GAAA,KAAA;AACA,QAAA,EAAA,CAAA,YAAA,GAAA,QAAA;AACA,OAJA,EAIA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OANA;AAOA,KApBA;AAqBA,IAAA,kBArBA,8BAqBA,QArBA,EAqBA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,UAAA,UAAA,GAAA,QAAA,CAAA,UAAA;AACA,MAAA,eAAA,CAAA,WAAA,CAAA,UAAA,EAAA,IAAA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,EAAA,CAAA,gBAAA,GAAA,QAAA;AACA,QAAA,EAAA,CAAA,gBAAA,CAAA,QAAA,GAAA,QAAA,CAAA,QAAA;AACA,YAAA,GAAA,2BAAA,EAAA,CAAA,gBAAA,CAAA,UAAA,CAAA;AACA,QAAA,EAAA,CAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA,OANA,EAOA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OATA;AAWA,KAnCA;AAoCA,IAAA,WApCA,uBAoCA,MApCA,EAoCA;AACA,UAAA,aAAA,GAAA,IAAA;AACA,UAAA,aAAA,GAAA,OAAA,EAAA;AACA,UAAA,WAAA,GAAA,OAAA,EAAA,GAAA,EAAA;AACA,UAAA,UAAA,GAAA,OAAA,EAAA,GAAA,EAAA,GAAA,EAAA;AAEA,UAAA,KAAA,GAAA,IAAA,CAAA,GAAA,EAAA;AACA,UAAA,eAAA;AAEA,UAAA,IAAA,GAAA,KAAA,GAAA,MAAA;AAEA,UAAA,OAAA,GAAA,QAAA,CAAA,IAAA,GAAA,aAAA,CAAA;;AAEA,UAAA,OAAA,GAAA,EAAA,EAAA;AACA,QAAA,eAAA,GAAA,OAAA,GAAA,SAAA,IAAA,OAAA,IAAA,CAAA,GAAA,EAAA,GAAA,GAAA,IAAA,MAAA;AAEA,OAHA,MAGA;AACA,YAAA,OAAA,GAAA,QAAA,CAAA,OAAA,GAAA,EAAA,CAAA;;AAEA,YAAA,OAAA,GAAA,EAAA,EAAA;AACA,UAAA,eAAA,GAAA,OAAA,GAAA,SAAA,IAAA,OAAA,IAAA,CAAA,GAAA,EAAA,GAAA,GAAA,IAAA,MAAA;AAEA,SAHA,MAGA;AACA,cAAA,KAAA,GAAA,QAAA,CAAA,OAAA,GAAA,EAAA,CAAA;;AAEA,cAAA,KAAA,GAAA,EAAA,EAAA;AACA,YAAA,eAAA,GAAA,KAAA,GAAA,OAAA,IAAA,KAAA,IAAA,CAAA,GAAA,EAAA,GAAA,GAAA,IAAA,MAAA;AAEA,WAHA,MAGA;AACA,gBAAA,IAAA,GAAA,QAAA,CAAA,KAAA,GAAA,EAAA,CAAA;AAEA,YAAA,eAAA,GAAA,IAAA,GAAA,MAAA,IAAA,IAAA,IAAA,CAAA,GAAA,EAAA,GAAA,GAAA,IAAA,MAAA;AACA;AACA;AACA;;AAEA,aAAA,eAAA;AACA;AAzEA;AAlBA,CAAA","sourcesContent":["<template>\n  <el-card shadow=\"always\" class=\"tall-card\">\n    <el-row>\n      <div class=\"o-card-title-1\">Recent Activity</div>\n      <div class=\"dashboard-tile-body\">\n        <div class=\"dashboard-tile-table\">\n          <div v-show=\"!showLoading\" class=\"dashboard-row cursor-default\">\n            <div class=\"dashboard-table-cell-0\">&nbsp;</div>\n            <div class=\"dashboard-table-cell-1\" style=\"font-size: 14px;\">NAME</div>\n            <div class=\"dashboard-table-cell-2\" style=\"font-size: 14px; color: #9A9A9A;\">ACTIVITY</div>\n            <div class=\"dashboard-table-cell-3\">&nbsp;</div>\n          </div>\n          <div v-show=\"!showLoading\" v-for=\"(interaction, index) in interactions\" :key=\"index\" class=\"dashboard-row feedback\" @click=\"openCustomerDetail(interaction)\">\n            <div class=\"dashboard-table-cell-0\"><img src=\"../../../src/assets/img/Opiniioncx_Icon.png\" height=\"25\"/></div>\n            <div class=\"dashboard-table-cell-1\">{{interaction.customerName}}</div>\n            <div class=\"dashboard-table-cell-2\">{{interaction.type}}</div>\n            <div class=\"dashboard-table-cell-3\">{{displayDate(interaction.date)}}</div>\n          </div>\n          <div class=\"dashboard-row no-feedback\" v-show=\"!showLoading && interactions.length == 0\">You haven't received any feedback.</div>\n          <div class=\"dashboard-row dashboard-loader\" v-show=\"showLoading\"><i class=\"far fa-hourglass fa-spin\"></i></div>\n        </div>\n      </div>\n    </el-row>\n  </el-card>\n</template>\n\n<script>\nimport DashboardService from './../../services/DashboardService';\nimport {EventService} from '../../main';\nimport CustomerService from \"./../../services/CustomerService\";\n\nexport default {\n  name: 'interactionFeed',\n  data: function() {\n    return {\n      interactions: [],\n      showLoading: true\n    }\n  },\n  created() {\n    let currentUser = JSON.parse(localStorage.getItem('currentUser'));\n    if(!currentUser.isSuperAdmin) {\n      this.getInteractions();\n      EventService.$on('locationChange', this.getInteractions);\n    }\n  },\n  beforeDestroy: function () {\n    EventService.$off('locationChange', this.getInteractions);\n  },\n  methods: {\n    getInteractions() {\n      this.showLoading = true;\n      var locationId = localStorage.getItem('locationId');\n      var locationIdArray = [];\n\n      if(locationId) {\n        locationIdArray.push(locationId);\n      } else {\n        let locOptions = JSON.parse(localStorage.getItem('locationOptions'));\n        locOptions.map(el => locationIdArray.push(el.locationId));\n      }\n      var vm = this;\n      DashboardService.getInteractionData(locationIdArray)\n        .then(function(response) {\n          vm.showLoading = false;\n          vm.interactions = response;\n        }, function(error) {\n          console.log(error);\n      });\n    },\n    openCustomerDetail(feedback) {\n      var vm = this;\n      let customerId = feedback.customerId;\n      CustomerService.getCustomer(customerId).then(\n        function(response) {\n          vm.selectedCustomer = response;\n          vm.selectedCustomer.location = feedback.location;\n          let url = `/customerList/${vm.selectedCustomer.customerId}`;\n          vm.$router.push(url);\n        },\n        function(error) {\n          console.log(error);\n        }\n      );\n    },\n    displayDate(millis) {\n      var millisASecond = 1000;\n      var millisAMinute = 1000 * 60;\n      var millisAHour = 1000 * 60 * 60;\n      var millisADay = 1000 * 60 * 60 * 24;\n\n      var today = Date.now();\n      var timeSinceReview;\n\n      var diff = today - millis;\n\n      var seconds = parseInt(diff / millisASecond);\n\n      if (seconds < 60) {\n        timeSinceReview = seconds + ' second' + (seconds == 1 ? \"\" : \"s\") + ' ago';\n\n      } else {\n        var minutes = parseInt(seconds / 60);\n\n        if (minutes < 60) {\n          timeSinceReview = minutes + ' minute' + (minutes == 1 ? \"\" : \"s\") + ' ago';\n\n        } else {\n          var hours = parseInt(minutes / 60);\n\n          if (hours < 24) {\n            timeSinceReview = hours + ' hour' + (hours == 1 ? \"\" : \"s\") + ' ago';\n\n          } else {\n            var days = parseInt(hours / 24);\n\n            timeSinceReview = days + ' day' + (days == 1 ? \"\" : \"s\") + ' ago';\n          }\n        }\n      }\n\n      return timeSinceReview;\n    }\n  }\n}\n</script>\n\n<style scoped>\n\n.dashboard-tile-table {\n  margin: 10px 0 20px;\n}\n\n.dashboard-row.no-feedback {\n  font-size: 24px;\n  padding: 20px;\n  color: gray;\n  text-align: center;\n  line-height: 30px;\n}\n\n.dashboard-row {\n  width: calc(100% - 20px);\n  width: 95%;\n  padding: 10px 0;\n  height: auto;\n  display: flex;\n  flex-wrap: nowrap;\n  cursor: pointer;\n  margin: 0 auto;\n  width: 95%;\n  justify-content: space-between;\n}\n\n.dashboard-row.feedback:hover {\n  background: #fafafa;\n}\n\n.dashboard-row.feedback:nth-child(1):hover {\n  background: rgba(0, 0, 0, 0);\n}\n\n\ndiv[class|=dashboard-table-cell] {\n  align-self: center;\n}\n\n.dashboard-table-cell-0 {\n  flex: 0 75px;\n  text-align: left;\n  align-self: center;\n}\n\n.dashboard-table-cell-1 {\n  flex: 1 0;\n  color: #9A9A9A;\n}\n\n.dashboard-table-cell-2 {\n  flex: 2 0;\n  text-align: left;\n  color: #9A9A9A;\n}\n\n.dashboard-table-cell-3 {\n  flex: 0 100px;\n  color: #9A9A9A;\n}\n\n.dashboard-table-cell-4 {\n  flex: 0 20px;\n}\n</style>"],"sourceRoot":"src/components/DashboardComponents"}]}