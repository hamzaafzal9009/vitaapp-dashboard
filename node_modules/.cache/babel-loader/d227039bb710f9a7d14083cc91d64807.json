CA,UAAA,YAAA,CAAA,KAAA,CAAA,gBAAA,EAAA,8CAAA;AACA,SAFA,MAEA;AACA,UAAA,QAAA,CAAA,cAAA,CAAA,KAAA,EAAA,QAAA,GAAA,CAAA,QAAA,CAAA,cAAA,CAAA,KAAA,EAAA,QAAA;AACA,UAAA,YAAA,CAAA,KAAA,CAAA,cAAA,EAAA,yCAAA;AACA;AACA,OAPA;AAQA,KA3EA;AA4EA,IAAA,gBAAA,EAAA,0BAAA,QAAA,EAAA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,MAAA,WAAA,CAAA,gBAAA,CAAA,KAAA,IAAA,CAAA,MAAA,EAAA,QAAA,CAAA,UAAA,EAAA,QAAA,CAAA,IAAA,EAAA,QAAA,CAAA,SAAA,EAAA,IAAA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,YAAA,CAAA,KAAA,CAAA,SAAA,EAAA,MAAA;AACA,QAAA,YAAA,CAAA,KAAA,CACA,gBADA,EAEA,2CAFA;AAIA,OAPA,EAOA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA;AACA,UAAA,YAAA,CAAA,KAAA,CAAA,SAAA,EAAA,MAAA;AACA,UAAA,YAAA,CAAA,KAAA,CACA,cADA,EAEA,gDAFA;AAIA;AACA,OAfA;AAgBA;AA9FA;AAnBA,CAAA","sourcesContent":["<template>\n    <div class='user-container'>\n    <div class=\"test\"></div>\n      <div class=\"lower-container\">\n        <div class=\"column-left\">\n          <div class=\"user-image\" :src=\"userImage\" :style=\"{'background-image': userImageBackground}\"  />\n          <div class=\"modal-file-import\">\n            <input ref=\"imageUpload\" type=\"file\" accept=\"image/*\" placeholder=\"Image\" @change=\"userImageChange()\" required />\n          </div>\n          <span class=\"image-button\" @click=\"$refs.imageUpload.click()\">Choose Profile Image</span>\n          <h6>Please upload jpg, jpeg, or png file types only.</h6>\n        </div>\n        <div class=\"column-right\">\n          <div class=\"user-details\">\n            <div class=\"profile-details\">\n              <h3>Profile Details</h3>\n            </div>\n            <div class=\"edit-details\">\n              <div class=\"sub-column-left\">\n                <span>First Name: </span>\n                <span>Last Name: </span>\n                <span>Email: </span>\n                <span>Mobile: </span>\n              </div>\n              <div class=\"sub-column-right\">\n                <input v-model=\"user.firstName\" @change=\"changes=true\" />\n                <input v-model=\"user.lastName\" @change=\"changes=true\" />\n                <span>{{user.email}}</span>\n                <input v-model=\"user.phone\" @change=\"changes=true\" />\n              </div>\n            </div>\n            <div class=\"button-container\">\n              <button v-if=\"changes\" @click.once=\"saveUser()\">Save</button>\n              <button @click=\"followLink('/resetPassword')\">Reset Password</button>\n            </div>\n          </div>\n        </div>\n      </div>\n      <div class=\"locations-list\">\n        <h3>Assigned Locations</h3>\n        <div class=\"user-assign-header-row\">\n          <div class=\"user-assign-table-cell-0\" >Location Name</div>\n          <div class=\"user-assign-table-cell-1\">Receive Location Emails</div>\n        </div>\n        <div class=\"user-assign-row\" v-for=\"location in userLocations\">\n          <div class=\"user-assign-table-cell-0\">{{location.name}}</div>\n            <div class=\"assignment-row\" v-if=\"location.communications && location.role\" v-for=\"(coms, comIndex) in location.communications\">\n              <span class=\"location-tag\" :class=\"{'active': coms.receives == true}\" @click=\"changeCommunication(location, comIndex)\">{{coms.type}}</span>\n            </div>\n        </div>\n      </div>\n    </div>\n</template>\n\n<script>\nimport { EventService } from \"../main\";\nimport UserService from \"./../services/UserService\";\nimport imageFile from '../assets/imageFile.js';\n\nexport default {\n  name: \"userProfile\",\n  data: function() {\n    return {\n      user: {\n        userImage: ''\n      },\n      userImage: \"\",\n      changes: false,\n      showLogoSizeError: false,\n      userImageBackground: '',\n      userLocations: []\n    };\n  },\n  created() {\n    this.getUser();\n    let imgText = imageFile.imgBlock();\n    this.userImageBackground = 'url(\"data:image/png;base64,' + imgText;\n  },\n  methods: {\n    getUser() {\n      this.user = JSON.parse(localStorage.getItem('currentUser'));\n      if(this.user.userImage) {\n        if(this.user.userImage[0] == ',') {\n          this.setUserImageBackground('data:image/png;base64' + this.user.userImage)\n        } else {\n          this.setUserImageBackground('data:image/png;base64,' + this.user.userImage)\n        }\n      }\n      UserService.getUserLocations(this.user.userId).then(response => {\n        this.userLocations = response.results;\n      })\n    },\n    userImageChange: function() {\n      var element = this.$refs.imageUpload;\n      var vm = this;\n      this.showLogoSizeError = false;\n      this.userImageBackground = {};\n      if (element.files && element.files[0]) {\n        var reader = new FileReader();\n        reader.onloadend = function () {\n          if(reader.result.length > 500000 * 1.36) {\n            vm.showLogoSizeError = true;\n          } else {\n            vm.user.userImage = reader.result.substring(22);\n            vm.setUserImageBackground(reader.result);\n          }\n        };\n        reader.readAsDataURL(element.files[0]);\n      }\n      this.changes = true;\n    },\n    setUserImageBackground: function(rawData) {\n      this.userImageBackground = \"url('\" + rawData + \"')\";\n    },\n    followLink(link) {\n      this.$router.push(link);\n    },\n    saveUser() {\n      EventService.$emit(\"loading\", \"show\");\n      let vm = this;\n      UserService.saveUser(vm.user).then(\n        function(response) {\n          if (response) {\n            vm.changes = false;\n            EventService.$emit(\"loading\", \"hide\");\n            EventService.$emit(\n              \"successMessage\",\n              \"Success, your user info has been updated.\"\n            );\n          }\n        },\n        function(error) {\n          if (error) {\n            EventService.$emit(\"loading\", \"hide\");\n            EventService.$emit(\n              \"errorMessage\",\n              \"Sorry, something went wrong. Please try again.\"\n            );\n          }\n        }\n      );\n    },\n    changeCommunication(location, index){\n      let vm = this;\n      if(index >= 0) location.communications[index].receives = !location.communications[index].receives;\n      UserService.saveUserComms(location, vm.user).then(response => {\n        if(response.success) {\n          EventService.$emit('successMessage', 'Communication Preferences Have Been Updated!')\n        } else {\n          location.communications[index].receives = !location.communications[index].receives;\n          EventService.$emit('errorMessage', 'Something went wrong, pleast try again.')\n        }      \n      })\n    },\n    saveUserLocation: function(location) {\n        var vm = this;\n        UserService.saveUserLocation(this.user.userId, location.locationId, location.role, location.getsEmail).then(\n          function(response) {\n            EventService.$emit(\"loading\", \"hide\");\n            EventService.$emit(\n              \"successMessage\",\n              \"Success, your user info has been updated.\"\n            );\n          }, function(error) {\n            if (error) {\n              EventService.$emit(\"loading\", \"hide\");\n              EventService.$emit(\n                \"errorMessage\",\n                \"Sorry, something went wrong. Please try again.\"\n              );\n            }\n        });\n      }\n    }\n};\n</script>\n\n\n<style scoped>\n.user-container {\n  background: white;\n  box-shadow: 0px 3px 3px rgba(0, 0, 0, 0.25);\n  padding: 20px;\n  margin-top: 0px;\n}\n\n.test {\n  transform: translateY(-30px);\n  padding-top: 10px;\n}\n.test::before {\n  content: '';\n  width: 0;\n  height: 0;\n  border-bottom: 30px solid white;\n  border-right: 30px solid transparent;\n  position: absolute;\n  transform: translateX(130px);\n  z-index: 999;\n}\n\n.test::after {\n  position: absolute;\n  content: 'PROFILE INFO';\n  border-right: calc(100vw - 231px) solid #efefef;\n  width: 120px;\n  padding: 0;\n  margin: 0;\n  margin-left: 50px;\n  margin-right: 10px;\n  padding-top: 10px;\n  padding-right: 10px;\n  z-index: -1;\n  transform: translateX(-50px);\n}\n\n.lower-container {\n  display: flex;\n  flex-direction: row-reverse;\n}\n\n.modal-file-import {\n  display: none;\n}\n\n.user-details {\n  box-shadow: 0px 2px 8px rgba(0, 0, 0, 0.25);\n  border-radius: 3px;\n}\n\n.edit-details {\n  display: flex;\n  /* flex-direction: column; */\n}\n\n.column-left {\n  margin-left: 12vw;\n  margin-top: 150px;\n  width: 45%;\n  display: flex;\n  flex-direction: column;\n}\n\n.sub-column-left {\n  display: flex;\n  flex-direction: column;\n  margin-left: 20px;\n}\n.sub-column-left span {\n  margin-top: 30px;\n}\n\n.sub-column-right {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  margin-left: 20px;\n}\n\n.sub-column-right input {\n  margin-top: 12px;\n  margin-left: 70px;\n}\n\n.sub-column-right span {\n  margin-top: 10px;\n  margin-left: 70px;\n}\n\n\n.column-right {\n  margin: 30px 50px 70px 20px;\n  min-width: 50%;\n}\n\n.profile-details {\n  border-bottom: 3px solid #4fcbc5;\n  width: 100%;\n  display: flex;\n  justify-content: center;\n}\n\n.user-details input {\n  position: relative;\n  right: 0;\n}\n\n.user-image {\n  border: 1px solid grey;\n  border-radius: 50%;\n  height: 200px;\n  width: 200px;\n  background-repeat: no-repeat;\n  background-size: cover;\n}\n\n.image-button {\n  background: #4fcbc5;\n  padding: 5px 10px;\n  color: white;\n  border-radius: 5px;\n  width: 150px;\n  display: flex;\n  justify-content: center;\n  margin-left: 10px;\n  margin-top: 20px;\n  cursor: pointer;\n}\n\n.locations-list {\n  box-shadow: 0px 2px 8px rgba(0, 0, 0, 0.25);\n  width: 50%;\n  margin: 0px 0px 0px 20px;\n  display: flex;\n  justify-content: center;\n  flex-direction: column;\n  clear: both;\n  transform:translateY(-50px);\n}\n\n.locations-list h3 {\n  margin-left: 50%;\n  transform: translateX(-25%);\n}\n\n.user-assign-header-row {\n  width: calc(100% - 40px);\n  font-weight: bold !important;\n  padding: 10px 20px;\n  border-top: 3px solid #4fcbc5;\n  border-bottom: 1px solid #F2F4F8;\n  height: 14px;\n  display: flex;\n  flex-wrap: nowrap;\n  justify-content: space-evenly;\n  cursor: pointer;\n}\n\n.user-assign-row {\n  width: calc(100% - 40px);\n  clear: both;\n  padding: 10px 20px;\n  border-bottom: 1px solid #F2F4F8;\n  height: auto;\n  display: flex;\n  flex-wrap: nowrap;\n  justify-content: space-evenly;\n}\n\n.user-assign-row:hover {\n  background: #F2F4F8;\n}\n\n.user-assign-table-cell-0 {\n  flex: 1 1 200px;\n}\n\n.user-assign-table-cell-1 {\n  flex: 0 0 225px;\n  margin-right: 75px;\n}\n\n.assignment-row {\n  text-align: center;\n  display: flex;\n}\n\n.location-tag {\n  font-size: 12px;\n  margin-right: 5px;\n  cursor: pointer;\n  border: 2px solid #4fcbc5;\n  border-radius: 10px;\n  padding: 2px 5px;\n}\n\n.location-tag:hover {\n  background: #efefef;\n}\n\n.active {\n  background: #4fcbc5;\n  color: white;\n}\n\n.active:hover {\n  background: #4fcbc5aa;\n}\n\n.button-container {\n  margin-top: 10px;\n  display: flex;\n  justify-content: center;\n  margin-bottom: 10px;\n}\n\nbutton {\n  padding: 10px 20px;\n  font-size: 14px;\n  border-radius: 3px;\n  cursor: pointer;\n  background: #f48d46;\n  margin: 0px 10px 10px;\n  box-shadow: 0px 2px 4px rgba(0, 0, 0, 0.25);\n  color: white;\n}\n</style>\n"],"sourceRoot":"src/components"}]}