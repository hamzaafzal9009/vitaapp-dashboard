s=\"feedback-cell-data-5\" v-if=\"feedback.rating == 3\">\n                <i class=\"fa fa-star\"></i>\n                <i class=\"fa fa-star\"></i>\n                <i class=\"fa fa-star\"></i>\n              </div>\n              <div class=\"feedback-cell-data-5\" v-if=\"feedback.rating == 4\">\n                <i class=\"fa fa-star\"></i>\n                <i class=\"fa fa-star\"></i>\n                <i class=\"fa fa-star\"></i>\n                <i class=\"fa fa-star\"></i>\n              </div>\n              <div class=\"feedback-cell-data-5\" v-if=\"feedback.rating == 5\">\n                <i class=\"fa fa-star\"></i>\n                <i class=\"fa fa-star\"></i>\n                <i class=\"fa fa-star\"></i>\n                <i class=\"fa fa-star\"></i>\n                <i class=\"fa fa-star\"></i>\n              </div>\n            </div>\n            <img :src=\"getIconForSource(feedback.source)\" height=\"35\"/>\n          </div>\n          <div class=\"modal-customer-message\" v-if=\"feedback.reviewText\">\n            <label>Review</label>\n            {{feedback.reviewText || \"No feedback left\"}}\n          </div>\n          <div class=\"customers-match-container\">\n            <div class=\"customer-sear\">\n\n            </div>\n            <div class=\"suggested-customers\">\n              <!-- <div v-for=\"suggested in suggestedCustomers\">\n\n              </div> -->\n            </div>\n          </div>\n        </div>\n        <div class=\"modal-footer text-right\">\n          <span class=\"modal-default-close\" @click=\"closeModal()\">\n            Close\n          </span>\n          <button class=\"modal-default-button\" @click.once=\"matchFeedback()\" >\n            Match Customer\n          </button>\n        </div>\n      </div>\n    </div>\n  </transition>\n</template>\n\n<script>\nimport FeedbackService from \"./../services/FeedbackService\";\nimport CustomerService from \"./../services/CustomerService\";\nimport { EventService } from \"../main\";\n\nexport default {\n  name: \"feedbackMatch\",\n  props: [\"selectedFeedback\"],\n\n  data: function() {\n    return {\n      feedback: {\n        source: \"\",\n        date: null,\n        name: '',\n        rating: null,\n        status: '',\n      },\n      suggestedCustomers: [],\n      selectedCustomer: null,\n      customerForSearching: {\n        firstName: '',\n        lastName: ''\n      }\n    };\n  },\n  created() {\n    this.feedback = this.selectedFeedback;\n    if(this.feedback.name) {\n      let splitName = this.feedback.name.split(' ');\n      this.customerForSearching.firstName = splitName[0];\n      if(splitName.length > 1) this.customerForSearching.lastName = splitName[1];\n      this.searchCustomers();\n    }\n  },\n  methods: {\n    searchCustomers() {\n      CustomerService.findPossibleCustomerMatches(feedback, customerForSearching).then(res => {\n        console.log(res);\n      })\n    },\n    matchFeedback() {\n\n    },\n    changeFeedbackStatus() {\n\n    },\n    closeModal: function() {\n      this.$emit(\"closeModal\");\n      this.user = {};\n      EventService.$emit(\"loading\", \"hide\");\n    },\n    getIconForSource(source) {\n      if (source == \"Google\") {\n        return \"./img/Google_Icon.png\";\n      } else if (source == \"Yelp\") {\n        return \"./img/Yelp_Icon.png\";\n      } else if (source == \"Facebook\") {\n        return \"./img/Facebook_Icon.png\";\n      } else if (source == \"Foursquare\") {\n        return \"./img/Foursquare_Icon.png\";\n      } else if (source == \"Citysearch\") {\n        return \"./img/Citysearch_Icon.png\";\n      } else if (source == \"Yellowpages\") {\n        return \"./img/YP_Icon.png\";\n        // } else if (source == 'TripAdvisor') {\n        //   return './img/TripAdvisor_Icon.png';\n        // } else if (source == 'BBB') {\n        //   return './img/BBB_Icon.png';\n      } else {\n        return \"./img/Opiniion_Icon.png\";\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n.modal-container {\n  width: 550px;\n  margin: 125px auto 0;\n  padding: 20px 30px;\n  background-color: #fff;\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.33);\n  transition: all 0.3s ease;\n  position: sticky;\n  top: 40px;\n}\n\n.modal-header {\n  display: flex;\n  justify-content: space-between;\n  border-bottom: 3px solid lightgrey;\n  padding: 0 10px;\n}\n\n.modal-header span {\n  margin: 15px 0 0 0;\n  font-size: 25px;\n  color: #696969;\n  cursor: pointer;\n}\n\n.modal-body {\n  margin-top: 20px;\n  display: flex;\n  flex-direction: column;\n}\n\n.modal-feedback-detail {\n  display: flex;\n  justify-content: space-between;\n  margin: 5px 20px;\n}\n\n.fa-star {\n  color: #4fcbc5;\n}\n\n.modal-customer-message {\n  margin: 10px 20px;\n}\n\n.modal-feedback-response {\n  margin-top: 10px;\n  border-top: 3px solid lightgrey;\n  display: flex;\n  flex-direction: column;\n}\n\n.modal-feedback-response label {\n  margin: 5px 0;\n}\n\n.modal-feedback-response textarea {\n  max-width: 100%;\n  height: 100px;\n  max-height: 200px;\n  margin-top: 10px;\n}\n\n.modal-feedback-response span {\n  font-size: 11px;\n  color: #4fcbc5;\n}\n\n.method-options {\n  text-transform: capitalize;\n}\n\n.modal-footer {\n  clear: both;\n  text-align: right;\n  margin-top: 20px;\n}\n\n.modal-default-close {\n  cursor: pointer;\n  color: #4fcbc5;\n  font-size: 14px;\n}\n\nbutton {\n  background: #4fcbc5;\n  border-radius: 3px;\n  margin: 0 30px 0 10px;\n  padding: 10px 80px;\n  cursor: pointer;\n  color: white;\n}\n</style>\n"],"sourceRoot":"src/components"}]}