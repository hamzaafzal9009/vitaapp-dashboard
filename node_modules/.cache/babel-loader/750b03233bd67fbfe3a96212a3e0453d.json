> -->\n\n    <footer class=\"page-footer\">\n      This is the footer which will contain all the specific page elements that are required by every business because originality is dead.\n      <div class=\"ul-container\">\n      <ul>\n        <li>Link 1</li>\n        <li>Link 1</li>\n        <li>Link 1</li>\n        <li>Link 1</li>\n        <li>Link 1</li>\n        <li>Link 1</li>\n      </ul>\n      <ul>\n        <li>Link 2</li>\n        <li>Link 2</li>\n        <li>Link 2</li>\n        <li>Link 2</li>\n        <li>Link 2</li>\n        <li>Link 2</li>\n      </ul>\n      <ul>\n        <li>Link 3</li>\n        <li>Link 3</li>\n        <li>Link 3</li>\n        <li>Link 3</li>\n        <li>Link 3</li>\n        <li>Link 3</li>\n      </ul>\n      </div>\n    </footer>\n  </div>\n</template>\n\n<script>\nimport SharedFunctions from '../services/SharedFunctions.js';\n\nexport default {\n  name: \"reviewSite\",\n  data: function() {\n    return {\n      businessData: '',\n      businessCandidates: '',\n    };\n  },\n  created() {},\n  methods: {\n    searchForBusinesses() {\n      if(this.businessData ) {\n        SharedFunctions.gatherCandidates(this.businessData).then(response => {\n          this.businessCandidates = response;\n          this.getBusinessesAfterSearch(response.candidates);\n        })\n      }\n    },\n    getBusinessesAfterSearch(data) {\n      let bdata = [];\n      let businesses = []\n      SharedFunctions.getBusinessInfo(data).then(response => {\n        bdata = response;\n        // console.log(bdata);      \n      })\n      bdata.map((e,i) => {\n        businesses.push(e)        \n      })\n      console.log(businesses);\n    }\n  }\n};\n</script>\n\n<style scoped>\n.site-container {\n  display: flex;\n  flex-direction: column;\n  /* justify-content: center; */\n  /* height: 100vh; */\n  /* border: 1px solid #545454; */\n  border-radius: 5px;\n  box-shadow: 0px 2px 8px rgba(0,0,0, .25);\n}\n\n.navigation-bar {\n  position: sticky;\n  top: 0px;\n  