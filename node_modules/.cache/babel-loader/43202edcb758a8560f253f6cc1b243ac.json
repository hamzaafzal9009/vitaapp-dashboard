A,GAjEA;AAkEA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,wBACA;AACA,MAAA,YAAA,CAAA,KAAA,CAAA,SAAA,EAAA,MAAA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,WAAA,UAAA,GAAA,YAAA,CAAA,OAAA,CAAA,YAAA,CAAA;AACA,MAAA,YAAA,CAAA,UAAA,CAAA,KAAA,QAAA,EAAA,IAAA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,EAAA,CAAA,OAAA,GAAA,QAAA;AACA,QAAA,YAAA,CAAA,KAAA,CAAA,SAAA,EAAA,MAAA;AACA,OAJA,EAIA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,QAAA,YAAA,CAAA,KAAA,CAAA,SAAA,EAAA,MAAA;AACA,OAPA;AAQA,KAbA;AAcA,IAAA,WAdA,uBAcA,MAdA,EAcA;AACA,UAAA,IAAA,GAAA,IAAA,IAAA,CAAA,MAAA,CAAA;AACA,aAAA,IAAA,IAAA,CAAA,QAAA,EAAA,GAAA,GAAA,GAAA,IAAA,CAAA,OAAA,EAAA,GAAA,GAAA,GAAA,IAAA,CAAA,WAAA,KAAA,GAAA;AACA,KAjBA;AAkBA,IAAA,kBAlBA,8BAkBA,UAlBA,EAkBA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,MAAA,eAAA,CAAA,WAAA,CAAA,UAAA,EAAA,IAAA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,EAAA,CAAA,gBAAA,GAAA,QAAA;AACA,QAAA,EAAA,CAAA,uBAAA,GAAA,IAAA;AACA,OAJA,EAIA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OANA;AAOA,KA3BA;AA4BA,IAAA,UA5BA,sBA4BA,WA5BA,EA4BA;AACA,UAAA,KAAA,QAAA,IAAA,WAAA,EAAA;AACA,aAAA,QAAA,GAAA,WAAA;AACA,OAFA,MAEA;AACA,aAAA,QAAA,GAAA,MAAA,KAAA,QAAA;AACA;AACA,KAlCA;AAmCA,IAAA,WAnCA,yBAmCA;AACA,UAAA,KAAA,QAAA,EAAA;AACA,aAAA,UAAA,CAAA,MAAA;AACA,OAFA,MAEA;AACA,aAAA,UAAA,CAAA,OAAA;AACA;;AACA,WAAA,UAAA;AACA,KA1CA;AA2CA,IAAA,kBA3CA,gCA2CA;AACA,WAAA,uBAAA,GAAA,KAAA;AACA,WAAA,gBAAA,GAAA,IAAA;AACA;AA9CA;AAlEA,CAAA","sourcesContent":["<template>\n  <div>\n    <div class=\"sms-header-top\">\n      <div class=\"smslog-controls-toggle\">\n        <span>Get queued texts? <input type=\"checkbox\" v-model=\"getQueue\" @change=\"queueToggle()\"/></span>\n      </div>\n      <div class=\"smslog-controls-search\">\n        <input v-model=\"searchText\" placeholder=\"Filter SMS...\"/>\n        <span class=\"smslog-controls-search-icon\">\n          <i class=\"fas fa-search\"></i>\n        </span>\n      </div>\n    </div>\n    <div class=\"smslog-table\">\n      <div class=\"smslog-header\" style=\"font-family: FuturaPTDemi;font-weight: bolder;\">\n        <div class=\"smslog-cell-0\" v-if=\"!locationId\">Location</div>\n        <div class=\"smslog-cell-1\" style=\"cursor: pointer;\" @click=\"toggleSort('date')\">\n          <span>Date </span>\n          <span v-if=\"sortType == '-date'\"><i class=\"fas fa-sort-up\"></i></span>\n          <span v-if=\"sortType == 'date'\"><i class=\"fas fa-sort-down\"></i></span>\n        </div>\n        <div class=\"smslog-cell-2\" style=\"cursor: pointer;\" @click=\"toggleSort('number')\">\n          <span>Number </span>\n          <span v-if=\"sortType == '-number'\"><i class=\"fas fa-sort-up\"></i></span>\n          <span v-if=\"sortType == 'number'\"><i class=\"fas fa-sort-down\"></i></span>\n        </div>\n        <div class=\"smslog-cell-3\" style=\"cursor: pointer;\" @click=\"toggleSort('name')\">\n          <span>Name </span>\n          <span v-if=\"sortType == '-name'\"><i class=\"fas fa-sort-up\"></i></span>\n          <span v-if=\"sortType == 'name'\"><i class=\"fas fa-sort-down\"></i></span>\n        </div>\n        <div class=\"smslog-cell-4\" style=\"cursor: pointer;\" @click=\"toggleSort('text')\">\n          <span>Text </span>\n          <span v-if=\"sortType == '-text'\"><i class=\"fas fa-sort-up\"></i></span>\n          <span v-if=\"sortType == 'text'\"><i class=\"fas fa-sort-down\"></i></span>\n        </div>\n        <div class=\"smslog-cell-5\" style=\"cursor: pointer;\" @click=\"toggleSort('status')\">\n          <span>Status </span>\n          <span v-if=\"sortType == '-status'\"><i class=\"fas fa-sort-up\"></i></span>\n          <span v-if=\"sortType == 'status'\"><i class=\"fas fa-sort-down\"></i></span>\n        </div>\n        <!-- <div class=\"smslog-cell-6\">Actions</div> -->\n      </div>\n      <div class=\"smslog-row\" v-for=\"smslog in filteredLogs\">\n        <div class=\"smslog-cell-0\" v-if=\"!locationId\">{{smslog.locationName}}</div>\n        <div class=\"smslog-cell-1\">{{displayDate(smslog.date)}}</div>\n        <div class=\"smslog-cell-2\">{{smslog.number}}</div>\n        <div class=\"smslog-cell-3\"><span class=\"underline\" @click=\"openCustomerDetail(smslog.customerId)\">{{smslog.name}}</span></div>\n        <div class=\"smslog-cell-4\">{{smslog.text}}</div>\n        <div class=\"smslog-cell-5\">{{smslog.status}}</div>\n        <!-- <div class=\"smslog-cell-6\"></div> -->\n      </div>\n      <div class=\"smslog-row empty\" v-if=\"smsList.length == 0\">No SMS logs match your filters.</div>\n    </div>\n    <customerDetail :selectedCustomer=\"selectedCustomer\" v-if=\"openCustomerDetailModal\" v-on:closeModal=\"closeCustomerModal()\"/>\n  </div>\n</template>\n\n<script>\nimport customerDetail from './CustomerDetail'\nimport CustomerService from './../services/CustomerService'\nimport CommsService from './../services/CommsService'\nimport {EventService} from '../main'\n\nexport default {\n  name: 'smslog',\n  components: {\n    customerDetail\n  },\n  data: function() {\n    return {\n      smsList: [],\n      locationId: null,\n      openCustomerDetailModal: false,\n      selectedCustomer: null,\n      searchText: '',\n      sortType: '-date',\n      getQueue: false\n    }\n  },\n  computed: {\n    filteredLogs: function() {\n      var vm = this;\n      var filteredList = this.smsList.filter(function(smslog) {\n        var smslogMatch = false;\n        if (!vm.locationId) smslogMatch = smslogMatch || smslog.locationName.toLowerCase().indexOf(vm.searchText.toLowerCase()) > -1;\n        smslogMatch = smslogMatch || smslog.number.indexOf(vm.searchText.toLowerCase()) > -1;\n        smslogMatch = smslogMatch || smslog.name.toLowerCase().indexOf(vm.searchText.toLowerCase()) > -1;\n        smslogMatch = smslogMatch || smslog.text.toLowerCase().indexOf(vm.searchText.toLowerCase()) > -1;\n        smslogMatch = smslogMatch || smslog.status.toLowerCase().indexOf(vm.searchText.toLowerCase()) > -1;\n\n        return smslogMatch;\n      });\n\n      var filteredSortedList = filteredList.sort((a,b) => {\n          if (this.sortType == 'date') {\n          return a.date - b.date;\n        } else if (this.sortType == '-date') {\n          return b.date - a.date;\n        } else if (this.sortType == 'number') {\n          return a.number.localeCompare(b.number);\n        } else if (this.sortType == '-number') {\n          return b.number.localeCompare(a.number);\n        } else if (this.sortType == 'name') {\n          return a.name.localeCompare(b.name);\n        } else if (this.sortType == '-name') {\n          return b.name.localeCompare(a.name);\n        } else if (this.sortType == 'text') {\n          return a.text.localeCompare(b.text);\n        } else if (this.sortType == '-text') {\n          return b.text.localeCompare(a.text);\n        } else if (this.sortType == 'status') {\n          return a.status.localeCompare(b.status);\n        } else if (this.sortType == '-status') {\n          return b.status.localeCompare(a.status);\n        } else {\n          true;\n        }\n      });\n\n      return filteredSortedList;\n    }\n  },\n  created() {\n    this.getSMSData();\n    EventService.$on('locationChange', this.getSMSData);\n  },\n  beforeDestroy: function () {\n    EventService.$off('locationChange', this.getSMSData);\n  },\n  methods: {\n    getSMSData() {\n      EventService.$emit(\"loading\", \"show\");      \n      var vm = this;\n      this.locationId = localStorage.getItem(\"locationId\");\n      CommsService.getSMSLogs(this.getQueue).then(\n        function(response) {\n          vm.smsList = response;\n          EventService.$emit(\"loading\", \"hide\");\n        }, function(error) {\n          console.log(error);\n          EventService.$emit(\"loading\", \"hide\");\n      });\n    },\n    displayDate(millis) {\n      var date = new Date(millis);\n      return (1 + date.getMonth()) + '/' + date.getDate() + '/' + (date.getFullYear() % 100);\n    },\n    openCustomerDetail(customerId) {\n      var vm = this;\n      CustomerService.getCustomer(customerId).then(\n        function(response) {\n          vm.selectedCustomer = response;\n          vm.openCustomerDetailModal = true;\n        }, function(error) {\n          console.log(error);\n      });\n    },\n    toggleSort(setSortType) {\n      if (this.sortType != setSortType) {\n        this.sortType = setSortType;\n      } else {\n        this.sortType = '-' + this.sortType;\n      }\n    },\n    queueToggle() {\n      if (this.getQueue) {\n        this.toggleSort('date');\n      } else {\n        this.toggleSort('-date');\n      }\n      this.getSMSData();\n    },\n    closeCustomerModal() {\n      this.openCustomerDetailModal = false;\n      this.selectedCustomer = null;\n    }\n  }\n}\n</script>\n\n<style scoped>\n\n.sms-header-top {\n  display: flex;\n  justify-content: space-between;\n  background: #fff;\n  box-shadow: 0px 2px 4px rgba(0,0,0,.25);\n  border-radius: 1px;\n  margin-bottom: 20px;\n  align-items: center;\n}\n\n.smslog-controls-toggle {\n  background: transparent;\n  /* float: left; */\n  padding: 15px 15px 0px 15px;\n  margin: 0 0 10px 20px;\n  color: #545454;\n  font-size: 16px;\n  height: 38px;\n}\n\n.smslog-controls-search {\n  background: white;\n  margin: 20px 20px;\n  border: 2px solid #efefef;\n  border-radius: 1px;\n  width: 350px;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.smslog-controls-search input {\n  border: none;\n  padding: 15px 15px 5px 15px;\n  font-size: 12px;\n}\n\n.smslog-controls-search-icon {\n  vertical-align: middle;\n  margin-right: 15px;\n  color: #4fcbc5;\n  font-size: 16px;\n}\n\n.smslog-table {\n  background: white;\n  font-size: 14px;\n  padding: 10px 0px;\n  clear: both;\n  box-shadow: 0px 2px 4px rgba(0,0,0,.25);\n  border-radius: 1px;\n}\n\n.smslog-header {\n  width: calc(100% - 20px);\n  font-weight: bold;\n  padding: 10px 0px 20px 20px;\n  /* border-top: 1px solid #efefef; */\n  border-bottom: 3px solid #efefef;\n  height: 14px;\n  display: flex;\n  flex-wrap: nowrap;\n  justify-content: space-evenly;\n  cursor: pointer;\n}\n\n.smslog-header div, .smslog-row div {\n  margin: auto;\n}\n\n.smslog-row {\n  width: 97%;\n  clear: both;\n  padding: 10px 20px;\n  border-bottom: 1px solid #efefef;\n  height: auto;\n  display: flex;\n  flex-wrap: nowrap;\n  justify-content: space-evenly;\n}\n\n.smslog-row:nth-child(even) {\n  /* background: #efefef; */\n}\n\n.smslog-row:hover {\n  background: #dadada;\n}\n\n.smslog-cell-0 {\n  flex: 0 0 150px;\n}\n\n.smslog-cell-1 {\n  flex: 0 0 75px;\n}\n\n.smslog-cell-2 {\n  flex: 0 0 125px;\n}\n\n.smslog-cell-3 {\n  flex: 1 1 80px;\n}\n\n.smslog-cell-3 span.underline {\n  text-decoration: underline;\n  cursor: pointer;\n}\n\n.smslog-cell-4 {\n  flex: 2 2 200px;\n}\n\n.smslog-cell-5 {\n  flex: 0 0 75px;\n}\n\n.smslog-cell-6 {\n  flex: 0 0 75px;\n}\n</style>\n"],"sourceRoot":"src/components"}]}