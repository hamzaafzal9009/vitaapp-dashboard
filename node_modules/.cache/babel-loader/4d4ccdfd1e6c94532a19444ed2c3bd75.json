,IAAA;AACA,UAAA,IAAA,GAAA,EAAA,CAAA,MAAA,CAAA,MAAA,CAAA,QAAA;AACA,UAAA,OAAA,GAAA,IAAA,CAAA,KAAA,CAAA,GAAA,CAAA;AACA,MAAA,iBAAA,CAAA,qBAAA,CAAA,OAAA,CAAA,CAAA,CAAA,EAAA,OAAA,CAAA,CAAA,CAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,UAAA,IAAA,QAAA,CAAA,UAAA,CAAA,QAAA,EAAA,EAAA,CAAA,QAAA,GAAA,QAAA,CAAA,UAAA,CAAA,QAAA;AACA,OAFA;AAGA,KA9DA;AA+DA,IAAA,UA/DA,sBA+DA,OA/DA,EA+DA;AACA,MAAA,YAAA,CAAA,KAAA,YAAA,CAAA;AACA,UAAA,GAAA,GAAA,kBAAA,KAAA,MAAA,CAAA,MAAA,CAAA,GAAA,GAAA,GAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,GAAA;AACA,UAAA,KAAA,MAAA,CAAA,KAAA,CAAA,CAAA,IAAA,KAAA,MAAA,CAAA,KAAA,CAAA,CAAA,IAAA,WAAA,EAAA,GAAA,IAAA,MAAA,KAAA,MAAA,CAAA,KAAA,CAAA,CAAA;AACA,UAAA,KAAA,MAAA,CAAA,MAAA,CAAA,QAAA,IAAA,KAAA,MAAA,CAAA,MAAA,CAAA,QAAA,IAAA,WAAA,EAAA,GAAA,IAAA,MAAA,KAAA,MAAA,CAAA,MAAA,CAAA,QAAA;AACA,UAAA,KAAA,MAAA,CAAA,MAAA,CAAA,IAAA,IAAA,KAAA,MAAA,CAAA,MAAA,CAAA,IAAA,IAAA,WAAA,EAAA,GAAA,IAAA,MAAA,KAAA,MAAA,CAAA,MAAA,CAAA,IAAA;AACA,UAAA,UAAA,GAAA,OAAA,IAAA,4BAAA;AAEA,UAAA,EAAA,GAAA,IAAA;AACA,MAAA,eAAA,CAAA,aAAA,CAAA,EAAA,CAAA,MAAA,CAAA,MAAA,CAAA,GAAA,EAAA,UAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,EAAA,CAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA,OAFA,EAEA,UAAA,KAAA,EAAA;AACA,QAAA,EAAA,CAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA,OAJA;AAKA;AA7EA;AAvBA,CAAA","sourcesContent":["<template>\n  <div class=\"redirect-page\">\n    <div class=\"redirect-container\">\n      <el-card class=\"redirect-card\">\n        <div class=\"redirect-logo\">\n          <img :src=\"locationImage\"/>\n        </div>\n        <!-- If English -->\n        <div class=\"redirect-content\" v-if=\"language == 'en'\">\n          <div class=\"tellUsMore\">Tell Us More!</div>\n          <div><el-button type=\"primary\" size=\"mini\" class=\"feedback-button\" v-if=\"displaysExpButton\" :style=\"{background: locationColor}\" @click=\"followLink()\">Did you have a bad experience? - Click here!</el-button></div>\n          <div class=\"please-wait\">Redirecting, please wait...</div>\n          <!-- <div>Please leave comments and then click \"Post\"!</div> -->\n          <div class=\"spinner\" style=\"color: #4fcbc5;\"><i class=\"fal fa-circle-notch fa-spin fa-5x\"></i></div>\n          <div><el-link :href=\"redirectUrl\">Click here if not redirected after 10 seconds.</el-link></div>\n        </div>\n        <!-- If Spanish -->\n        <div class=\"redirect-content\" v-if=\"language == 'sp'\">\n          <div class=\"tellUsMore\">¡Cuentanos más!</div>\n          <div><el-button type=\"primary\" size=\"mini\" class=\"feedback-button\" v-if=\"displaysExpButton\" :style=\"{background: locationColor}\" @click=\"followLink()\">¿Tuviste una mala experiencia? - ¡Haga clic aquí!</el-button></div>\n          <!-- <div>¡Por favor, deje sus comentarios y luego haga clic en \"Publicar\"!</div> -->\n          <div class=\"spinner\" style=\"color: #4fcbc5;\"><i class=\"fal fa-circle-notch fa-spin fa-5x\"></i></div>\n          <div><el-link :href=\"redirectUrl\">Haga clic aquí si no se redirige después de 10 segundos.</el-link></div>\n        </div>\n      </el-card>\n    </div>\n  </div>\n</template>\n\n<script>\nimport LocationService from './../services/LocationService'\nimport FeedbackService from './../services/FeedbackService'\nimport AutomationService from './../services/AutomationService'\nimport CampaignService from './../services/CampaignService'\n\nexport default {\n  name: 'reviewRedirect',\n  data: function() {\n    return {\n      location: null,\n      locationImage: '',\n      redirectUrl: '',\n      locationColor: '#4fccc6',\n      displaysExpButton: false,\n      delayValue: 7000,\n      timeRedirect: '',\n      rotator: {},\n      language: 'en'\n    }\n  },\n  created() {\n    if(this.$route.params.template && this.$route.params.template.startsWith('AU')) {\n      this.getAutomation();\n    } else {\n      this.getCampaign();\n    }\n    this.getLocation();\n  },\n  methods: {\n    getLocation() {\n      var vm = this;\n      LocationService.getAnonymousLocation(this.$route.params.loc).then(function(response) {\n        vm.location = response;\n\n        if (!response.locationStyle.logo) {\n          vm.locationImage = 'data:image/png;base64,';\n        } else {\n          var logoData = response.locationStyle.logo;\n          if (logoData && logoData[0] == ',') logoData = logoData.substring(1);\n          vm.locationImage =  'data:image/png;base64,' + logoData;\n        }\n\n        vm.locationColor = response.locationStyle.buttonColor;\n        if (response.rotators) {\n          vm.rotator = response.rotators[vm.$route.params.rIndex];\n\n          if (vm.rotator) {\n            vm.redirectUrl = vm.rotator.url;\n            vm.displaysExpButton = vm.rotator.showReviewButton;\n            vm.delayValue = 15000;\n\n            if (vm.$route.query.r) {\n              try {\n                var rating = parseInt(vm.$route.query.r);\n                if (!isNaN(rating)) {\n                  if (rating > 3) vm.delayValue = 3000;\n                }\n              } catch (e) {}\n            }\n\n            vm.timeRedirect = setTimeout(() => {\n              FeedbackService.recordRouting(vm.$route.params.cid, vm.rotator.name).then(function(response) {\n                window.location = vm.redirectUrl;\n              }, function(error) {\n                window.location = vm.redirectUrl;\n              });\n            }, vm.delayValue);\n          } else {\n            vm.followLink('Routed to Private Feedback');\n          }\n        } else {\n          vm.followLink('Routed to Private Feedback');\n        }\n      }, function(error) {\n        console.log(error);\n      });\n    },\n    getCampaign() {\n      let vm = this;\n      CampaignService.getCampaign(vm.$route.params.template).then(response => {\n        if(response && response.language) vm.language = response.language;\n      })\n    },\n    getAutomation() {\n      var vm = this;\n      let auto = vm.$route.params.template;\n      let autoObj = auto.split(':');\n      AutomationService.getRedirectAutomation(autoObj[0], autoObj[1]).then(response => {\n        if(response.automation && response.automation.language) vm.language = response.automation.language;\n      })\n    },\n    followLink(routing) {\n      clearTimeout(this.timeRedirect);\n      var url = '/leaveReview/' + this.$route.params.loc + '/' + this.$route.params.cid;\n      if (this.$route.query.r && this.$route.query.r != 'undefined') url += '/' + this.$route.query.r;\n      if (this.$route.params.template && this.$route.params.template != 'undefined') url += '/' + this.$route.params.template;\n      if (this.$route.params.date && this.$route.params.date != 'undefined') url += '/' + this.$route.params.date;\n      var routingLog = routing || 'Direct Response via Button';\n\n      var vm = this;\n      FeedbackService.recordRouting(vm.$route.params.cid, routingLog).then(function(response) {\n          vm.$router.push(url);\n        }, function(error) {\n          vm.$router.push(url);\n      });\n    },\n  }\n}\n</script>\n\n<style scoped>\n.redirect-page {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  background: #f2f2f2;\n  height: 100vh;\n  width: 100%;\n  max-width: 100%;\n  padding: 10px;\n  box-sizing: border-box;\n}\n.redirect-card {\n  animation: slideFadeFromTop .65s ease;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  flex-direction: column;\n  width: 400px;\n  height: auto;\n  max-width: 100%;\n}\n.redirect-logo {\n  max-width: 90%;\n  margin: auto;\n  width: 220px;\n\n}\n.redirect-logo img {\n  width: 100%;\n  max-width: 100%;\n}\n.redirect-content {\n  display: flex;\n  justify-content: space-between;\n  flex-direction: column;\n  align-items: center;\n  height: 460px;\n  max-height: 100%;\n  color: #757575;\n  font-size: 18px;\n}\n\n.tellUsMore::before {\n  content: '';\n  display: block;\n  width: 90%;\n  height: 1px;\n  margin: 30px auto;\n  background: #9a9a9a35;\n  /* margin-bottom: 30px; */\n}\n.please-wait::before {\n  content: '';\n  display: block;\n  width: 100%;\n  height: 1px;\n  margin: 30px auto;\n  background: #9a9a9a35;\n}\n.please-wait {\n  font-size: 14px;\n  color: #757575;\n}\n.tellUsMore {\n  /* font-weight: normal; */\n  font-size: 24px;\n  color: #424242;\n  margin-bottom: 10px;\n}\n.feedback-button {\n  height: 48px;\n  font-weight: 600;\n}\n.spinner {\n  /* margin: auto; */\n  margin-top: 10px;\n  margin-bottom: 10px;\n  font-size: 14px;\n}\n\n.feedback-button {\n  box-shadow: 0 3px 6px rgba(0, 0, 0, .15);\n  /* margin: 20px 0;\n  padding: 15px 50px;\n  background: #f48d46;\n  color: white;\n  cursor: pointer; */\n}\n@media only screen and (max-width: 420px) {\n\n}\n\n@media only screen and (max-width : 480px) {\n  /* img.logo {\n    width: calc(100% - 20px);\n  }\n\n  .center-box {\n    margin: 0;\n  } */\n}\n</style>\n}\n</style>\n"],"sourceRoot":"src/components"}]}