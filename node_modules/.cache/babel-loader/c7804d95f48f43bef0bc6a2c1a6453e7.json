A,KAAA;AACA,QAAA,YAAA,CAAA,KAAA,CAAA,SAAA,EAAA,MAAA;AACA,OAPA;AAQA,KAnCA;AAoCA,IAAA,UApCA,sBAoCA,WApCA,EAoCA;AACA,UAAA,KAAA,QAAA,IAAA,WAAA,EAAA;AACA,aAAA,QAAA,GAAA,WAAA;AACA,OAFA,MAEA;AACA,aAAA,QAAA,GAAA,MAAA,KAAA,QAAA;AACA;AACA;AA1CA;AA9DA,CAAA","sourcesContent":["<template>\n  <div>\n    <div class=\"upload-controls\">\n      <div class=\"upload-controls-add\">\n      </div>\n      <div class=\"upload-controls-search\">\n        <input v-model=\"searchText\" @keyup.enter=\"searchForUploads(searchText, locations)\" placeholder=\"Search uploads...\"/>\n        <span class=\"upload-controls-search-icon\">\n          <i class=\"fas fa-search\" @click=\"searchForUploads(searchText, locationId)\"></i>\n        </span>\n      </div>\n    </div>\n    <div class=\"upload-table\">\n      <div class=\"upload-header-row\">\n        <div class=\"upload-table-cell-0\" @click=\"toggleSort('location')\">\n          <span>Location </span>\n          <span v-if=\"sortType == '-location'\"><i class=\"fas fa-sort-up\"></i></span>\n          <span v-if=\"sortType == 'location'\"><i class=\"fas fa-sort-down\"></i></span>\n        </div>\n        <div class=\"upload-table-cell-1\" @click=\"toggleSort('name')\">\n          <span>Name </span>\n          <span v-if=\"sortType == '-name'\"><i class=\"fas fa-sort-up\"></i></span>\n          <span v-if=\"sortType == 'name'\"><i class=\"fas fa-sort-down\"></i></span>\n        </div>\n        <div class=\"upload-table-cell-2\" @click=\"toggleSort('status')\">\n          <span>Status </span>\n          <span v-if=\"sortType == '-status'\"><i class=\"fas fa-sort-up\"></i></span>\n          <span v-if=\"sortType == 'status'\"><i class=\"fas fa-sort-down\"></i></span>\n        </div>\n        <!-- <div class=\"upload-table-cell-3\">\n          <span>Notes</span>\n        </div> -->\n        <div class=\"upload-table-cell-3\" @click=\"toggleSort('date')\">\n          <span>Upload Date </span>\n          <span v-if=\"sortType == '-date'\"><i class=\"fas fa-sort-up\"></i></span>\n          <span v-if=\"sortType == 'date'\"><i class=\"fas fa-sort-down\"></i></span>\n        </div>\n        <div class=\"upload-table-cell-5\">\n            <span>File Size </span>\n        </div>\n        <!-- <div class=\"upload-table-cell-6\">\n            <span>Approximate Records </span>\n        </div>         -->\n      </div>\n      <div class=\"upload-row quick-transition\" v-for=\"upload in filteredUploads\" :key=\"upload._id\" v-if=\"upload.status != 'deleted' || isSuperAdmin\">\n        <div class=\"upload-table-cell-0\">{{upload.location.name}}</div>        \n        <div class=\"upload-table-cell-1\">{{upload.recordName}}</div>\n        <!-- <div class=\"upload-table-cell-2\">{{upload.method}}</div> -->\n        <div class=\"upload-table-cell-2\">{{upload.status || 'Unknown'}}</div>\n        <div class=\"upload-table-cell-3\">{{displayDate(upload.uploadDate)}}</div>\n        <div class=\"upload-table-cell-4\">{{upload.fileSize}}</div>\n        <!-- <div class=\"upload-table-cell-5\">{{upload.approximateRecords}}</div> -->\n      </div>\n      <div class=\"upload-row\" style=\"text-align: center\" v-if=\"filteredUploads.length == 0\">\n        No uploads match your search criteria.\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport UploadService from './../services/UploadService'\nimport LocationService from './../services/LocationService'\nimport {EventService} from '../main'\nimport UserService from './../services/UserService'\n\n\nexport default {\n  name: 'uploadList',\n  data: function() {\n    return {\n      uploads: [],\n      searchText: '',\n      sortType: 'name',\n      rawData: [],\n      locationSelected: '',\n      locations: [],\n      isSuperAdmin: false\n    }\n  },\n  computed: {\n    filteredUploads: function() {\n      var vm = this;\n      if(this.uploads) {\n        var filteredList = this.uploads.filter(function(upload) {\n          var uploadMatch = false;        \n        // uploadMatch = uploadMatch || upload.location.name.toLowerCase().indexOf(vm.searchText.toLowerCase()) > -1;\n        uploadMatch = uploadMatch || upload.status.toLowerCase().indexOf(vm.searchText.toLowerCase()) > -1;\n        \n        return uploadMatch;\n        \n      });\n\n      var filteredSortedList = filteredList.sort((a,b) => {\n        if (this.sortType == 'name') {\n          return a.name.localeCompare(b.name);\n        } else if (this.sortType == '-name') {\n          return b.name.localeCompare(a.name);\n        // } else if (this.sortType == 'location') {\n        //   return a.location.name.localeCompare(b.location.name);\n        // } else if (this.sortType == '-location') {\n        //   return b.location.name.localeCompare(a.location.name);        \n        } else if (this.sortType == 'status') {\n          return a.status.localeCompare(b.status);\n        } else if (this.sortType == '-status') {\n          return b.status.localeCompare(a.status);\n        } else if (this.sortType == 'date') {\n          return a.createdDate - b.createdDate;\n        } else if (this.sortType == '-date') {\n          return b.createdDate - a.createdDate;\n        } else {\n          true;\n        }\n      });\n\n      return filteredSortedList;\n      }\n    },\n\n  },\n  created() {\n    this.getUploads();\n    this.checkForAdmin();    \n    EventService.$on('locationChange', this.getuploads);\n    // EventService.$on('locationChange', this.clearList);\n  },\n  beforeDestroy: function () {\n    EventService.$off('locationChange', this.getuploads);\n  },\n  methods: {\n    checkForAdmin() {\n      var vm = this;\n      let currentUser = JSON.parse(localStorage.getItem('currentUser'));\n      this.isSuperAdmin = currentUser.isSuperAdmin;\n      this.userId = currentUser.userId;\n    },\n    // searchForUploads(string, id){\n    //   if(string == '') {\n    //     this.getUploads();\n    //   } else {\n    //   this.uploads = [];\n    //   UploadService.searchUploads(string, id).then( response => {\n    //     this.uploads = response;\n    //   })\n    //   }\n    // },\n    displayDate(millis) {\n      var date = new Date(millis);\n      return (1 + date.getMonth()) + '/' + date.getDate() + '/' + (date.getFullYear() % 100);\n    },\n    checkForLocation() {\n      this.locationSelected = localStorage.getItem('locationId')\n    },\n    getUploads() {\n      EventService.$emit(\"loading\", \"show\");      \n      var vm = this;\n      UploadService.getUploads().then(\n        function(response) {\n          vm.uploads = response.results;                   \n          EventService.$emit(\"loading\", \"hide\");\n          }, function(error) {\n            console.log(error);\n          EventService.$emit(\"loading\", \"hide\");\n      });\n    },\n    toggleSort(setSortType) {\n      if (this.sortType != setSortType) {\n        this.sortType = setSortType;\n      } else {\n        this.sortType = '-' + this.sortType;\n      }\n    }\n  }\n}\n</script>\n\n<style scoped>\n.upload-controls {\n  float: right;\n  color: #545454;\n  font-size: 20px;\n  width: 100%;\n  background: #ffffff;\n  border-radius: 1px;\n  box-shadow: 0px 2px 4px rgba(0,0,0,.25);\n  margin-bottom: 20px;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  align-content: center;\n}\n\n.upload-controls-add {\n  float: left;\n  padding: 10px 10px 0 0;\n  cursor: pointer;\n  margin-left: 50px;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n}\n\n.upload-controls-add i {\n  color: #4fcbc5;\n}\n\n.upload-controls-add span {\n  font-size: 12px;\n  margin-top: 5px;\n  margin-bottom: 10px;\n  font-weight: bold;\n}\n\n.upload-controls-search {\n  background: white;\n  margin: 20px 20px;\n  border: 2px solid #efefef;\n  border-radius: 1px;\n  width: 350px;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.upload-controls-search input {\n  border: none;\n  padding: 15px 15px 5px 15px;\n  font-size: 12px;\n}\n\n.upload-controls-search-icon {\n  vertical-align: middle;\n  margin-right: 15px;\n  color: #4fcbc5;\n  font-size: 16px;\n}\n\n.upload-table {\n  background: white;\n  font-size: 14px;\n  padding: 10px 0px 20px 0px;\n  clear: both;\n  border-radius: 1px;\n  box-shadow: 0px 2px 4px rgba(0,0,0,.25);\n}\n\n.superadmin-row {\n  background: #4fcbc52b;\n}\n\n.upload-header-row {\n  width: calc(100% - 20px);\n  font-weight: bold;\n  padding: 10px 10px 20px 10px;\n  border-bottom: 3px solid #efefef;\n  height: 14px;\n  display: flex;\n  flex-wrap: nowrap;\n  justify-content: space-evenly;\n  cursor: pointer;\n}\n\n.upload-header-row div,\n.upload-row div {\n  margin: auto;\n}\n\n.upload-row {\n  width: calc(100% - 20px);\n  clear: both;\n  padding: 10px;\n  border-bottom: 1px solid #efefef;\n  height: auto;\n  display: flex;\n  flex-wrap: nowrap;\n  justify-content: space-evenly;\n  font-size: 12px;\n}\n\n.upload-row:hover {\n  background: #dfdfdf;\n}\n\n.upload-table-cell-0 {\n  flex: 0 0 200px;\n  padding-left: 20px;\n}\n\n.upload-table-cell-1 {\n  flex: 0 0 150px;\n  text-transform: capitalize;\n}\n\n.upload-table-cell-1 img {\n  width: 40px;\n  height: 40px;\n}\n\n.upload-table-cell-2 {\n  flex: 1 1 50px;\n  cursor: pointer;\n}\n\n.upload-table-cell-3 {\n  flex: 2 2 200px;\n}\n\n.upload-table-cell-4 {\n  flex: 0 0 100px;\n  text-transform: capitalize;\n}\n\n.centered {\n  text-align: center;\n  /* margin-left: 20px; */\n}\n\n.upload-table-cell-4 .fa-check {\n  color: green;\n}\n\n.upload-table-cell-4  .fa-ban {\n  color: orange;\n}\n\n.upload-table-cell-4  .fa-times-circle {\n  color: red;\n}\n\n.upload-table-cell-5 {\n  flex: 0 0 200px;\n  text-align: center;\n  text-transform: capitalize;\n}\n\n.upload-table-cell-6 {\n  flex: 0 0 220px;\n  text-align: center;\n  text-overflow: ellipsis;\n}\n\n.upload-table-cell-5 span {\n  margin-right: 5px;\n  cursor: pointer;\n}\n\n.color-change {\n  color: #2d2d2d;\n}\n\n.upload-pagination {\n  border-top: 3px solid #efefef;\n  padding: 10px 10px 0 10px;\n}\n\n.upload-pagination button {\n  border: 1px solid #efefef;\n  padding: 10px;\n  cursor: pointer;\n  background: white;\n  border-radius: 0;\n  min-width: 50px;\n}\n\n.upload-pagination button.button-selected {\n  background: #dfdfdf;\n}\n\n.upload-pagination button:hover {\n  background: #dfdfdf;\n}\n</style>\n"],"sourceRoot":"src/components"}]}