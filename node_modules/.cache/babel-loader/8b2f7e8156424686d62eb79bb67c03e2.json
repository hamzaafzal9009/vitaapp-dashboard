ons: {\n        scales: {\n          yAxes: [{\n            stacked: true,\n            barPercentage: 1,\n            categoryPercentage: 1,\n            gridLines: {\n                offsetGridLines: true\n            },\n            position: 'left',\n          }],\n          xAxes: [{\n            stacked: true,\n            display: true,\n          }]\n        },\n        legend: {\n          display: false,\n        },\n        maintainAspectRatio: false,\n        title: {\n          display: false\n        },\n      },\n    }\n  },\n  created() {\n    this.getDataSetsAndLabels()\n  },\n  methods: {\n    getDataSetsAndLabels(){\n      let vm = this;\n      let datasets = [\n        {\n          backgroundColor: ['orange', '#4fcbc5','#545454' , 'blue','yellow'],\n          data: []\n        },\n      ];\n      let labels = [];\n      for(var i in vm.barData.data) {\n        labels.push(i);\n        datasets[0].data.push([vm.barData.data[i]]);\n      }\n      \n      vm.report = {\n        datasets,\n        labels\n      }\n    },\n    removeReport(data){\n      // console.log(data);\n      this.$emit('removeReport', data.i)\n    }\n  }\n}\n</script>\n\n<style scoped>\n\n.report-container {\n  width: 100%;\n  height: 345px;\n  background: white;\n  box-shadow: 1px 0px 5px rgba(0,0,0,.35);\n  border-radius: 5px;\n  margin: 20px;\n  display: flex;\n  flex-direction: column;\n  align-content: center;\n  align-items: center;\n  position: relative;\n}\n\n.report-title {\n  width: 90%;\n  display: flex;\n  /* justify-content: center; */\n  padding-left: 30px;\n  padding-bottom: 10px;\n  padding-top: 10px;\n  border-bottom: 2px solid #efefef;\n}\n\n.report-title span {\n  margin: 10px;\n  font-weight: bolder;\n}\n\n.title {\n  font-size: 18px;\n}\n\n.remove-report {\n  cursor: pointer;\n  color: #545454;\n  position: absolute;\n  right: 0;\n}\n\n.report-data {\n  margin-top: 0;\n  padding-bottom: 2px;\n}\n\n</style>\n"],"sourceRoot":"src/components/Reports"}]}