AAA,GAAA;AAAA,WAAA,EAAA,CAAA,CAAA;;AAEA,cAAA,EAAA,CAAA,YAAA,GAAA,CAAA,EAAA;AACA,YAAA,EAAA,CAAA,oBAAA,CAAA,QAAA,CAAA,MAAA,CAAA,CAAA,CAAA;AACA,WAFA,MAEA;AACA,YAAA,EAAA,CAAA,SAAA,GAAA,CAAA,CAAA,EAAA,CAAA,EAAA,CAAA,EAAA,CAAA,EAAA,CAAA,CAAA;AACA;AACA;;AAEA,QAAA,EAAA,CAAA,WAAA,GAAA,KAAA;AAEA,OAdA,EAcA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,OAAA,EAAA,KAAA;AACA,QAAA,EAAA,CAAA,WAAA,GAAA,KAAA;AACA,QAAA,EAAA,CAAA,UAAA,GAAA,IAAA;AACA,OAlBA;AAoBA;AAlEA;AAxBA,CAAA","sourcesContent":["<template>\n  <el-card shadow=\"always\" class=\"medium-card\">\n    <el-row>\n      <el-col :span=\"23\">\n        <div class=\"o-card-title-2 text-center clickable\" @click=\"followLink('/feedback')\">\n          Private Feedback\n        </div>\n      </el-col>\n      <el-col :span=\"1\">\n        <el-dropdown trigger=\"click\" @command=\"changeTimeframe\">\n          <span class=\"opiniion-grey el-dropdown-link\">\n            <i class=\"fa fa-ellipsis-v clickable\" aria-hidden=\"true\"></i>\n          </span>\n          <el-dropdown-menu slot=\"dropdown\">\n            <el-dropdown-item :icon=\"checkForSelected('always')\" command=\"always\">All time</el-dropdown-item>\n            <el-dropdown-item :icon=\"checkForSelected(30)\" :command=\"30\">Last 30 days</el-dropdown-item>\n            <el-dropdown-item :icon=\"checkForSelected(120)\" :command=\"120\">Last 3 months</el-dropdown-item>\n            <el-dropdown-item :icon=\"checkForSelected(240)\" :command=\"240\">Last 6 months</el-dropdown-item>\n            <el-dropdown-item :icon=\"checkForSelected(365)\" :command=\"365\">In the last year</el-dropdown-item>\n          </el-dropdown-menu>\n        </el-dropdown>\n      </el-col>\n    </el-row>\n    <el-row style=\"padding:0;\">\n      <el-col>\n        <div class=\"o-card-title-3 text-center\">\n          Based on Opiniion private reviews\n        </div>\n      </el-col>\n    </el-row>\n    <el-row id=\"pf-rating-bars\" v-show=\"!showLoading && chartData\">\n      <el-col>\n        <el-row class=\"flex-align-center\" style=\"padding:5px 0;\">\n          <el-col :xs=\"4\" :sm=\"4\" :xl=\"3\">\n            <div class=\"rating-text\">5 star</div>\n          </el-col>\n          <el-col :xs=\"16\" :sm=\"16\" :xl=\"18\">\n            <div class=\"rating-bar-wrapper\"><span class=\"rating-bar-filler\" v-bind:style=\"{ width: chartData[4].toFixed(0) + '%' }\"></span></div>\n          </el-col>\n          <el-col :xs=\"4\" :sm=\"4\" :xl=\"3\" style=\"text-align:center;\">\n            <div class=\"rating-text\">{{ chartData[4].toFixed(0) }}%</div>\n          </el-col>\n        </el-row>\n        <el-row class=\"flex-align-center\" style=\"padding:5px 0;\">\n          <el-col :xs=\"4\" :sm=\"4\" :xl=\"3\">\n            <div class=\"rating-text\">4 star</div>\n          </el-col>\n          <el-col :xs=\"16\" :sm=\"16\" :xl=\"18\">\n            <div class=\"rating-bar-wrapper\"><span class=\"rating-bar-filler\" v-bind:style=\"{ width: chartData[3].toFixed(0) + '%' }\"></span></div>\n          </el-col>\n          <el-col :xs=\"4\" :sm=\"4\" :xl=\"3\" style=\"text-align:center;\">\n            <div class=\"rating-text\">{{ chartData[3].toFixed(0) }}%</div>\n          </el-col>\n        </el-row>\n        <el-row class=\"flex-align-center\" style=\"padding:5px 0;\">\n          <el-col :xs=\"4\" :sm=\"4\" :xl=\"3\">\n            <div class=\"rating-text\">3 star</div>\n          </el-col>\n          <el-col :xs=\"16\" :sm=\"16\" :xl=\"18\">\n              <div class=\"rating-bar-wrapper\"><span class=\"rating-bar-filler\" v-bind:style=\"{ width: chartData[2].toFixed(0) + '%' }\"></span></div>\n          </el-col>\n          <el-col :xs=\"4\" :sm=\"4\" :xl=\"3\" style=\"text-align:center;\">\n            <div class=\"rating-text\">{{ chartData[2].toFixed(0) }}%</div>\n          </el-col>\n        </el-row>\n        <el-row class=\"flex-align-center\" style=\"padding:5px 0;\">\n          <el-col :xs=\"4\" :sm=\"4\" :xl=\"3\">\n            <div class=\"rating-text\">2 star</div>\n          </el-col>\n          <el-col :xs=\"16\" :sm=\"16\" :xl=\"18\">\n            <div class=\"rating-bar-wrapper\"><span class=\"rating-bar-filler\" v-bind:style=\"{ width: chartData[1].toFixed(0) + '%' }\"></span></div>\n          </el-col>\n          <el-col :xs=\"4\" :sm=\"4\" :xl=\"3\" style=\"text-align:center;\">\n            <div class=\"rating-text\">{{ chartData[1].toFixed(0) }}%</div>\n          </el-col>\n        </el-row>\n        <el-row class=\"flex-align-center\" style=\"padding:5px 0;\">\n          <el-col :xs=\"4\" :sm=\"4\" :xl=\"3\">\n            <div class=\"rating-text\">1 star</div>\n          </el-col>\n          <el-col :xs=\"16\" :sm=\"16\" :xl=\"18\">\n            <div class=\"rating-bar-wrapper\"><span class=\"rating-bar-filler\" v-bind:style=\"{ width: chartData[0].toFixed(0) + '%' }\"></span></div>\n          </el-col>\n          <el-col :xs=\"4\" :sm=\"4\" :xl=\"3\" style=\"text-align:center;\">\n            <div class=\"rating-text\">{{ chartData[0].toFixed(0) }}%</div>\n          </el-col>\n        </el-row>\n        <el-row style=\"padding:0;\">\n          <el-col>\n            <div class=\"total-reviews-text\">Total Reviews: {{ totalReviews | formatNum }}</div>\n          </el-col>\n        </el-row>\n      </el-col>\n    </el-row>\n    <el-row v-show=\"!showLoading && erroredOut\">\n      <el-col>\n        <div style=\"text-align: center;padding-top: 40px;\">An error occured while getting this data.</div>\n      </el-col>\n    </el-row>\n    <el-row v-show=\"showLoading\">\n      <el-col>\n        <div class=\"dashboard-row dashboard-loader\"><i class=\"far fa-hourglass fa-spin\"></i></div>\n      </el-col>\n    </el-row>\n  </el-card>\n</template>\n\n<script>\nimport ReportService from './../../services/ReportService'\nimport UserService from './../../services/UserService'\nimport {EventService} from '../../main'\nimport { type } from 'os'\n\nexport default {\n  name: 'publicRating',\n  data() {\n    return {\n      showLoading: true,\n      chartData: [ 0, 0, 0, 0, 0],\n      timeframe: 30,\n      totalReviews: 0,\n      erroredOut: false\n    }\n  },\n  created() {\n    this.getReviewsData();\n    EventService.$on('locationChange', this.getReviewsData);\n  },\n  beforeDestroy: function () {\n    EventService.$off('locationChange', this.getReviewsData);\n  },\n  filters: {\n    formatNum(value) {\n        let val = (value/1).toFixed(0).replace('.', ',')\n        return val.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \",\")\n    }\n  },\n  methods: {\n    followLink(targetPage) {\n      localStorage.setItem('passedFilters', JSON.stringify({name: 'source', value: ['Opiniion']}));\n      this.$router.push(targetPage);\n    },\n    changeTimeframe(time) {\n      this.timeframe = time;\n      this.getReviewsData();\n    },\n    checkForSelected(command) {\n      let vm = this;\n      if(command == vm.timeframe) {\n        return 'el-icon-check'\n      } else {\n        return '';\n      }\n    },\n    convertToPercentages(totals) {\n      var percentages = [];\n      var i;\n      for (i = 0; i < totals.length; i++) {\n        percentages.push(totals[i] / this.totalReviews * 100);\n      }\n      this.chartData = percentages;\n    },\n    getReviewsData() {\n      this.showLoading = true;\n      var locationId = localStorage.getItem('locationId');\n      var locationIdArray = [];\n      let todaysDate = Date.now();\n      let timeframe = 100000;\n      if (typeof this.timeframe == 'number')\n        timeframe = this.timeframe;\n\n      let previousDate = new Date().getTime() - 1000 * 60 * 60 * 24 * timeframe;\n      let startDate = new Date(previousDate).toISOString().substring(0, 10);\n      let endDate = new Date(todaysDate).toISOString().substring(0, 10);\n\n      if(locationId) {\n        locationIdArray.push(locationId);\n      } else {\n        let locOptions = JSON.parse(localStorage.getItem('locationOptions'));\n        locOptions.map(el => locationIdArray.push(el.locationId));\n      }\n\n      var vm = this;\n      ReportService.getCurrentVsPreviousReport(new Date(startDate).getTime(), new Date(endDate).getTime(), locationIdArray).then(\n        function(response) {\n          if (response.result) {\n            vm.totalReviews = response.result[0].reduce(function(acc, val) { return acc + val; }, 0);\n\n            if (vm.totalReviews > 0) {\n              vm.convertToPercentages(response.result[0]);\n            } else {\n              vm.chartData = [ 0, 0, 0, 0, 0];\n            }\n          }\n          \n          vm.showLoading = false;\n          \n        }, function(error) {\n          console.log(\"error\", error);\n          vm.showLoading = false;\n          vm.erroredOut = true;\n        }\n      );\n    }\n  }\n}\n</script>\n\n<style>\n\n#pf-rating-bars {\n  padding: 30px 0;\n}\n\n.rating-bar-wrapper {\n  width: 100%;\n  border: 1px solid #9A9A9A59;\n  height: 24px;\n  border-radius: 6px;\n}\n\n.rating-bar-filler {\n  background-color: #FFCE00;\n  height: 24px;\n  display: inline-block;\n  border-radius: 6px;\n}\n\n.rating-text {\n  color: #757575;\n  font-size: 14px;\n}\n\n.total-reviews-text {\n  color: #9A9A9A;\n  font-size: 14px;\n  text-align: center;\n  padding-top: 40px;\n}\n\n</style>\n"],"sourceRoot":"src/components/DashboardComponents"}]}