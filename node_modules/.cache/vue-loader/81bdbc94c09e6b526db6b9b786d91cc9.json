EA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Login.vue","sourceRoot":"src/components","sourcesContent":["<template>\n  <div class=\"login-wrapper\">\n    <div class=\"content\">\n      <div class=\"header\">\n        <img class=\"header-image\" src=\"../assets/img/logo-white.svg\" width=\"170\" alt=\"Opiniion\" />\n        <div class=\"header-link-wrapper\">\n          <el-link class=\"header-link\" href=\"https://opiniion.com\" type=\"#FFFFFF\" target=\"_blank\" icon=\"fa fa-angle-left\">&nbsp;&nbsp;Go to www.opiniion.com</el-link>\n        </div>\n      </div>\n      <el-card class=\"absolute-centered login-card\">\n        <span class=\"login-header horizontal-centered\">Login</span>\n        <el-input\n          v-model=\"email\"\n          prefix-icon=\"fal fa-envelope\"\n          placeholder=\"Email\"\n          @keyup.enter.native=\"doLogin()\"\n        ></el-input>\n        <el-input\n          v-model=\"password\"\n          prefix-icon=\"fal fa-key\"\n          placeholder=\"Password\"\n          type=\"password\"\n          show-password\n          @keyup.enter.native=\"doLogin()\"\n        ></el-input>\n        <div class=\"error-message\" v-if=\"loginError\">{{loginError}}</div>\n        <div class=\"remember-container flex-space-between\">\n          <el-checkbox :checked=\"remember\" v-model=\"remember\">Remember Me</el-checkbox>\n          <router-link to=\"/resetPassword\" class=\"fp float-right\" href>Forgot Password?</router-link>\n        </div>\n        \n        <el-button class=\"login-button\" @click=\"doLogin\" type=\"primary\" round >Login</el-button>\n\n        <p class=\"agreement-container\">\n          By logging in you agree to Opiniion's <br>\n          <!-- <router-link to=\"/terms\" class=\"fp\">Terms of Use </router-link>and -->\n          <!-- <router-link to=\"/terms\" class=\"fp\">Privacy Policy</router-link> -->\n          <a type=\"primary\" href=\"/files/OpiniionPrivacyPolicy.pdf\" target=\"_blank\">\n            Privacy Policy\n          </a>\n          &nbsp; & &nbsp;\n          <a type=\"primary\" href=\"/files/Website Terms of Use.pdf\" target=\"_blank\">\n            Terms of Use\n          </a>\n        </p>\n      </el-card>\n    </div>\n    <footer class=\"footer\">\n      <div class=\"login-bottom-nav flex-center\">\n        <div>\n          <div class=\"credits ml-auto\">\n            <div\n              class=\"copyright\"\n            >&copy; {{new Date().getFullYear()}} Opiniion Inc. All Rights Reserved</div>\n          </div>\n        </div>\n      </div>\n    </footer>\n  </div>\n</template>\n\n<script>\nimport AuthService from \"../services/AuthService\";\nimport UserService from \"../services/UserService\";\nimport { EventService } from \"../main.js\";\n\nexport default {\n  name: \"login\",\n  data: function() {\n    return {\n      email: \"\",\n      password: \"\",\n      loginError: null,\n      logoutReason: null,\n      remember: false\n    };\n  },\n  created() {\n    this.logoutReason = this.$route.params.reason;\n    this.checkCookie();\n  },\n  methods: {\n    doLogin() {\n      var vm = this;\n      EventService.$emit(\"loading\", \"show\");\n      vm.loginError = null;\n      localStorage.removeItem(\"locationId\");\n      AuthService.login(this.email, this.password).then(\n        function(response) {\n          if (response.success) {\n            vm.setCookie();\n            UserService.getUserLocations().then(\n              function(response) {\n                EventService.$emit(\"loading\", \"hide\");\n                if (\n                  response &&\n                  response.results &&\n                  response.results.length > 0\n                ) {\n                  if (response.length == 1) {\n                    try {\n                      localStorage.setItem(\n                          \"locationId\",\n                        response.results[0].locationId\n                      );\n                    } catch (error) {\n                      console.log(error);\n                    }\n\n                  } else {\n                    try {\n                        localStorage.setItem(\n                          \"locationOptions\",\n                        JSON.stringify(response.results)\n                      );\n                    } catch (error) {\n                      console.log(error)\n                    }\n                  }\n                }\n                vm.$router.push(\"/dashboard\");\n                // let screen = [window.screen.availHeight * window.devicePixelRatio, window.screen.availWidth * window.devicePixelRatio]\n                // if(screen[0] < 1000 && screen[1] < 800) {\n                //   // vm.$router.push(\"/addCustomerMobile\");\n                //   vm.$router.push(\"/admin/overview\");\n                // } else {\n                //   vm.$router.push(\"/admin/overview\");\n                // }\n              },\n              function(error) {\n                EventService.$emit(\"loading\", \"hide\");\n                //vm.loginError = error;\n                vm.loginError =\n                  \"Error finding user locations.  Please try again.\";\n              }\n            );\n          } else {\n            EventService.$emit(\"loading\", \"hide\");\n            vm.loginError = response.error;\n          }\n        },\n        function(error) {\n          EventService.$emit(\"loading\", \"hide\");\n          vm.loginError = \"Unknown error encountered.  Please try again.\";\n        }\n      );\n    },\n    isMobile() {\n      let screen = [\n        window.screen.availHeight * window.devicePixelRatio,\n        window.screen.availWidth * window.devicePixelRatio\n      ];\n      if (screen[0] < 1000 && screen[1] < 800) {\n        return true;\n      } else {\n        return false;\n      }\n    },\n    setCookie() {\n      let year = 60 * 60 * 24 * 1000 * 365;\n      let now = new Date();\n      let then = now.getTime() + year;\n      if (this.remember) {\n        document.cookie = `email=${this.email}; expires=${new Date(then)};`;\n        document.cookie = `remember=true; expires=${new Date(then)};`;\n      }\n      if (!this.remember) {\n        document.cookie = `email=; expires=${new Date(now.getTime() - year)};`;\n        document.cookie = `remember=; expires=-1;`;\n      }\n    },\n    getCookie(cname) {\n      let name = cname + \"=\";\n      let decodedCookie = decodeURIComponent(document.cookie);\n      let mappedCookie = decodedCookie.split(\";\");\n      mappedCookie.map((e, i) => {\n        if (e.includes(cname)) {\n          this[cname] = e.substring(e.indexOf(\"=\") + 1);\n        }\n      });\n      return \"\";\n    },\n    checkCookie() {\n      this.getCookie(\"remember\");\n      if (this.remember) {\n        this.getCookie(\"email\");\n      }      \n    },\n    goToWWW() {\n      window.location.href = \"http://www.opiniion.com\";\n    }\n  }\n};\n</script>\n\n<style scoped>\n@media (max-width: 600px) {\n  .header-image {\n    margin: auto;\n  }\n  .header-link-wrapper {\n    margin: 10px auto;\n  }\n}\n.login-wrapper {\n  height: 100vh;\n  width: 100%;\n  background: url('../assets/img/login-bg.svg');\n  background-size: cover;\n  background-repeat: no-repeat;\n  /* background: #F4F3EF; */\n  /* background: -webkit-linear-gradient(300deg, #22c6c7 60%, #F4F3EF 60%); */\n}\n\n.header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  flex-wrap: wrap;\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 100%;\n  box-sizing: border-box;\n  height: 130px;\n  padding: 40px;\n}\n\n.header-link {\n  color: white;\n}\n.header > .header-row {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin: 10px 25px;\n}\n\n.header-row > img {\n  width: 170px;\n}\n\n.left-header {\n  font-size: 15px !important;\n  color: white;\n  z-index: 999;\n}\n\n.login-button {\n  width: 100%;\n  margin-top: 10px;\n}\n.login-card {\n  animation: .35s ease slideFadeFromBottom;\n  position: relative;\n  width: 340px;\n  max-width: 90%;\n  height: 380px;\n}\n\n.content {\n  width: 100vw;\n  height: 100vh;\n  margin: auto;\n  z-index: 10;\n  position: absolute;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  top: 0;\n  margin: auto;\n  display: flex;\n}\n\n.login-header {\n  margin: 0 0 40px 0;\n  font-size: 30px;\n}\n\n.remember-container {\n  margin: 20px 0px 20px 0px;\n}\n\n.fp {\n  text-decoration: none;\n  font-size: 12px;\n}\n\n.agreement-container {\n  font-size: 12px;\n  width: 80%;\n  text-align: center;\n  margin: 20px auto;\n  line-height: 1.5;\n}\n\nfooter {\n  position: absolute;\n  width: 100%;\n  bottom: 10px;\n  font-size: 12px;\n  color: #757575;\n}\n\n.login-bottom-nav {\n  margin: 30px 50px;\n}\n\n.router-links {\n  width: 200px;\n}\n\n.fc {\n  color: white;\n  text-decoration: none;\n}\n\n.fa-angle-left {\n  margin-right: 8px;\n}\n\n.error-message {\n  color: #DD2C00;\n  width: 100%;\n  font-size: 11px;\n  text-align: left;\n  margin-bottom: 5px;\n}\n@media (max-width: 360px) {\n  .header {\n    margin: 0;\n    padding: 10px;\n  }\n}\n@media (max-width: 1200px) {\n  .header {\n    margin-top: 20px;\n  }\n  .login-wrapper {\n    background: url('../assets/img/login-bg.svg') 50% 50%;\n  }\n}\n@media screen and (max-width: 500px) {\n  .header > .header-row {\n    flex-direction: column;\n    justify-content: center;\n  }\n  .header > .header-row > div {\n    margin-top: 10px;\n  }\n}\n</style>"]}]}