     \n          <span>Subject: {{email.email.subject}}</span>\n          <span>Attachments: </span>\n          <div class=\"processed-emails\" v-for=\"attachment in email.email.attachments\">\n            <span>{{attachment.filename}}</span>            \n            <span style=\"cursor: pointer;\" @click=\"downloadFile(attachment)\">Download File</span>\n          </div>\n          <!-- <div class=\"processed-data\">\n            <span>Click Here to see Processed Data</span>\n            <div v-if=\"email.processedData\">\n              <span>Customers Added:</span>\n              <div v-for=\"data in email.processedData\">\n                {{data}}\n              </div>\n            </div>\n          </div> -->\n        </div>\n      </div>\n  </div>\n </div>\n</template>\n\n<script>\nimport ParsingService from '../services/ParsingService.js';\n\nexport default {\n  name: 'processedEmails',\n  props: ['selectedMailbox'],\n  data: function() {\n    return {\n      processedEmails: [],\n      mailboxName: '',\n\n    }\n  },\n  created() {\n    this.mailboxName = this.selectedMailbox;\n    this.getProcessedEmails();\n  },\n  methods: {\n    downloadFile(attachment) {\n      let data = atob(attachment.content)\n      let blob = new Blob([data], {type: attachment.contentType})\n      let link = document.createElement(\"a\");\n      link.href = window.URL.createObjectURL(blob);\n      link.download = attachment.filename;\n      link.click();\n    },\n    getProcessedEmails() {\n      let vm = this;\n      let mailbox = vm.mailboxName.substring(0, vm.mailboxName.indexOf('@'))\n      ParsingService.getProcessedEmails(mailbox).then(response => {\n        vm.processedEmails = response.result;\n      })\n    },\n    closeModal() {\n      this.$emit(\"closeModal\");\n      this.mailboxName = '';\n      this.processedEmails = [];\n    }\n  }\n}\n</script>\n\n<style scoped>\n\n.modal-mask {\n  /* all: unset; */\n}\n\n.mailbox-name {\n  text-align: center;\n  margin-bottom: 30px;\n}\n\n.processed-container {\n  background: white;\n  margin: 30px;\n  padding: 30px;\n}\n\n.processed-emails {\n  display: flex;\n  flex-direction: column;\n  padding: 10px;\n  width: 80vw;\n}\n\n.processed-emails:nth-child(odd) {\n  border-bottom: 1px solid lightgrey;\n}\n\n.processed-emails span{\n  white-space: nowrap;\n  overflow: hidden;\n  text-overflow: ellipsis;\n}\n\n</style>\n"]}]}