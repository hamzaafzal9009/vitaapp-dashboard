 <el-button type=\"back\" @click=\"$emit('close')\">Cancel</el-button>\n      <!-- <el-button type=\"danger\" @click=\"$emit('close')\">Yes, Delete</el-button> -->\n      <el-button type=\"danger\" @click=\"deleteCustomer(selectedCustomer)\">Yes, Delete</el-button>\n    </span>\n  </el-dialog>\n</template>\n\n<script>\nimport CustomerService from \"../services/CustomerService\";\n\nexport default {\n  name: \"deleteCustomer\",\n  props: [\"selectedCustomer\", \"showCustomerDetail\", \"deleteCustomerModal\"],\n  data() {\n    return {};\n  },\n  methods: {\n    deleteCustomer(selectedCustomer) {\n      var vm = this;\n      if (selectedCustomer.status != 'deleted') {\n        CustomerService.deleteCustomer(selectedCustomer.customerId).then(\n          function(response) {\n            this.$notify({\n              title: 'Successfully Deleted!',\n              type: 'success',\n              message: `${selectedCustomer.firstName + ' ' + selectedCustomer.lastName} was successfully deleted from customers.`,\n              position: 'bottom-right'\n            })\n            vm.$emit('close'); // close delete modal\n            vm.$emit('back'); // go back to customer list\n          },\n\n          function(error) {\n            console.log(error);\n            this.$notify({\n              title: 'Error',\n              type: 'error',\n              message: `${error}`,\n              position: 'bottom-right'\n            }),\n            vm.$emit('close');\n          },\n        )\n      } else {\n        console.log('Customer has already been deleted');\n        this.$notify({\n          title: 'Warning',\n          type: 'warning',\n          message: `Customer has already been deleted!`,\n          position: 'bottom-right'\n        }),\n        vm.$emit('close');\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n.dialog-body {\n  text-align: center;\n  box-sizing: border-box;\n  font-size: 28px;\n  font-weight: 600;\n}\n.input {\n  margin-top: 5px;\n}\n.input-label .input:nth-child(2) {\n  padding: 20px;\n}\n.dialog-footer button {\n  width: 260px;\n}\n</style>"]}]}