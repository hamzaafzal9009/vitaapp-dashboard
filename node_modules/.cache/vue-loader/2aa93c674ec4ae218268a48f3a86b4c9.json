      <span class=\"question-body\">{{question.question}}</span>\n        </div>\n        <span v-if=\"question.type == 'freeResponse' || question.type == ''\"><textarea v-model=\"question.response\" @change=\"changeInputSelection(i, question.response)\" /></span>          \n        <span class=\"radial-range\" v-if=\"question.type == 'selectAny'\" >\n          <div class=\"radial-container\" v-for=\"option in question.answerOptions\">\n            <label>{{option}}</label>\n            <input type=\"checkbox\" @click=\"changeCheckboxSelection(i, option)\">\n          </div>\n        </span>\n        <span class=\"radial-range\" v-if=\"question.type == '1to5' || question.type == 'disagreeToAgree' || question.type == 'yesOrNo' || question.type == 'trueOrFalse'\">\n          <div class=\"radial-container\" v-if=\"question.type == '1to5'\" v-for=\"number in numberRangeResponse\">\n            <label>{{number}}</label>\n            <input :value=\"number\" type=\"radio\" v-model=\"question.picked\" @change=\"changeInputSelection(i, question.picked)\">\n          </div>\n          <div class=\"radial-container\" v-if=\"question.type == 'disagreeToAgree'\" v-for=\"agreement in agreementRangeResponse\">\n            <label>{{agreement}}</label>\n            <input :value=\"agreement\" type=\"radio\" v-model=\"question.picked\" @change=\"changeInputSelection(i, question.picked)\">\n          </div>            \n          <div class=\"radial-container\" v-if=\"question.type == 'yesOrNo'\" v-for=\"ans in ynResponse\">\n            <label>{{ans}}</label>\n            <input :value=\"ans\" type=\"radio\" v-model=\"question.picked\" @change=\"changeInputSelection(i, question.picked)\">\n          </div>            \n          <div class=\"radial-container\" v-if=\"question.type == 'trueOrFalse'\" v-for=\"ans in tfResponse\">\n            <label>{{ans}}</label>\n            <input :value=\"ans\" type=\"radio\" v-model=\"question.picked\" @change=\"changeInputSelection(i, question.picked)\">\n          </div>            \n        </span>\n        <span class=\"explanation-container\" v-if=\"question.explain\">Please Explain<textarea v-model=\"question.explanation\" @change=\"changeInputDetails(i, question.explanation)\" /></span>          \n      </div>\n    </div>\n    <div class=\"preview-modal-container\" v-if=\"previewType == 'reviewRequest'\">\n      <div class=\"modal-header\">\n        <span class=\"landing-page-type\" :class=\"{'active': activeLandingPage == 'Positive'}\" @click=\"changeActivePage('Positive')\" >Landing Page Preview </span>\n        <span class=\"landing-page-type\" :class=\"{'active': activeLandingPage == 'Negative'}\" @click=\"changeActivePage('Negative')\" >Negative Page Preview </span>\n      </div>\n      <div class=\"review-container\" >\n        <div class=\"review-text\" >\n          <div class=\"review-text-header\" v-if=\"activeLandingPage == 'Positive'\">{{landingPageItems.lp1}}</div>\n          <div class=\"review-text-line2\" v-if=\"activeLandingPage == 'Positive'\">{{landingPageItems.lp2}}</div>\n          <div class=\"review-text-line3\" v-if=\"activeLandingPage == 'Positive'\">{{landingPageItems.lp3}}</div>\n          <div class=\"review-text-header\" v-if=\"activeLandingPage == 'Negative'\">{{landingPageItems.rp1}}</div>\n          <div class=\"review-text-line2\" v-if=\"activeLandingPage == 'Negative'\">{{landingPageItems.rp2}}</div>\n          <div class=\"review-text-line3\" v-if=\"activeLandingPage == 'Negative'\">{{landingPageItems.rp3}}</div>\n        </div>\n        <div class=\"review-box\">\n          <span class=\"review-label\">Feedback</span>\n          <div class=\"review-options\">\n            <textarea class=\"review-textarea\" placeholder=\"Please share your experience here...\"></textarea>\n            <span class=\"review-length\">Reviews must be at least 4 characters.</span>\n          </div>\n          <div class=\"review-button\">\n            <button>Submit</button>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n\n<script>\n\nimport { EventService } from '../main';\n\nexport default {\n  name: \"PreviewLanding\",\n  data: function() {\n    return {\n      previewType: \"\",\n      surveyQuestions: [],\n      landingPageItems: [],\n      activeLandingPage: 'Positive',\n      surveyAnswers: null,\n      numberRangeResponse: [1,2,3,4,5],\n      agreementRangeResponse: ['Strongly Disagree', 'Disagree', 'Neutral', 'Agree', 'Strongly Agree'],\n      ynResponse: ['Yes', 'No'],\n      tfResponse: ['True', 'False'],\n    };\n  },\n  created() {\n    // TODO: Get this stuff working below before automations go live customer facing.\n    this.previewType = localStorage.getItem('automationType');\n    this.landingPageItems = localStorage.getItem('previewData');\n    this.surveyQuestions = localStorage.getItem('surveyData');\n  },\n  beforeDestroy() {\n    localStorage.removeItem('automationType');\n    localStorage.removeItem('previewData');\n    localStorage.removeItem('surveyData');\n  },\n  methods: {\n    changeCheckboxSelection(index, answer) {\n      let vm = this;\n      let submittedAnswer = answer;\n      if(vm.surveyAnswers[index] == 0) {\n        vm.surveyAnswers[index] = {}\n        vm.surveyAnswers[index].answer = []\n      }\n      if(vm.surveyAnswers[index].answer && vm.surveyAnswers[index].answer.length > 0) {\n        let tester = vm.surveyAnswers[index].answer.includes(submittedAnswer)\n        if(tester) {\n          let removalIndex = vm.surveyAnswers[index].answer.findIndex( word => word == submittedAnswer)\n          vm.surveyAnswers[index].answer.splice(removalIndex, 1);\n        } else {\n          vm.surveyAnswers[index].answer.push(submittedAnswer)\n        }\n      } else {\n        vm.surveyAnswers[index].answer.push(submittedAnswer);\n      }\n    },\n    changeInputSelection(index, answer) {\n      let vm = this;\n      let submittedAnswer = answer;\n      vm.surveyAnswers[index] =  {\n        answer: submittedAnswer \n        };\n    },\n    changeInputDetails(index, answer) {\n      let vm = this;\n      let submittedAnswer = answer;\n      vm.surveyAnswers[index].explanation = submittedAnswer;\n    },\n    changeActivePage(type) {\n      this.activeLandingPage = type;\n    },\n    closeModal: function() {\n      this.$emit(\"closePreviewModal\");\n      EventService.$emit(\"loading\", \"hide\");\n    },\n  }\n};\n</script>\n\n\n<style scoped>\n\n.modal-mask {\n  height: auto;\n}\n\n.preview-items-container {\n  text-align: center;\n  background: white;\n  width: calc(100vw - 110px);\n  min-height: 90vh;\n  margin: 110px 0px 50px 55px;\n  padding-top: 20px;\n}\n\n.fa-times {\n  position: absolute;\n  right: 0;\n  margin-right: 55px;\n  margin-top: 10px;\n  font-size: 35px;\n  color: grey;\n  cursor: pointer;\n}\n\n.modal-header {\n  color: lightgrey;\n  font-size: 35px;\n}\n\n.survey-questions {\n  display: flex;\n  flex-direction: column;\n  margin-top: 30px;\n  padding-bottom: 30px;\n}\n\n.survey-question-header {\n  display: flex;\n  flex-direction: column;\n}\n\n.survey-question-header span {\n  margin-left: 30px;\n}\n\n.question-body { \n  margin: 25px 0 15px 0;\n}\n\n.radial-range {\n  display: flex;\n  flex-direction: row;\n  width: 400px;\n  margin-top: 20px;\n  justify-content: space-around;\n  margin-left: 50%;\n  transform: translateX(-50%);\n}\n\n.radial-container {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-content: center;\n  align-items: center;\n}\n\n.radial-container input {\n  margin-top: 10px;\n}\n\n.review-name {\n  padding: 10px;\n  font-size: 20px;\n}\n\n.review-logo {\n  width: 60%;\n  min-width: 400px;\n  margin: 10px auto;\n}\ndiv.review-logo span img {\n  width: 400px;\n}\n\n.review-text {\n  padding: 20px;\n}\n\n.review-text-header {\n  padding: 10px;\n  font-size: 56px;\n  font-family: Raleway;\n  color: black;\n  font-weight: bold;\n}\n\n.review-text-line2 {\n  padding: 10px;\n  font-size: 24px;\n  font-family: Raleway;\n  color: #999;\n  font-weight: bold;\n}\n\n.review-text-line3 {\n  padding: 10px;\n  width: 55%;\n  margin-left: 50%;\n  transform: translateX(-50%);\n  font-size: 16px;\n  line-height: 28px;\n  color: #666;\n}\n\n.review-label {\n  font-size: 24px;\n}\n\n.review-options {\n  width: 60%;\n  margin: 20px auto;\n  padding: 20px 0;\n  border-top: 2px solid #ddd;\n  border-bottom: 2px solid #ddd;\n}\n\n.review-textarea {\n  width: 100%;\n  height: 60px;\n  border: none;\n}\n\n.review-length {\n  width: 100%;\n  text-align: left;\n  display: inline-block;\n  color: #888;\n  font-size: 14px;\n}\n\n.review-button {\n  margin: 20px;\n}\n\n.review-button button {\n  color: white;\n  background: #4fcbc5;\n  padding: 10px 20px;\n  font-weight: bold;\n}\n\n.review-box {\n  width: 80%;\n  margin: auto;\n  margin-bottom: 10px;\n  box-shadow: 0px 2px 8px rgba(0, 0, 0, .25);\n  padding: 15px 0 10px 0;\n}\n\n.review-container {\n  pointer-events: none\n}\n\n.row {\n  flex-direction: row;\n}\n\ntextarea {\n  width: 80%;\n  height: 75px;\n  max-width: 80%;\n  max-height: 75px;\n  min-width: 60%;\n  min-height: 75px;\n}\n\n@media only screen and (max-width: 700px) {\n  div.review-logo {\n    min-width: unset;\n    width: calc(100% - 20px);\n  }\n\n  div.review-logo span img {\n    width: 80%;\n  }\n\n  .review-options {\n    width: 90%;\n  }\n}\n\n@media only screen and (max-width: 480px) {\n  .review-box {\n    line-height: 25px;\n    margin: 5px;\n    width: 100%;\n  }\n  .review-textarea {\n    height: 150px;\n  }\n\n  .review-button {\n    margin: 0;\n  }\n\n  .review-text {\n    padding: 0;\n  }\n\n  .review-box {\n    width: 90%;\n  }\n\n  .review-text-header {\n    font-size: 25px;\n  }\n  .review-text-line2 {\n    font-size: 18px;\n  }\n  .review-text-line3 {\n    font-size: 14px;\n    line-height: 20px;\n  }\n}\n\n.modal-mask {\n  z-index: 10000;\n}\n\n.landing-page-type {\n  margin-left: 20px;\n  cursor: pointer;\n}\n\n.active {\n  color: #4fcbc5;\n}\n\n.explanation-container {\n  margin-top: 20px;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n}\n\n.explanation-container textarea{\n  margin-top: 10px;\n}\n\n</style>\n"]}]}