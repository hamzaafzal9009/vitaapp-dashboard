class=\"report-data\">\n      <line-chart height=\"225\" width=\"245\" :chart-data=\"report\" :options=\"chartOptions\" ></line-chart>\n    </div>\n  </div>\n</template>\n\n<script>\nimport LineChart from './../Charts/LineChart'\nimport ReportService from './../../services/ReportService'\nimport {EventService} from '../../main'\n\n\nexport default {\n  name: 'monthlyReport',\n  props: [\"monthlyData\"],\n  components: {LineChart},\n  data: function() {\n    return {\n      chartData: null,\n      chartOptions: {\n        scales: {\n          yAxes: [{\n            ticks: {\n              beginAtZero: true\n            }\n          }]\n        },\n        fill: false,\n        maintainAspectRatio: false,\n        title: {\n          display: false\n        },\n      },\n    }\n  },\n  created() {\n    this.getDataSetsAndLabels()\n  },\n  methods: {\n    getDataSetsAndLabels(){\n      let vm = this;\n      let datasets = [\n        {\n          label: 'Total Customers',\n          backgroundColor: '#FFBD3355',\n          data: []\n        },\n        {\n          label: 'Reviews',\n          backgroundColor: '#0C51E555',\n          data: []\n        },\n      ];\n      let labels = [];\n      for(var i in vm.monthlyData.data) {\n        labels.push(i);\n        datasets[0].data.push([vm.monthlyData.data[i].Customers]);\n        // datasets[0].label = ''\n        datasets[1].data.push([vm.monthlyData.data[i].Reviews]);\n        // datasets[1].data.push([vm.monthlyData.data[i].Rating]);\n      }      \n      vm.report = {\n        datasets,\n        labels\n      }\n    },\n    removeReport(data){\n      // console.log(data);\n      this.$emit('removeReport', data.i)\n    }\n  }\n}\n</script>\n\n<style scoped>\n\n.report-container {\n  width: 100%;\n  height: 315px;\n  background: white;\n  box-shadow: 1px 2px 5px rgba(0,0,0,.75);\n  border-radius: 5px;\n  margin: 20px;\n  display: flex;\n  flex-direction: column;\n  align-content: center;\n  align-items: center;\n  position: relative;\n}\n\n.report-title {\n  width: 90%;\n  display: flex;\n  /* justify-content: center; */\n  padding-left: 30px;\n  /* padding-bottom: 10px; */\n  padding-top: 10px;\n  /* border-bottom: 2px solid #efefef; */\n}\n\n.report-title span {\n  margin: 10px;\n  font-weight: bolder;\n}\n\n.title {\n  font-size: 18px;\n}\n\n.remove-report {\n  cursor: pointer;\n  color: #dadada;\n  position: absolute;\n  right: 0;\n  font-size: 25px;\n}\n\n.remove-report i {\n  transform: translateY(-10px);\n  margin-right: 5px;\n}\n\n.report-data {\n  margin-top: 0;\n  padding-bottom: 2px;\n}\n\n.report-data:hover {\n  cursor: auto;\n}\n\n</style>\n"]}]}