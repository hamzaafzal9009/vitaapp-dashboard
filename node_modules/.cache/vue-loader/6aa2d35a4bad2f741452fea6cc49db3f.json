" @click=\"hideBanner()\"><i class=\"fal fa-times\"></i></div>\n      <div class=\"message\">\n        <p>It looks like you have locations that aren't linked up to your Google My Business pages!<br />Click <a class=\"hvr-underline-from-center\" :href=\"gmbUrl + userId\">here</a> to link them up and get access to more advanced features.</p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport config from '../../config.js';\nimport UserService from '../../services/UserService'\n\nexport default {\n  name: \"gmbLinkBanner\",\n  data: function() {\n    return {\n      userId: '',\n      gmbUrl: \"\",\n      displayed: false,\n      isSuperAdmin: null,\n      businesses: {}\n    };\n  },\n  created() {\n    this.gmbUrl = config.gmbUrl;\n    this.checkForAdmin();\n  },\n  methods: {\n    checkForAdmin() {\n      var vm = this;\n      let currentUser = JSON.parse(localStorage.getItem('currentUser'));\n      vm.isSuperAdmin = currentUser.isSuperAdmin;\n      vm.userId = currentUser.userId;\n      if(currentUser.isSuperAdmin) {\n        vm.hideBanner();\n      } else {\n        vm.getUserLocations();\n      }\n    },\n    getUserLocations() {\n      let vm = this;\n      UserService.getAllUserLocations().then(response => {\n        let businesses = response;\n        vm.checkLocationsForGMB(businesses);\n      })\n    },\n    checkLocationsForGMB(businesses) {\n      let vm = this;\n      vm.displayed = businesses.some((e) => {\n       return !e.gmb\n      })\n    },\n    hideBanner() {\n      this.displayed = false;\n    }\n  }\n};\n</script>\n\n<style scoped>\n\n.dashboard-banner-wrapper {\n  width: 100%;\n  text-align: center;\n  display: flex;\n  z-index: 999;\n  position: fixed;\n  top: -100px;\n  transition: top 1s;\n  pointer-events: none;\n}\n\n.dashboard-banner {\n  width: 50%;\n  background: #4fcbc6;\n  color: white;\n  border-radius: 5px;\n  margin: 0 auto;\n  position: relative;\n  border-style: ridge;\n  pointer-events: initial;\n}\n\na {\n  color: #545454;\n  text-decoration: none;\n}\n\na.hvr-underline-from-center {\n  -webkit-transform: perspective(1px) translateZ(0);\n  transform: perspective(1px) translateZ(0);\n  position: relative;\n  overflow: hidden;\n}\n\na.hvr-underline-from-center::before {\n  content: \"\";\n  position: absolute;\n  z-index: -1;\n  left: 51%;\n  right: 51%;\n  bottom: 1px;\n  background: #545454;\n  height: 1px;\n  -webkit-transition-property: left, right;\n  transition-property: left, right;\n  -webkit-transition-duration: 0.3s;\n  transition-duration: 0.3s;\n  -webkit-transition-timing-function: ease-out;\n  transition-timing-function: ease-out;\n}\n\na.hvr-underline-from-center:hover::before,\na.hvr-underline-from-center:focus::before,\na.hvr-underline-from-center:active::before {\n  left: 0;\n  right: 0;\n}\n\n.close {\n  position: absolute;\n  top: 3px;\n  right: 10px;\n  font-size: 20px;\n  cursor: pointer;\n  transition: transform .5s;\n  line-height: 0;\n}\n\n.close:hover {\n  transform: rotate(180deg);\n}\n\n.displayed {\n  top: 5px;\n}\n\n</style>"]}]}