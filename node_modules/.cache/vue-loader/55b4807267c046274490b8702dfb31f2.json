{"remainingRequest":"/Library/WebServer/Documents/Projects/OPINIION FINAL/opiniionv4/node_modules/vue-loader/lib/index.js??vue-loader-options!/Library/WebServer/Documents/Projects/OPINIION FINAL/opiniionv4/src/components/LocationList.vue?vue&type=style&index=0&id=993b76ba&scoped=true&lang=css&","dependencies":[{"path":"/Library/WebServer/Documents/Projects/OPINIION FINAL/opiniionv4/src/components/LocationList.vue","mtime":1581270622000},{"path":"/Library/WebServer/Documents/Projects/OPINIION FINAL/opiniionv4/node_modules/css-loader/dist/cjs.js","mtime":499162500000},{"path":"/Library/WebServer/Documents/Projects/OPINIION FINAL/opiniionv4/node_modules/vue-loader/lib/loaders/stylePostLoader.js","mtime":499162500000},{"path":"/Library/WebServer/Documents/Projects/OPINIION FINAL/opiniionv4/node_modules/postcss-loader/src/index.js","mtime":499162500000},{"path":"/Library/WebServer/Documents/Projects/OPINIION FINAL/opiniionv4/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Library/WebServer/Documents/Projects/OPINIION FINAL/opiniionv4/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["LocationList.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAqeA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA","file":"LocationList.vue","sourceRoot":"src/components","sourcesContent":["<template>\n  <div class=\"location-list\">\n    <el-card>\n      <div class=\"card-header\">\n        <div class=\"card-title\">\n          <h3>Locations</h3>\n        </div>\n        <div class=\"card-actions\">\n          <el-button type=\"primary\" @click=\"editLocation()\" v-if=\"isSuperAdmin\">\n            <i class=\"fa fa-user-plus fa-flip-horizontal\"></i>\n            &nbsp;Add Location\n          </el-button>\n          <div class=\"input-group\">\n            <el-popover trigger=\"click\" class=\"filter clickable\" v-model=\"showFilters\">\n              <div class=\"filter-button\" :class=\"{'filter-active': filtersActive}\" slot=\"reference\">\n                <i class=\"fas fa-filter\"></i>\n              </div>\n              <div class=\"dropdown-filter-menu\">\n                <div class=\"dropdown-filter-option\">\n                  <label>Status</label>\n                  <el-select v-model=\"filters.status\" :collapse-tags=\"true\" multiple>\n                    <el-option class=\"capitalize\" v-for=\"(status, statusIndex) in locationStatus\" :key=\"statusIndex\" :value=\"status.value\" :label=\"status.label\">\n                    </el-option>\n                  </el-select>\n                </div>\n                <div class=\"dropdown-filter-option\">\n                  <label>Tags</label>\n                  <el-select v-model=\"filters.tags\" :collapse-tags=\"true\" multiple :multiple-limit=\"4\">\n                    <el-option v-for=\"tags in tagOptions\" :key=\"tags\" :value=\"tags\">\n                      {{tags}}\n                    </el-option>\n                  </el-select>\n                </div>\n                <div class=\"dropdown-filter-option\">\n                  <label>Google Linked</label>\n                  <el-select v-model=\"filters.GMB\" clearable>\n                    <el-option class=\"capitalize\" v-for=\"(value, index) in googleLinked\" :key=\"index\" :value=\"value.value\" :label=\"value.label\">\n                    </el-option>\n                  </el-select>\n                </div>\n                <div class=\"dropdown-filter-buttons\">\n                  <el-button type=\"text\" size=\"small\" style=\"color:#F48D46\" @click=\"resetFilters()\">Reset Filters</el-button>\n                  <el-button type=\"warning\" size=\"small\" round @click=\"searchLocations()\">Apply</el-button>\n                </div>\n              </div>              \n            </el-popover>\n            <el-input\n              v-model=\"searchText\"\n              @keyup.native.enter=\"searchLocations()\"\n              placeholder=\"Search Locations...\"\n            ><i slot=\"suffix\" class=\"far fa-search\"></i></el-input>\n          </div>\n        </div>\n      </div>\n      <el-popover trigger=\"click\" placement=\"bottom-end\">\n        <el-button type=\"primary\" slot=\"reference\" v-show=\"showBulkActions\" size=\"medium\" round>\n          <span >Actions &nbsp;<i class=\"fas fa-caret-down\"></i></span>\n        </el-button>\n        <div class=\"location-mass-actions-menu\">\n          <el-link round class=\"dropdown-item\" @click=\"massChangeStatus('active')\">\n            Set Active\n          </el-link>\n          <el-link round class=\"dropdown-item\" @click=\"massChangeStatus('inactive')\">\n            Set Inactive\n          </el-link>\n          <el-link round v-if=\"isSuperAdmin\" class=\"dropdown-item\" @click=\"massChangeStatus('deleted')\">\n            Delete Locations\n          </el-link>\n          <!-- <el-link round class=\"dropdown-item\" @click=\"openTagsMenu()\">\n            Add Tag(s)\n          </el-link> -->\n          <!-- <div class=\"tags-menu\" v-if=\"showTagsMenu\">\n            <h3>Add Tags</h3>\n            <el-input v-model=\"newTag\" @keyup.native.enter=\"addTagToList\" :collapse-tags=\"true\" multiple></el-input>\n            <div class=\"tags-container\">\n              <el-tag v-for=\"(tag, tagIndex) in tagsToAdd\" closable :key=\"tag\" @close=\"removeTagFromList(tagIndex)\">{{tag}}</el-tag>\n            </div>\n            <div class=\"tags-menu-buttons\">\n              <span class=\"discard clickable\" @click=\"discardTags\">Discard</span>\n              <el-button round type=\"primary\">ADD</el-button>\n            </div>\n          </div> -->\n        </div>\n      </el-popover>\n      <div class=\"block\" v-if=\"locations\">\n        <el-table size=\"small\" class=\"card-table-min-height\" row-class-name=\"clickable\" :data=\"locations\" highlight-current-row \n        empty-text=\"No results found based on your search criteria.\"\n        ref=\"customerTable\" height=\"calc(100vh - var(--navbar-height) - var(--footer-height) - 280px)\" @row-click=\"selectLocation($event)\" \n        @sort-change=\"handleSort\" @selection-change=\"handleSelectionChange\">\n          <el-table-column type=\"selection\" width=\"55\" v-model=\"selectedLocations\"></el-table-column>\n          <el-table-column prop=\"name\" label=\"Name\" min-width=\"200\" show-overflow-tooltip sortable=\"custom\" :sort-orders=\"sortOptions\" ></el-table-column>\n          <el-table-column label=\"Address\" min-width=\"300\">\n            <template\n              slot-scope=\"scope\"\n            >{{scope.row.address.street1}} {{scope.row.address.city}} {{scope.row.address.state}}</template>\n          </el-table-column>\n          <el-table-column prop=\"status\" label=\"Status\" min-width=\"150\" sortable=\"custom\" :sort-orders=\"sortOptions\" >\n            <template slot-scope=\"scope\">\n              <span v-if=\"scope.row.status == 'active'\" class=\"status-bubble subscribed\">{{scope.row.status}}</span>\n              <span v-if=\"scope.row.status == 'inactive'\" class=\"status-bubble unsubscribed\">{{scope.row.status}}</span>\n              <span v-if=\"scope.row.status == 'deleted'\" class=\"status-bubble deleted\">{{scope.row.status}}</span>\n            </template>\n          </el-table-column>\n          <!-- <el-table-column prop=\"tags\" label=\"Tags\" min-width=\"300\" show-overflow-tooltip>\n            <template slot-scope=\"scope\">\n              <el-tag class=\"tag\" size=\"small\" v-for=\"tag in scope.row.tags\" :key=\"tag\">{{ tag }}</el-tag>\n            </template>\n          </el-table-column> -->\n          <el-table-column label=\"Google Link\" prop=\"gmb\" sortable=\"custom\" :sort-orders=\"sortOptions\"  min-width=\"150\" show-overflow-tooltip>\n            <template slot-scope=\"scope\">\n              <el-button\n                type=\"success\"\n                size=\"mini\"\n                icon=\"far fa-check\"\n                round\n                v-if=\"scope.row.gmb && scope.row.gmb.accessToken\"\n              >&nbsp;&nbsp;Linked</el-button>\n              <el-button\n                class=\"btn-link-google\"\n                size=\"mini\"\n                round\n                icon=\"far fa-unlink\"\n                @click=\"linkGMB()\"\n                v-if=\"!scope.row.gmb || !scope.row.gmb.accessToken\"\n              >&nbsp;&nbsp;Not Linked</el-button>\n            </template>\n          </el-table-column>\n          <!-- <el-table-column prop=\"contacted\" label=\"Last Contact\" width=\"200\">\n          </el-table-column>-->\n        </el-table>\n      </div>\n      <div class=\"pagination block\" v-if=\"locations && locations.length\">\n        <span>Showing {{locations.length}} of {{paginationData.total}} Total Results</span>\n        <el-pagination\n          @current-change=\"searchLocations\"\n          background\n          layout=\"prev, pager, next\"\n          :current-page=\"paginationPage\"\n          :page-size=\"paginationData.perPage\"\n          :total=\"paginationData.total\"\n          small\n        ></el-pagination>\n        <el-select class=\"per-page-select\" size=\"mini\" v-model=\"paginationData.perPage\" @change=\"searchLocations()\">\n          <el-option\n            v-for=\"option in paginationData.perPageOptions\"\n            :value=\"option\"\n            :key=\"option\"\n          >{{option}}</el-option>\n        </el-select>\n      </div>\n      <div class=\"block\" v-else></div>\n    </el-card>\n    <!-- <addLocation :selectedLocation=\"selectedLocation\" :copiedLocation=\"copiedLocation\" v-if=\"openLocationModal\" v-on:closeModal=\"closeLocationModal()\" v-on:closeAndAssignModal=\"closeAndAssignModal()\" /> -->\n    <addLocation\n      :selectedLocation=\"selectedLocation\"\n      v-if=\"openLocationModal\"\n      :openLocationModal=\"openLocationModal\"\n      v-on:closeEditor=\"closeLocationEditor()\"\n    />\n    <locationDetail\n      :selectedLocation=\"selectedLocation\"\n      v-if=\"openLocationDetailModal\"\n      v-on:closeModal=\"closeLocationModal()\"\n      v-on:editLocation=\"openLocationEditor()\"\n\n    />\n    <addAssign v-if=\"openAddAssignModal\" v-on:closeModal=\"closeLocationModal()\" />\n  </div>\n</template>\n\n<script>\nimport AddLocation from './AddLocation'\nimport LocationDetail from './LocationDetail'\nimport AddAssign from './AddAssign'\nimport LocationService from './../services/LocationService'\nimport UserService from './../services/UserService'\nimport config from './../config'\nimport table from './Tables/autocompletetable.js';\nimport { EventService } from \"../main\";\n\nexport default {\n  name: \"locationList\",\n  components: {\n    AddLocation,\n    LocationDetail,\n    AddAssign\n  },\n  data: function() {\n    return {\n      locations: [],\n      searchText: '',\n      filters: {\n        status: [],\n        tags: [],\n        GMB: '',\n      },\n      googleLinked: [{value: 'linked', label: 'Linked'}, {value: 'unlinked', label: 'Unlinked'}],\n      locationStatus: [{value: 'active', label: 'Active'}, {value: 'inactive', label: 'Inactive'}],\n      sortOptions: ['ascending', 'descending'],\n      selectedLocations: [],\n      tagOptions: [],\n      showFilters: false,\n      openLocationModal: false,\n      showBulkActionsPopover: false,\n      openLocationDetailModal: false,\n      openAddAssignModal: false,\n      selectedLocation: null,\n      sortType: \"\",\n      isSuperAdmin: false,\n      copiedLocation: null,\n      userId: \"\",\n      gmbUrl: \"\",\n      isSelected: \"line\",\n      paginationData: {\n        perPage: 25,\n        currentPage: 0,\n        perPageOptions: [5, 10, 25],\n        total: 0,\n        changePage: \"setPageNum\",\n        rowChange: \"searchCustomers\"\n      }\n    };\n  },\n  computed: {\n    filtersActive() {\n      let vm = this;\n      if(vm.filters.status.length || vm.filters.tags.length || vm.filters.GMB) {\n        return true;\n      } else {\n        return false;\n      }\n    },\n    paginationPage() {\n      return this.paginationData.currentPage + 1;\n    },\n    showBulkActions() {\n      let vm = this;\n      if(vm.selectedLocations && vm.selectedLocations.length) {\n        return true;\n      }\n      return false;\n    }\n  },\n  created() {\n    var vm = this;\n    let currentUser = JSON.parse(localStorage.getItem(\"currentUser\"));\n    vm.isSuperAdmin = currentUser.isSuperAdmin;\n    vm.userId = currentUser.userId;\n    vm.searchLocations();\n    vm.searchOnRoute();\n    UserService.logPageVisit(\"locationList\");\n    vm.gmbUrl = config.gmbUrl;\n    vm.tagOptions = table.locationTags;\n    EventService.$on(\"locationChange\", this.searchLocations);\n  },\n  beforeDestroy: function () {\n    EventService.$off(\"locationChange\", this.searchLocations);\n  },\n  methods: {\n    searchOnRoute() {\n      if (this.$route.params.location) {\n        this.searchText = this.$route.params.location;\n        this.searchLocations();\n      }\n    },\n    setPageNum(pageNum) {\n      this.selectedPage = pageNum;\n      this.searchLocations(pageNum);\n    },\n    handleSort(data) {\n      let vm = this;\n      let direction = '-';\n      if(vm.sortType[0] == '-') {\n        direction = ''\n      }\n      vm.sortType = direction + data.prop;      \n      vm.searchLocations();\n    },\n    searchLocations(pageNum) {\n      EventService.$emit(\"loading\", \"show\");\n      this.selectedLocations = [];\n      this.paginationData.pageNum = pageNum;\n      this.showBulkActionsPopover = false;\n      var searchObject = {};\n      if (this.searchText) searchObject.searchText = this.searchText.trim();\n      if (this.sortType) {\n        searchObject.sort = this.sortType;\n      }\n\n      if (pageNum) {\n        searchObject.pageNum = this.paginationData.pageNum;\n        this.paginationData.currentPage = pageNum -1;\n      } else {\n        this.selectedPage = 0;\n        this.paginationData.currentPage = 0;\n      }\n      var locationIdArray = [];\n      let locationId = localStorage.getItem(\"locationId\");\n      if (locationId) {\n        locationIdArray.push(locationId);\n      } else {\n        let locOptions = JSON.parse(localStorage.getItem(\"locationOptions\"));\n        locOptions.map(el => locationIdArray.push(el.locationId));\n      }\n      searchObject.resPerPage = this.paginationData.perPage;\n      searchObject.locations = locationIdArray;\n      var vm = this;\n      vm.showFilters = false;\n      searchObject.filters = vm.filters;\n      LocationService.searchLocationsAdvanced(searchObject).then(\n        function(response) {\n          if (response[0]) vm.locations = response[0].results;\n          if (response[0].count && response[0].count[0]) {\n            vm.paginationData.total = response[0].count[0].total;\n          }\n          EventService.$emit(\"loading\", \"hide\");\n        },\n        function(error) {\n          console.log(error);\n          EventService.$emit(\"loading\", \"hide\");\n        }\n      );\n    },\n    linkGMB() {\n      let gmb = `${this.gmbUrl + this.userId}`;\n      window.open(gmb, '_self');\n    },\n    openLocationEditor() {\n      this.openLocationDetailModal = false;\n      this.openLocationModal = true;\n    },\n    closeLocationEditor() {\n      this.selectedLocation = null;\n      this.openLocationModal = false;\n      this.$router.go();\n      // this.searchLocations();\n    },\n    toggleSort(setSortType) {\n      if (this.sortType != setSortType) {\n        this.sortType = setSortType;\n      } else {\n        this.sortType = \"-\" + this.sortType;\n      }\n      this.searchLocations(this.selectedPage);\n    },\n    selectStyle(style) {\n      this.isSelected = style;\n    },\n    editLocation(location) {\n      if (location) {\n        LocationService.getLocation(location.locationId).then(response => {\n          this.selectedLocation = response;\n          if (this.selectedLocation) {\n            this.openLocationModal = true;\n          }\n        });\n      } else {\n        this.openLocationModal = true;\n      }\n    },\n    followLink(targetPage) {\n      var vm = this;\n      this.$router.push(targetPage);\n    },\n    copyLocation(location) {\n      LocationService.getLocation(location.locationId).then(response => {\n        this.copiedLocation = response;\n        if (this.copiedLocation) {\n          this.openLocationModal = true;\n        }\n      });\n    },\n    handleSelectionChange(customers) {\n      this.selectedLocations = customers;\n    },\n    selectLocation(location) {\n      LocationService.getLocation(location.locationId).then(response => {\n        this.selectedLocation = response;\n        if (this.selectedLocation) {\n          this.openLocationDetailModal = true;\n        }\n      });\n    },\n    newLocationModal() {\n      this.openLocationModal = true;\n    },\n    deleteLocation(location) {\n      if (window.confirm(\"Are you sure you want to delete this location?\")) {\n        var vm = this;\n        // if(deleting === user.pwd) {\n        LocationService.deleteLocation(location.locationId).then(\n          function(response) {\n            vm.searchLocations(this.selectedPage);\n          },\n          function(error) {\n            console.log(error);\n          }\n        );\n      }\n    },\n    setLocationStatus(location, status) {\n      var vm = this;\n      if (vm.isSuperAdmin) {\n        LocationService.setLocationStatus(location.locationId, status).then(\n          function(response) {},\n          function(error) {\n            console.log(error);\n          },\n          vm.searchLocations(this.selectedPage)\n        );\n      }\n    },\n    canActionLocation(action, location) {\n      if (action == \"activate\" && location.status != \"active\") return true;\n      if (action == \"deactivate\" && location.status != \"inactive\") return true;\n      if (action == \"delete\" && location.status != \"deleted\") return true;\n      if (action == \"edit\") return true;\n      return false;\n    },\n    massChangeStatus(status) {\n      let vm = this;\n      this.$confirm('Are you sure you want to change these statuses?', 'Confirmation', {\n        confirmButtonText: 'Yes',\n        cancelButtonText: 'Cancel',\n        type: \"error\"\n      }).then(() => {\n        this.selectedLocations.map(e => {\n          LocationService.setLocationStatus(e.locationId, status).then(response => {\n            if(response) {\n              EventService.$emit('successMessage', 'Status changed successfully!')\n              vm.searchLocations()\n            } else {\n              EventService.$emit('errorMessage', 'Something went wrong, please try again.')\n            }\n          })\n        })\n      }).catch(() => {\n        this.$message({\n          type: 'info',\n          message: \"Delete Cancelled\"\n        })\n      }) \n    },\n    resetFilters() {\n      this.filters = {\n        status: [],\n        tags: [],\n        GMB: '',\n      }\n    },\n    closeLocationModal() {\n      this.selectedLocation = null;\n      this.copiedLocation = null;\n      this.openLocationModal = false;\n      this.openLocationDetailModal = false;\n      this.selectedLocations = [];\n      this.openAddAssignModal = false;\n      // this.searchLocations(this.selectedPage);\n      EventService.$emit(\"loading\", \"hide\");\n    },\n    closeAndAssignModal() {\n      this.selectedLocation = null;\n      this.copiedLocation = null;\n      this.openLocationModal = false;\n      this.openLocationDetailModal = false;\n      this.openAddAssignModal = false;\n      this.openAddAssignModal = true;\n      EventService.$emit(\"loading\", \"hide\");\n    },\n    appendImageHeader(imgText) {\n      if (!imgText)\n        imgText =\n          \"\";\n      if (imgText && imgText[0] == \",\") {\n        return \"data:image/png;base64\" + imgText;\n      } else {\n        return \"data:image/png;base64,\" + imgText;\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n\n.per-page-select {\n  max-width: 80px;\n}\n.card-table {\n  /* min-height: 600px; */\n}\n.el-card {\n  /* height: 80vh; */\n}\n/* .el-pagination {\n  width: 400px;\n} */\n.location-gmb-button {\n  position: absolute;\n  margin-left: 20px;\n  font-size: 14px;\n}\n.location-gmb-button button {\n  background: #4fccc6;\n  padding: 5px 10px;\n  color: white;\n  cursor: pointer;\n}\n\n.location-controls {\n  display: flex;\n  justify-content: space-between;\n  color: #545454;\n  font-size: 20px;\n  background: white;\n  margin-bottom: 20px;\n  box-shadow: 0px 2px 4px rgba(0, 0, 0, 0.25);\n  padding: 0px 10px 0px 0px;\n  border-radius: 1px;\n  align-content: center;\n  align-items: center;\n}\n\n.location-controls-add {\n  float: left;\n  padding: 10px 0 0 30px;\n  cursor: pointer;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-content: center;\n  align-items: center;\n  color: #4fcbc5;\n}\n\n.location-controls-add .label-text {\n  font-size: 12px;\n  color: #545454;\n  margin-top: 3px;\n  font-weight: bold;\n}\n\n.location-controls-search {\n  background: transparent;\n  margin: 20px 20px;\n  border: 2px solid #efefef;\n  border-radius: 1px;\n  width: 350px;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.location-controls-search input {\n  border: none;\n  padding: 15px 15px 5px 15px;\n  font-size: 12px;\n}\n\n.location-controls-search-icon {\n  vertical-align: middle;\n  margin-right: 15px;\n  color: #4fcbc5;\n  font-size: 16px;\n}\n\n.right-search-container {\n  display: flex;\n  justify-content: center;\n  align-content: center;\n  align-items: center;\n}\n\n.style-switch {\n  display: flex;\n  justify-content: space-between;\n  color: lightgrey;\n}\n\n.style-switch i {\n  margin-right: 20px;\n  font-size: 24px;\n  cursor: pointer;\n}\n\n.style-selected {\n  color: #4fcbc5