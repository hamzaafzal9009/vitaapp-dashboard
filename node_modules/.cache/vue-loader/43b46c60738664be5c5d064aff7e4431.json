nput v-model=\"searchText\" placeholder=\"Filter Emails...\"/>\n        <span class=\"email-controls-search-icon\">\n          <i class=\"fas fa-search\"></i>\n        </span>\n      </div>\n    </div>\n    <div class=\"email-table\">\n      <div class=\"email-header\" style=\"font-family: FuturaPTDemi;font-weight: bolder;\">\n        <div class=\"email-cell-0\" v-if=\"!locationId\">Location</div>\n        <div class=\"email-cell-1\" style=\"cursor: pointer;\" @click=\"toggleSort('date')\">\n          <span>Date </span>\n          <span class=\"sort-indicator\" v-if=\"sortType == '-date'\"><i class=\"fas fa-sort-up\"></i></span>\n          <span class=\"sort-indicator\" v-if=\"sortType == 'date'\"><i class=\"fas fa-sort-down\"></i></span>\n        </div>\n        <div class=\"email-cell-2\" style=\"cursor: pointer;\" @click=\"toggleSort('email')\">\n          <span>Email </span>\n          <span class=\"sort-indicator\" v-if=\"sortType == '-email'\"><i class=\"fas fa-sort-up\"></i></span>\n          <span class=\"sort-indicator\" v-if=\"sortType == 'email'\"><i class=\"fas fa-sort-down\"></i></span>\n        </div>\n        <div class=\"email-cell-3\" style=\"cursor: pointer;\" @click=\"toggleSort('subject')\">\n          <span>Subject </span>\n          <span class=\"sort-indicator\" v-if=\"sortType == '-subject'\"><i class=\"fas fa-sort-up\"></i></span>\n          <span class=\"sort-indicator\" v-if=\"sortType == 'subject'\"><i class=\"fas fa-sort-down\"></i></span>\n        </div>\n        <div class=\"email-cell-4\" style=\"cursor: pointer;\" @click=\"toggleSort('template')\">\n          <span>Template </span>\n          <span class=\"sort-indicator\" v-if=\"sortType == '-template'\"><i class=\"fas fa-sort-up\"></i></span>\n          <span class=\"sort-indicator\" v-if=\"sortType == 'template'\"><i class=\"fas fa-sort-down\"></i></span>\n        </div>\n        <!-- <div class=\"email-cell-5\">Template Variables</div> -->\n        <div class=\"email-cell-6\" style=\"cursor: pointer;\" @click=\"toggleSort('status')\">\n          <span>Status </span>\n          <span class=\"sort-indicator\" v-if=\"sortType == '-status'\"><i class=\"fas fa-sort-up\"></i></span>\n          <span class=\"sort-indicator\" v-if=\"sortType == 'status'\"><i class=\"fas fa-sort-down\"></i></span>\n        </div>\n        <!-- <div class=\"email-cell-7\">Actions</div> -->\n      </div>\n      <div class=\"email-row\" v-for=\"email in filteredEmail\">\n        <div class=\"email-cell-0\" v-if=\"!locationId\">{{email.locationName}}</div>\n        <div class=\"email-cell-1\">{{displayDate(email.date)}}</div>\n        <div class=\"email-cell-2\">{{email.email}}</div>\n        <div class=\"email-cell-3\">{{email.subject}}</div>\n        <div class=\"email-cell-4\">{{email.template}}</div>\n        <!-- <div class=\"email-cell-5\">{{email.templateVars}}</div> -->\n        <div class=\"email-cell-6\">{{email.status}}</div>\n        <!-- <div class=\"email-cell-7\"></div> -->\n      </div>\n      <div class=\"email-row empty\" v-if=\"filteredEmail.length == 0\">No emails match your filter.</div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport CommsService from './../services/CommsService'\nimport {EventService} from '../main'\n\nexport default {\n  name: 'emailLog',\n  data: function() {\n    return {\n      emailList: [],\n      locationId: null,\n      searchText: '',\n      sortType: '-date',\n      getQueue: false\n    }\n  },\n  computed: {\n    filteredEmail: function() {\n      var vm = this;\n      var filteredList = this.emailList.filter(function(email) {\n        var emailMatch = false;\n        emailMatch = emailMatch || email.email.toLowerCase().indexOf(vm.searchText.toLowerCase()) > -1;\n        emailMatch = emailMatch || email.subject.toLowerCase().indexOf(vm.searchText.toLowerCase()) > -1;\n        emailMatch = emailMatch || email.template.toLowerCase().indexOf(vm.searchText.toLowerCase()) > -1;\n        emailMatch = emailMatch || email.status.toLowerCase().indexOf(vm.searchText.toLowerCase()) > -1;\n\n        return emailMatch;\n      });\n\n      var filteredSortedList = filteredList.sort((a,b) => {\n        if (this.sortType == 'email') {\n          return a.email.localeCompare(b.email);\n        } else if (this.sortType == '-email') {\n          return b.email.localeCompare(a.email);\n        } else if (this.sortType == 'date') {\n          return a.date - b.date;\n        } else if (this.sortType == '-date') {\n          return b.date - a.date;\n        } else if (this.sortType == 'subject') {\n          return a.subject.localeCompare(b.subject);\n        } else if (this.sortType == '-subject') {\n          return b.subject.localeCompare(a.subject);\n        } else if (this.sortType == 'template') {\n          return a.template.localeCompare(b.template);\n        } else if (this.sortType == '-template') {\n          return b.template.localeCompare(a.template);\n        } else if (this.sortType == 'status') {\n          return a.status.localeCompare(b.status);\n        } else if (this.sortType == '-status') {\n          return b.status.localeCompare(a.status);\n        } else {\n          true;\n        }\n      });\n\n      // console.log('fsl: ', filteredSortedList);\n\n      return filteredSortedList;\n    }\n  },\n  created() {\n    this.getEmailData();\n    EventService.$on('locationChange', this.getEmailData);\n  },\n  beforeDestroy: function () {\n    EventService.$off('locationChange', this.getEmailData);\n  },\n  methods: {\n    getEmailData() {\n      EventService.$emit(\"loading\", \"show\");\n      var vm = this;\n      this.locationId = localStorage.getItem(\"locationId\");\n      CommsService.getEmailLogs(this.getQueue).then(\n        function(response) {\n          vm.emailList = response;\n          EventService.$emit(\"loading\", \"hide\");\n        }, function(error) {\n          console.log(error);\n          EventService.$emit(\"loading\", \"hide\");\n      });\n    },\n    displayDate(millis) {\n      var date = new Date(millis);\n      return (1 + date.getMonth()) + '/' + date.getDate() + '/' + (date.getFullYear() % 100);\n    },\n    toggleSort(setSortType) {\n      if (this.sortType != setSortType) {\n        this.sortType = setSortType;\n      } else {\n        this.sortType = '-' + this.sortType;\n      }\n    },\n    queueToggle() {\n      if (this.getQueue) {\n        this.toggleSort('date');\n      } else {\n        this.toggleSort('-date');\n      }\n      this.getEmailData();\n    }\n  }\n}\n</script>\n\n<style scoped>\n\n.email-header-top {\n  display: flex;\n  justify-content: space-between;\n  background: #fff;\n  box-shadow: 0px 2px 4px rgba(0,0,0,.25);\n  border-radius: 1px;\n  margin-bottom: 20px;\n  align-items: center;\n}\n\n.email-controls-toggle {\n  background: transparent;\n  padding: 15px 15px 0px 15px;\n  margin: 5px 0 10px 20px;\n  color: #545454;\n  font-size: 16px;\n  height: 38px;\n}\n\n.email-controls-search {\n  background: transparent;\n  margin: 20px 20px;\n  border: 2px solid #efefef;\n  border-radius: 1px;\n  width: 350px;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.email-controls-search input {\n  border: none;\n  padding: 15px 15px 5px 15px;\n  font-size: 12px;\n}\n\n.email-controls-search-icon {\n  vertical-align: middle;\n  margin-right: 15px;\n  color: #4fcbc5;\n  font-size: 16px;\n}\n\n.email-table {\n  background: white;\n  font-size: 14px;\n  padding: 10px 0px;\n  clear: both;\n  box-shadow: 0px 2px 4px rgba(0,0,0,.25);\n  border-radius: 1px;\n}\n\n.email-header {\n  width: calc(100% - 20px);\n  font-weight: bold;\n  padding: 10px 0px 20px 20px;\n  /* border-top: 1px solid #efefef; */\n  border-bottom: 3px solid #efefef;\n  height: 14px;\n  display: flex;\n  flex-wrap: nowrap;\n  justify-content: space-evenly;\n}\n\n.email-header div, .email-row div {\n  margin: auto;\n}\n\n.email-row {\n  width: calc(100% - 20px);\n  clear: both;\n  padding: 10px;\n  border-bottom: 1px solid #efefef;\n  height: auto;\n  display: flex;\n  flex-wrap: nowrap;\n  justify-content: space-evenly;\n}\n\n.email-row:nth-child(even) {\n  /* background: #efefef; */\n}\n\n.email-row:hover {\n  background: #dadada;\n}\n\n.email-cell-0 {\n  flex: 1 1 100px;\n}\n\n.email-cell-1 {\n  flex: 0 0 75px;\n}\n\n.email-cell-2 {\n  flex: 1 1 200px;\n}\n\n.email-cell-3 {\n  flex: 1 1 100px;\n}\n\n.email-cell-4 {\n  flex: 0 0 150px;\n}\n\n.email-cell-5 {\n  flex: 1 1 200px;\n}\n\n.email-cell-6 {\n  flex: 0 0 60px;\n  text-transform: capitalize;\n  text-align: center;\n}\n\n.email-cell-7 {\n  flex: 0 0 75px;\n  text-align: center;\n}\n\n.email-cell-7 span {\n  margin-right: 5px;\n  cursor: pointer;\n}\n\n.sort-indicator {\n  display: none;\n}\n\n\n.email-cell-0:hover .sort-indicator {\n  display: inline;\n}\n.email-cell-1:hover .sort-indicator {\n  display: inline;\n}\n.email-cell-2:hover .sort-indicator {\n  display: inline;\n}\n.email-cell-3:hover .sort-indicator {\n  display: inline;\n}\n.email-cell-4:hover .sort-indicator {\n  display: inline;\n}\n.email-cell-5:hover .sort-indicator {\n  display: inline;\n}\n.email-cell-6:hover .sort-indicator {\n  display: inline;\n}\n\n\n</style>\n"]}]}