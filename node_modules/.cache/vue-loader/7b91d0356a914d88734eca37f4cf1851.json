\"removeReport(taskData)\"></i></span>\n    </div>\n    <div class=\"report-data\">\n      <div class=\"tasks-header\">\n        <span>{{report.tasks}} Open Tasks </span>\n        <span>{{report.messages}} New Messages </span>\n      </div>\n      <div class=\"task-list\" v-for=\"data in report\">\n        <span>{{checkDate(data.createdDate)}}</span>\n        <span v-if=\"data.name\">{{data.name}}</span>\n        <span v-if=\"data.status\">{{data.status}}</span>      \n      </div>      \n    </div>\n  </div>\n</template>\n\n<script>\nimport ReportService from './../../services/ReportService'\nimport {EventService} from '../../main'\n\n\nexport default {\n  name: 'taskChart',\n  props: [\"taskData\"],\n  data: function() {\n    return {\n\n    }\n  },\n  created() {    \n    this.report = this.taskData.data.tasks;\n    this.report.messages = 1;\n    this.report.tasks = 2;\n  },\n  methods: {\n    checkDate(date) {\n      let newTime = new Date(+date)     \n      return newTime.toString().substr(0,24);\n    },\n    getDataSetsAndLabels(){\n      let vm = this;\n      let datasets = [\n        {\n          label: 'Total Customers',\n          backgroundColor: '#FFBD3355',\n          data: []\n        },\n        {\n          label: 'Rating',\n          backgroundColor: '#4fcbc555',\n          data: []\n        },\n        {\n          label: 'Reviews',\n          backgroundColor: '#0C51E555',\n          data: []\n        },\n      ];\n      let labels = [];\n      for(var i in vm.taskData.data) {\n        labels.push(i);\n        datasets[0].data.push([vm.taskData.data[i].Customers]);\n        datasets[1].data.push([vm.taskData.data[i].Rating]);\n        datasets[2].data.push([vm.taskData.data[i].Reviews]);\n      }\n      \n      vm.report = {\n        datasets,\n        labels,\n        tasks: 2,\n        messages: 1\n      }\n      console.log('rep', vm.report)\n    },\n    removeReport(data){\n      // console.log(data);\n      this.$emit('removeReport', data.i)\n    }\n  }\n}\n</script>\n\n<style scoped>\n\n.report-container {\n  width: 100%;\n  height: 345px;\n  background: white;\n  box-shadow: 1px 0px 5px rgba(0,0,0,.35);\n  border-radius: 5px;\n  margin: 20px;\n  display: flex;\n  flex-direction: column;\n  align-content: center;\n  align-items: center;\n  position: relative;\n}\n\n.report-title {\n  width: 90%;\n  display: flex;\n  /* justify-content: center; */\n  padding-left: 30px;\n  padding-bottom: 10px;\n  padding-top: 10px;\n  border-bottom: 2px solid #efefef;\n}\n\n.report-title span {\n  margin: 10px;\n  font-weight: bolder;\n}\n\n.title {\n  font-size: 18px;\n}\n\n.remove-report {\n  cursor: pointer;\n  color: #545454;\n  position: absolute;\n  right: 0;\n}\n\n.report-data {\n  margin-top: 0;\n  padding-bottom: 2px;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  width: 100%;\n}\n\n.task-list {\n  display: flex;\n  justify-content: space-between;\n  padding: 10px 15px;\n}\n\n.task-list span {\n  width: 100px;\n  font-size: 12px;\n}\n\n.tasks-header {\n  margin: 5px 15px;\n  display: flex;\n  justify-content: space-around;\n  background: #efefef;\n  padding: 10px;\n}\n\n.report-data:hover {\n  cursor: auto;\n}\n\n</style>\n"]}]}