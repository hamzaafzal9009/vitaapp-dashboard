   <div class=\"modal-header\">\n          <span class=\"modal-header-text\">Assign User to Locations</span>\n          <span class=\"modal-image\">\n            <div ref=\"userImageDiv\" :style=\"{'background-image': userImageBackground}\"/>\n          </span>\n          <div class=\"modal-header-subtext\">{{user.firstName}} {{user.lastName}} - {{user.email}}</div>\n        </div>\n        <div class=\"modal-body\">\n          <div class=\"assign-controls-search\">\n            <input v-model=\"searchText\" @keyup.enter=\"runSearch(searchText)\" placeholder=\"Search Locations...\"/>\n            <span class=\"assign-controls-search-icon\">\n              <i class=\"fas fa-search\" @click=\"runSearch(searchText)\"></i>\n            </span>\n          </div>\n          <div class=\"user-assign-table\">\n            <div class=\"user-assign-header-row\">\n              <div class=\"user-assign-table-cell-0\">Location</div>\n              <div class=\"user-assign-table-cell-1\">Role</div>\n              <div class=\"user-assign-table-cell-2\">Receive Communication</div>\n            </div>\n            <div class=\"user-assign-row\" v-for=\"location in filteredUserLocations\">\n              <div class=\"user-assign-table-cell-0\" >{{location.name}}</div>\n              <div class=\"user-assign-table-cell-1\">\n                <select v-if=\"actingUserRole.isSuperAdmin || actingUserRole.role == 'owner' || actingUserRole.role == 'manager'\" v-model=\"location.role\" @change=\"checkForEmail(location); saveUserLocation(location)\">\n                  <option value=\"owner\" title=\"Is the account admininistrator.\">Owner</option>\n                  <option value=\"manager\" title=\"Is able to manage users and customers, but cannot change location details.\">Manager</option>\n                  <option value=\"agent\" title=\"Is able to add and respond to customers.\">Agent</option>\n                  <option value=\"\">None</option>\n                </select>\n                <span v-if=\"!actingUserRole.isSuperAdmin && actingUserRole.role != 'owner' && actingUserRole.role != 'manager'\" style=\"text-transform: capitalize\">\n                  {{location.role}}\n                </span>\n              </div>\n              <div class=\"user-assign-table-cell-2 font-shrink\">\n                <div v-if=\"location.communications && location.role\" v-for=\"(coms, comIndex) in location.communications\">\n                  <span class=\"location-tag\" :class=\"{'active': coms.receives == true}\" @click=\"changeCommunication(location, comIndex)\">{{coms.type}}</span>\n                </div>\n              </div>\n            </div>\n            <div class=\"user-assign-row\" v-if=\"userLocations.length == 0\">No locations matched your filter.</div>\n          </div>\n        </div>\n        <div class=\"modal-footer\">\n          <span class=\"modal-footer-button\"><button class=\"modal-default-button\" @click=\"closeModal()\">Save</button></span>\n        </div>\n      </div>\n    </div>\n  </transition>\n</template>\n\n<script>\nimport UserService from './../services/UserService'\nimport { EventService } from '../main';\n\n\nexport default {\n  name: 'userAssign',\n  props: ['selectedUser'],\n  data: function() {\n    return {\n      actingUserRole: {},\n      user: {},\n      userLocations: [],\n      searchText: '',\n      userImageBackground: 'url(\"data:image/png;base64,\")'\n    }\n  },\n  computed: {\n    filteredUserLocations(){\n      return this.userLocations;\n    }\n  },\n  created() {\n    this.user = this.selectedUser;\n    var vm = this;\n    UserService.getUserRole().then(function(currentUserRole) {\n      vm.actingUserRole = currentUserRole;\n      vm.getAssignableUserLocations();\n    });\n    this.setUserImageBackground();\n  },\n  methods: {\n    closeModal: function () {\n      EventService.$emit( \"successMessage\", \"All changes have been saved.\" );\n      this.$emit('closeModal');\n    },\n    setUserImageBackground: function() {\n      if(this.user.userImage && this.user.userImage[0] == ',') {\n        this.userImageBackground = \"url('data:image/png;base64\" + this.user.userImage + \"')\";\n      } else {\n      this.userImageBackground = \"url('data:image/png;base64,\" + this.user.userImage + \"')\";\n      }\n    },\n    getAssignableUserLocations: function() {\n      var vm = this;\n      UserService.searchAssignableUserLocations(this.user.userId, '').then(\n        function(response) {\n          // console.log(response);\n          vm.userLocations = response\n\n        }, function(error) {\n          console.log(error);\n      });\n    },\n    runSearch: function() {\n      var vm = this;\n      UserService.searchAssignableUserLocations(this.user.userId, this.searchText).then(\n        function(response) {\n          // console.log('res2', response);\n          vm.userLocations = response;\n        }, function(error) {\n          console.log(error);\n      });\n    },\n    checkForEmail(location) {\n      if(location.role !== '') {\n        location.communications = [\n          {\n            \"type\" : \"New Review\",\n            \"receives\" : true\n          },\n          {\n            \"type\" : \"Potential Negative Review\",\n            \"receives\" : true\n          },\n          {\n            \"type\" : \"New Customer Message\",\n            \"receives\" : true\n          }\n        ];\n      } else if(location.role == '') {\n        location.communications = [];\n      } else {\n        null;\n      }\n    },\n    saveUserLocation: function(location) {\n      var vm = this;\n      if (location.role == '') {\n        location.getsEmail = false;\n        location.communications.map(el => {\n          el.receives = false;\n        })\n        vm.changeCommunication(location);\n        UserService.saveUserLocation(this.user.userId, location.locationId, location.role, location.getsEmail).then( function(response) {\n          }, function(error) {\n            console.log(error);\n        });\n        this.unassignUser(location);\n      } else {\n        UserService.saveUserLocation(this.user.userId, location.locationId, location.role, location.getsEmail).then(\n          function(response) {\n          }, function(error) {\n            console.log(error);\n        });\n      }\n    },\n    changeCommunication(location, index){\n      let vm = this;\n      if (actingUserRole && (actingUserRole.userId == user.userId || ['admin', 'owner', 'manager'].indexOf(actingUserRole.role) > -1)) {\n        if(index >= 0) location.communications[index].receives = !location.communications[index].receives;\n        UserService.saveUserComms(location, vm.user).then(response => {\n          if(response.success) {\n            EventService.$emit('successMessage', 'Communication Preferences Have Been Updated!')\n          } else {\n            location.communications[index].receives = !location.communications[index].receives;\n            EventService.$emit('errorMessage', 'Something went wrong, pleast try again.')\n          }\n        })\n      }\n    },\n    unassignUser: function(location) {\n      var vm = this;\n      UserService.removeUserLocation(this.user.userId, location.locationId).then(\n        function(response) {\n        }, function(error) {\n          console.log(error);\n      });\n    }\n  }\n}\n</script>\n\n<style scoped>\n.modal-container {\n  position: relative;\n  max-width: 760px;\n  margin: 40px auto 0;\n  padding: 20px 30px;\n  background-color: #fff;\n  border-radius: 2px;\n  box-shadow: 0 2px 8px rgba(0, 0, 0, .33);\n  transition: all .3s ease;\n  position: sticky;\n  top: 40px;\n}\n\n.modal-header-text {\n  font-size: 20px;\n  font-weight: bold;\n}\n\n.modal-header-subtext {\n  margin-top: 5px;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  width: 250px;\n}\n\n.modal-image {\n  position: absolute;\n  top: 10px;\n  right: 10px;\n}\n\n.modal-image div {\n  width: 40px;\n  height: 40px;\n  background-size: 40px 40px;\n  background-repeat: no-repeat;\n}\n\n.assign-controls-search {\n  float: right;\n  background: transparent;\n  margin: -40px 30px 20px 20px;\n  border: 2px solid #efefef;\n  border-radius: 1px;\n  width: 200px;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.assign-controls-search input{\n  border: none;\n  padding: 15px 15px 5px 15px;\n  font-size: 12px;\n}\n\n.assign-controls-search-icon {\n  vertical-align: middle;\n  margin-right: 15px;\n  color: #4fcbc5;\n  cursor: pointer;\n  font-size: 16px;\n}\n\n.modal-column-body {\n  border: 1px solid #aaa;\n  overflow-y: auto;\n  margin: 5px 0;\n  font-size: 14px;\n  padding: 5px;\n  text-align: left;\n}\n\n.user-assign-table {\n  background: white;\n  font-size: 14px;\n  padding: 10px;\n  clear: both;\n}\n\n.user-assign-header-row {\n  width: calc(100% - 20px);\n  font-weight: bold;\n  padding: 10px;\n  border-top: 1px solid #F2F4F8;\n  border-bottom: 1px solid #F2F4F8;\n  height: 14px;\n  display: flex;\n  flex-wrap: nowrap;\n  justify-content: space-evenly;\n  cursor: pointer;\n}\n\n.user-assign-header-row div, .user-assign-row div {\n  margin: auto;\n}\n\n.user-assign-row {\n  width: calc(100% - 20px);\n  clear: both;\n  padding: 10px;\n  border-bottom: 1px solid #F2F4F8;\n  height: auto;\n  display: flex;\n  flex-wrap: nowrap;\n  justify-content: space-evenly;\n}\n\n.user-assign-row:hover {\n  /* background: #F2F4F8; */\n}\n\n.user-assign-table-cell-0 {\n  flex: 1 1 400px;\n}\n\n.user-assign-table-cell-1 {\n  flex: 0 0 100px;\n}\n\n.user-assign-table-cell-2 {\n  flex: 0 0 420px;\n  text-align: center;\n  display: flex;\n\n}\n\n.font-shrink {\n  font-size: 12px;\n}\n\n.user-assign-table-cell-3 {\n  flex: 0 0 100px;\n  text-align: center;\n}\n\n.modal-footer {\n  clear: both;\n  margin-bottom: 20px;\n}\n\n.modal-footer-text {\n  font-size: 12px;\n}\n\n.modal-footer-button {\n  float: right;\n  border: 1px solid #4fcbc5;\n  border-radius: 5px;\n  padding: 5px 10px;\n  color: #4fcbc5;\n}\n\n.location-tag {\n  border: 2px solid #4fcbc4;\n  border-radius: 5px;\n  padding: 2px 5px;\n  margin-right: 5px;\n  cursor: pointer;\n  font-weight: 400;\n}\n\n.location-tag:hover {\n  background: #efefef;\n}\n\n.active {\n  background: #4fcbc5;\n  color: white;\n}\n\n.active:hover {\n  background: #4fcbc5aa;\n}\n\n\n</style>\n"]}]}