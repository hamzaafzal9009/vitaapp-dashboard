{"remainingRequest":"/Library/WebServer/Documents/Projects/OPINIION FINAL/opiniionv4/node_modules/vue-loader/lib/index.js??vue-loader-options!/Library/WebServer/Documents/Projects/OPINIION FINAL/opiniionv4/src/components/UserList.vue?vue&type=style&index=0&id=4a3fa6b9&scoped=true&lang=css&","dependencies":[{"path":"/Library/WebServer/Documents/Projects/OPINIION FINAL/opiniionv4/src/components/UserList.vue","mtime":1581270622000},{"path":"/Library/WebServer/Documents/Projects/OPINIION FINAL/opiniionv4/node_modules/css-loader/dist/cjs.js","mtime":499162500000},{"path":"/Library/WebServer/Documents/Projects/OPINIION FINAL/opiniionv4/node_modules/vue-loader/lib/loaders/stylePostLoader.js","mtime":499162500000},{"path":"/Library/WebServer/Documents/Projects/OPINIION FINAL/opiniionv4/node_modules/postcss-loader/src/index.js","mtime":499162500000},{"path":"/Library/WebServer/Documents/Projects/OPINIION FINAL/opiniionv4/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Library/WebServer/Documents/Projects/OPINIION FINAL/opiniionv4/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["UserList.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAyYA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA","file":"UserList.vue","sourceRoot":"src/components","sourcesContent":["<template>\n  <div>\n    <el-card class=\"card-min-height\" v-if=\"selectedView == 'list'\">\n      <div class=\"card-header\">\n        <div class=\"card-title\">\n          <h3>Users</h3>\n        </div>\n        <div class=\"card-actions\">\n          <el-button\n            type=\"primary\"\n            @click=\"editUser()\"\n          >\n          <i class=\"fa fa-user-plus fa-flip-horizontal\"></i>\n            &nbsp;Add User\n          </el-button>\n          <div class=\"input-group\">\n            <el-popover trigger=\"click\" class=\"filter clickable\" v-model=\"showFilters\">\n              <div class=\"filter-button\" :class=\"{'filter-active': filtersActive}\" slot=\"reference\">\n                <i class=\"fas fa-filter\"></i>\n              </div>\n              <div class=\"dropdown-filter-menu\">\n                <div class=\"dropdown-filter-option\" v-if=\"currentUserIsAdmin\">\n                  <label>Status</label>\n                  <el-select v-model=\"filters.status\" :collapse-tags=\"true\" multiple>\n                    <el-option class=\"capitalize\" v-for=\"(status, statusIndex) in userRoleStatus\" :key=\"statusIndex\" :value=\"status.value\" :label=\"status.label\">\n                    </el-option>\n                  </el-select>\n                </div>\n                <div class=\"dropdown-filter-option\">\n                  <label>Role</label>\n                  <!-- Doesn't detect the menu dropdown as a slot. -->\n                  <el-select v-model=\"filters.role\" :collapse-tags=\"true\" multiple>\n                    <el-option class=\"capitalize\" v-for=\"(role, roleIndex) in userRoleList\" :key=\"roleIndex\" :value=\"role.value\" :label=\"role.label\">\n                    </el-option>\n                  </el-select>\n                </div>\n                <div class=\"dropdown-filter-buttons\">\n                  <el-button type=\"text\" size=\"small\" style=\"color:#F48D46\" @click=\"resetFilters()\">Reset Filters</el-button>\n                  <el-button type=\"warning\" size=\"small\" round @click=\"searchUsers()\">Apply</el-button>\n                </div>\n              </div>              \n            </el-popover>\n            <el-input\n              v-model=\"searchText\"\n              @keyup.native.enter=\"searchUsers()\"\n              placeholder=\"Search Users\"\n            ><i slot=\"suffix\" class=\"far fa-search\"></i></el-input>\n          </div>\n        </div>\n      </div>\n      <div class=\"block\" v-if=\"users\">\n        <el-table size=\"small\" class=\"card-table-min-height\" row-class-name=\"clickable\" :data=\"users\" highlight-current-row \n        empty-text=\"No results found based on your search criteria.\"\n        ref=\"customerTable\" height=\"calc(80vh - var(--navbar-height) - var(--footer-height) - 40px)\" @row-click=\"selectUser($event)\" @sort-change=\"handleSort\">\n          <el-table-column label=\"\" v-if=\"currentUserIsAdmin\" width=\"60\" min-width=\"30\">\n            <template slot-scope=\"scope\">\n              <el-button circle icon=\"fas fa-user-secret\" size=\"small\" @click=\"spoofUser(scope.row)\"></el-button>\n            </template>\n          </el-table-column>\n          <el-table-column label=\"Name\" prop=\"firstName\" sortable=\"custom\" :sort-orders=\"sortOptions\" min-width=\"200\" show-overflow-tooltip>\n            <template slot-scope=\"scope\">\n              <span>{{scope.row.firstName}} {{scope.row.lastName}}</span>\n            </template>\n          </el-table-column>\n          <el-table-column v-if=\"currentUserIsAdmin\" label=\"Status\" prop=\"status\" sortable=\"custom\" :sort-orders=\"sortOptions\" min-width=\"200\" show-overflow-tooltip>\n            <template slot-scope=\"scope\">\n              <span v-if=\"scope.row.status == 'active'\" class=\"status-bubble subscribed\">{{scope.row.status}}</span>\n              <span v-if=\"scope.row.status == 'inactive'\" class=\"status-bubble unsubscribed\">{{scope.row.status}}</span>\n              <span v-if=\"scope.row.status == 'deleted'\" class=\"status-bubble deleted\">{{scope.row.status}}</span>\n            </template>\n          </el-table-column>\n          <el-table-column label=\"Email\" prop=\"email\" sortable=\"custom\" :sort-orders=\"sortOptions\"  min-width=\"300\">\n            <template slot-scope=\"scope\">\n              <span>{{scope.row.email.substr(0,25)}}<span v-if=\"scope.row.email.length > 26\">...</span></span>\n            </template>\n          </el-table-column>\n          <el-table-column prop=\"role\" label=\"Role\" min-width=\"100\">\n            <template slot-scope=\"scope\">\n              <span class=\"user-role\">{{scope.row.role}}</span>\n            </template>\n          </el-table-column>\n          <!-- <el-table-column prop=\"status\" label=\"Status\" min-width=\"100\">\n          </el-table-column> -->\n          <el-table-column prop=\"locations\" label=\"Locations\" min-width=\"250\" show-overflow-tooltip>\n            <template slot-scope=\"scope\">\n              <span v-for=\"(location, locationIndex) in scope.row.locations\" :key=\"locationIndex\">{{location}}<span v-if=\"locationIndex != scope.row.locations.length-1\">,&nbsp;</span></span>\n            </template>\n          </el-table-column>\n        </el-table>\n      </div>\n      <div class=\"pagination block\" v-if=\"users && users.length\">\n        <span>Showing {{users.length}} of {{paginationData.total}} Total Results</span>\n        <el-pagination\n          @current-change=\"searchUsers\"\n          background\n          layout=\"prev, pager, next\"\n          :current-page=\"paginationPage\"\n          :page-size=\"paginationData.perPage\"\n          :total=\"paginationData.total\"\n          small\n        ></el-pagination>\n        <el-select class=\"per-page-select\" size=\"mini\" v-model=\"paginationData.perPage\" @change=\"searchUsers()\">\n          <el-option v-for=\"option in paginationData.perPageOptions\"\n                      :value=\"option\"\n                      :key=\"option\"                       \n          >{{option}}\n          </el-option>\n        </el-select>\n      </div> \n      <div class=\"block\" v-else>\n\n      </div>\n      <addUser :selectedUser=\"selectedUser\" v-if=\"openUserModal\" v-on:closeModal=\"closeUserModal()\" v-on:switchModal=\"switchModal($event, user)\"/>\n      <!-- <userAssign :selectedUser=\"selectedUser\" v-on:assignUser=\"assignUser\" v-if=\"openUserAssignModal\" v-on:closeModal=\"closeUserModal()\"/> -->\n      <userResetPassword :selectedUser=\"selectedUser\" v-if=\"openUserResetModal\" v-on:closeModal=\"closeUserModal()\"/>\n    </el-card>\n    <userDetail :selectedUser=\"selectedUser\" v-if=\"selectedView == 'details'\" v-on:closeModal=\"closeUserModal()\"/>\n  </div>\n\n</template>\n\n<script>\nimport addUser from \"./AddUser\";\nimport userDetail from \"./UserDetail\";\nimport userAssign from \"./UserAssign\";\nimport userResetPassword from \"./UserResetPassword\";\nimport UserService from \"./../services/UserService\";\nimport { EventService } from \"../main\";\nimport imageFile from '../assets/imageFile.js';\n\nexport default {\n  name: \"userList\",\n  components: {\n    addUser,\n    userDetail,\n    userAssign,\n    userResetPassword\n  },\n  data: function() {\n    return {\n      userRoleStatus: [{value: 'active', label: 'Active'}, {value: 'inactive', label: 'Inactive'}],\n      userRoleList: [{value: 'owner', label: 'Owner'}, {value: 'manager', label: 'Manager'}, {value: 'agent', label: 'Agent'}, {value: 'none', label: 'None'}],\n      selectedView: 'list',\n      users: [],\n      filters: {\n        status: [],\n        role: []        \n      },\n      sortOptions: ['ascending', 'descending'],\n      showFilters: false,\n      searchText: \"\",\n      openUserModal: false,\n      openUserDetailModal: false,\n      openUserAssignModal: false,\n      openUserResetModal: false,\n      showListTooLongMessage: false,\n      selectedUser: null,\n      sortType: \"name\",\n      currentUserIsAdmin: false,\n      currentLocationId: null,\n      paginationData: {\n        perPage: 25,\n        currentPage: 0,\n        perPageOptions: [5,10,25],\n        total: 0,\n        changePage: 'setPageNum',\n        rowChange: 'searchCustomers'\n      } \n    };\n  },\n  computed: {\n    filtersActive() {\n      let vm = this;\n      if(vm.filters.status.length || vm.filters.role.length) {\n        return true;\n      } else {\n        return false;\n      }\n    },\n    paginationPage() {\n      return (this.paginationData.currentPage+1);\n    }\n  },\n  created() {\n    this.checkUser();\n    this.checkForState();\n    this.currentLocationId = localStorage.getItem(\"locationId\");\n    EventService.$on(\"locationChange\", this.locationChange);\n    EventService.$on(\"selectUser\", this.selectUser);\n  },\n  beforeDestroy: function() {\n    EventService.$off(\"locationChange\", this.locationChange);\n    EventService.$off(\"selectUser\", this.selectUser);\n  },\n  methods: {\n    checkUser() {\n      var vm = this;\n      vm.currentUserIsAdmin = JSON.parse(localStorage.getItem('currentUser')).isSuperAdmin;\n      vm.searchUsers();\n    },\n    checkForState() {\n      let stateCheck = this.$route.params.state;\n      \n      if (stateCheck == \"add\") {\n        this.openUserModal = true;\n      } else if(stateCheck) {\n        this.selectedUser = JSON.parse(localStorage.getItem('currentUser'));\n        this.selectedView = 'details';\n        \n      } else {\n        null;\n      }\n    },\n    locationChange() {\n      this.currentLocationId = localStorage.getItem(\"locationId\");\n      this.searchUsers();\n    },\n    handleSort(data) {\n      let vm = this;\n      let direction = '-';\n      if(vm.sortType[0] == '-') {\n        direction = ''\n      }\n      vm.sortType = direction + data.prop;      \n      vm.searchUsers();\n    },\n    searchUsers(pageNum) {\n      EventService.$emit(\"loading\", \"show\");\n      var searchObject = {};\n      if (this.searchText) searchObject.searchText = this.searchText.trim();\n      if (this.sortType) {\n        if (this.sortType == 'name') {\n          searchObject.sort = 'firstName';\n        } else if (this.sortType == '-name') {\n          searchObject.sort = '-firstName';\n        } else {\n          searchObject.sort = this.sortType;\n        }\n      }\n\n      var locationIdArray = [];\n      let locationId = localStorage.getItem(\"locationId\");\n      if (locationId) {\n        locationIdArray.push(locationId);\n      } else {\n        let locOptions = JSON.parse(localStorage.getItem(\"locationOptions\"));\n        locOptions.map(el => locationIdArray.push(el.locationId));\n      }\n      searchObject.locations = locationIdArray;\n\n      if (pageNum) {\n        searchObject.pageNum = pageNum;\n        this.paginationData.currentPage = pageNum - 1;\n      } else {\n        searchObject.pageNum = 0;\n        this.paginationData.currentPage = 0;\n      }\n\n      var vm = this;\n      searchObject.filters = vm.filters;\n      searchObject.resPerPage = vm.paginationData.perPage;\n      vm.showFilters = false;\n      UserService.searchUsersAdvanced(searchObject).then( response => {\n        if(response) {\n          EventService.$emit(\"loading\", \"hide\");\n          vm.users = response.result[0].results;\n          if(response.result[0].count && response.result[0].count[0] ) {\n            vm.paginationData.total = response.result[0].count[0].total;\n          } \n        } else {\n          EventService.$emit(\"errorMessage\", \"Something went wrong, please try again.\");\n          EventService.$emit(\"loading\", \"hide\");\n\n        }\n      });\n    },\n    resetFilters() {\n      this.filters = {\n        status: [],\n        role: []\n      }\n    },\n    toggleSort(setSortType) {\n      if (this.sortType != setSortType) {\n        this.sortType = setSortType;\n      } else {\n        this.sortType = \"-\" + this.sortType;\n      }\n      this.searchUsers(this.selectedPage);\n    },\n    setPageNum(pageNum) {\n      this.selectedPage = pageNum;\n      this.searchUsers(pageNum);\n    },\n    editUser(user) {\n      this.selectedUser = user;\n      this.openUserModal = true;\n    },\n    assignUser(user) {\n      this.selectedUser = user;\n      this.openUserAssignModal = true;\n    },\n    selectUser(user) {\n      this.selectedUser = user;\n      this.selectedView = 'details';\n    },\n    activateUser(user) {\n      var vm = this;\n      UserService.activateUser(user.userId).then(\n        function(response) {\n          vm.searchUsers(this.selectedPage);\n        },\n        function(error) {\n          console.log(error);\n        },\n        vm.searchUsers(vm.selectedPage)\n      );\n    },\n    deactivateUser(user) {\n      if (window.confirm(\"Are you sure you want to deactivate this user?\")) {\n        var vm = this;\n        UserService.deactivateUser(user.userId).then(\n          function(response) {\n            vm.searchUsers(this.selectedPage);\n          },\n          function(error) {\n            console.log(error);\n          },\n          vm.searchUsers(vm.selectedPage)\n        );\n      }\n    },\n    canActionUser(action, user) {\n      if (this.currentUserIsAdmin) {\n        if (action == \"activate\") {\n          return user.status != \"active\";\n        } else if (action == \"deactivate\") {\n          return user.status != \"inactive\";\n        } else {\n          return true;\n        }\n      } else {\n        if (action == \"activate\") return user.status != \"active\";\n        if (action == \"deactivate\") return user.status != \"inactive\";\n        if (action == \"edit\") return false;\n        if (action == \"spoof\") return false;\n        if (action == \"reset\") return true;\n        if (action == \"assign\") return true;\n      }\n      return false;\n    },\n    resetUserPassword(user) {\n      this.selectedUser = user;\n      this.openUserResetModal = true;\n    },\n    spoofUser(user) {\n      var vm = this;\n      UserService.spoofUser(user.userId).then(\n        function(response) {\n          EventService.$emit(\"spoofingUser\");\n        },\n        function(error) {\n          console.log(error);\n        }\n      );\n    },\n    switchModal(user) {\n      this.selectedUser = user;\n      this.openUserModal = false;\n      this.assignUser(this.selectedUser);\n    },\n    closeUserModal() {\n      this.selectedUser = null;\n      this.selectedView = 'list';\n      this.openUserModal = false;\n      EventService.$emit('loading', 'hide')\n      this.searchUsers(this.selectedPage);\n    },\n    appendImageHeader(imgText) {\n      if (!imgText)\n        imgText = imageFile.imgBlock();\n      if (imgText && imgText[0] == \",\") {\n        return \"data:image/png;base64\" + imgText;\n      } else {\n        return \"data:image/png;base64,\" + imgText;\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n\n.per-page-select {\n  max-width: 80px;\n}\n.user-role {\n  text-transform: capitalize;\n}\n.user-controls {\n  float: right;\n  color: #545454;\n  font-size: 20px;\n  width: 100%;\n  background: #ffffff;\n  border-radius: 1px;\n  box-shadow: 0px 2px 4px rgba(0,0,0,.25);\n  margin-bottom: 20px;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  align-content: center;\n}\n\n.user-controls-add {\n  float: left;\n  padding: 10px 10px 0 0;\n  cursor: pointer;\n  margin-left: 50px;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n}\n\n.user-controls-add i {\n  color: #4fcbc5;\n}\n\n.user-controls-add span {\n  font-size: 12px;\n  margin-top: 5px;\n  margin-bottom: 10px;\n  font-weight: bold;\n}\n\n.user-controls-search {\n  background: white;\n  margin: 20px 20px;\n  border: 2px solid #efefef;\n  border-radius: 1px;\n  width: 350px;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.user-controls-search input {\n  border: none;\n  padding: 15px 15px 5px 15px;\n  font-size: 12px;\n}\n\n.user-controls-search-icon {\n  vertical-align: middle;\n  margin-right: 15px;\n  color: #4fcbc5;\n  font-size: 16px;\n}\n\n.user-table {\n  background: white;\n  font-size: 14px;\n  padding: 10px 0px 20px 0px;\n  clear: both;\n  border-radius: 1px;\n  box-shadow: 0px 2px 4px rgba(0,0,0,.25);\n}\n\n.superadmin-row {\n  background: #4fcbc52b;\n}\n\n.user-header-row {\n  width: calc(100% - 20px);\n  font-weight: bold;\n  padding: 10px 10px 20px 10px;\n  border-bottom: 3px solid #efefef;\n  height: 14px;\n  display: flex;\n  flex-wrap: nowrap;\n  justify-content: space-evenly;\n  cursor: pointer;\n}\n\n.user-header-row div,\n.user-row div {\n  margin: auto;\n}\n\n.user-row {\n  width: calc(100% - 20px);\n  clear: both;\n  padding: 10px;\n  border-bottom: 1px solid #efefef;\n  height: auto;\n  display: flex;\n  flex-wrap: nowrap;\n  justify-content: space-evenly;\n  font-size: 12px;\n}\n\n.user-row:hover {\n  background: #dfdfdf;\n}\n\n.user-table-cell-0 {\n  flex: 0 0 60px;\n}\n\n.user-table-cell-1 {\n  flex: 0 0 80px;\n  text-transform: capitalize;\n}\n\n.user-table-cell-1 img {\n  width: 40px;\n  height: 40px;\n}\n\n.user-table-cell-2 {\n  flex: 1 1 200px;\n  cursor: pointer;\n}\n\n.user-table-cell-3 {\n  flex: 2 2 200px;\n}\n\n.user-table-cell-4 {\n  flex: 0 0 50px;\n  text-transform: capitalize;\n}\n\n.centered {\n  text-align: center;\n  /* margin-left: 20px; */\n}\n\n.user-table-cell-4 .fa-check {\n  color: green;\n}\n\n.user-table-cell-4  .fa-ban {\n  color: orange;\n}\n\n.user-table-cell-4  .fa-times-circle {\n  color: red;\n}\n\n.user-table-cell-5 {\n  flex: 0 0 200px;\n  text-align: center;\n  text-transform: capitalize;\n}\n\n.user-table-cell-6 {\n  flex: 0 0 220px;\n  text-align: center;\n  height: 40px;\n  overflow-y: auto;\n}\n\n.user-table-cell-5 span {\n  margin-right: 5px;\n  /* cursor: pointer; */\n}\n\n.color-change {\n  color: #2d2d2d;\n}\n\n.user-pagination {\n  border-top: 3px solid #efefef;\n  padding: 10px 10px 0 10px;\n}\n\n.user-pagination button {\n  border: 1px solid #efefef;\n  padding: 10px;\n  cursor: pointer;\n  background: white;\n  border-radius: 0;\n  min-width: 50px;\n}\n\n.user-pagination button.button-selected {\n  background: #dfdfdf;\n}\n\n.user-pagination button:hover {\n  background: #dfdfdf;\n}\n\n.no-pointer {\n  cursor: default;\n}\n\n.filter-button {\n  width: 60px;\n  height: 50px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n</style>\n"]}]}