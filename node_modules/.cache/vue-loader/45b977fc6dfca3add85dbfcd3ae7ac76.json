getReviewsData()\">Search</button> -->\n        <!-- <div id=\"export-wrapper\" @click=\"printReport()\">\n          <i class=\"fas fa-download\"></i>\n          <div>Export Report</div>\n        </div> -->\n      </div>\n    </div>\n\n    <!-- <div id=\"report-body\">\n      <div id=\"lo-card-wrapper\" class=\"box-shadow\">\n        <div class=\"card-body-wrapper\">\n          <div class=\"report-table\">\n            <div class=\"report-header\">\n              <div>Location</div>\n              <div>Start Date</div>\n              <div>Months on Opiniion</div>\n              <div>Starting Reviews</div>\n              <div>Current Reviews</div>\n              <div>Reviews Added</div>\n              <div>% Increase</div>\n              <div>Starting Rating</div>\n              <div>Current Rating</div>\n              <div>Rating Change</div>\n              <div>GMB Status</div>\n            </div>\n            <div class=\"report-body\">\n              <div class=\"report-row\" v-for=\"loc in reportData\">\n                <div>{{loc.name}}</div>\n                <div>{{loc.startDate}}</div>\n                <div>{{loc.months}}</div>\n                <div>{{loc.startReviews}}</div>\n                <div>{{loc.currentReviews}}</div>\n                <div>{{loc.addedReviews}}</div>\n                <div>{{loc.addedReviewsPercent}}</div>\n                <div>{{loc.startRating}}</div>\n                <div>{{loc.currentRating}}</div>\n                <div>{{loc.changeRating}}</div>\n                <div>{{loc.gmbStatus}}</div>\n              </div>\n            </div>\n            <div class=\"report-footer\">              \n            </div>\n          </div>\n          <iframe id=\"invisible\" style=\"display:none;\" src=\"downloadContent\"></iframe>\n        </div>\n      </div>\n    </div> -->\n\n    <div id=\"report-body\">\n      <el-row>\n        <el-col :span=\"24\">\n          <el-card>\n             <el-table             \n              class=\"location-overview-table\"\n              stripe\n              :data=\"reportData\">\n              <el-table-column\n                min-width=\"160\"\n                prop=\"name\"\n                label=\"LOCATION\">\n              </el-table-column>\n              <el-table-column\n                min-width=\"160\"\n                prop=\"startDate\"\n                label=\"START DATE\">\n              </el-table-column>\n              <el-table-column\n                min-width=\"160\"\n                prop=\"startReviews\"\n                label=\"STARTING REVIEW \">\n              </el-table-column>\n              <el-table-column\n                min-width=\"160\"\n                prop=\"currentReviews\"\n                label=\"Current REVIEW \">\n              </el-table-column>\n              <el-table-column\n                min-width=\"160\"\n                prop=\"addedReviews\"\n                label=\"REVIEWS ADDED\">\n              </el-table-column>\n              <el-table-column\n                min-width=\"160\"\n                prop=\"addedReviewsPercent\"\n                label=\"% INCREASED\">\n              </el-table-column>\n              <el-table-column\n                min-width=\"160\"\n                prop=\"startRating\"\n                label=\"STARTING RATING\">\n              </el-table-column>\n               <el-table-column\n                min-width=\"160\"\n                prop=\"currentRating\"\n                label=\"CURRENT RATING\">\n              </el-table-column>\n              <el-table-column\n                min-width=\"160\"\n                prop=\"changeRating\"\n                label=\"RATING CHANGE\">\n              </el-table-column>\n            </el-table>\n          </el-card>\n        </el-col>\n      </el-row>\n    </div>\n  </div>\n</template>\n\n<script>\nimport ReportService from './../../services/ReportService'\nimport UserService from './../../services/UserService'\nimport {EventService} from '../../main'\n\nexport default {\n  name: 'multiReport',\n  data: function() {\n    return {\n      reportData: [],\n      downloadContent: ''\n    }\n  },\n  created() {\n    UserService.logPageVisit('multiReport');\n    this.getMultiReportData();\n  },\n  methods: {\n    getMultiReportData() {\n      EventService.$emit(\"loading\", \"show\");\n      var vm = this;\n      ReportService.getMultiReportData().then(function(response) {\n        for (let i=0; i<response.length; i++) {\n          var ratingChange = 0;\n          if (response[i].currentRating) {\n            var diff = parseFloat(response[i].currentRating) - parseFloat(response[i].startRating);\n            ratingChange = Math.round(10*(diff))/10;\n          }\n          var newObj = {\n            locationId: response[i].locationId,\n            name: response[i].name,\n            startDate: new Date(response[i].startDate).toLocaleDateString(),\n            months: Math.floor((Date.now() - response[i].startDate) / (1000*60*60*24*30)),\n            startReviews: response[i].startReviews ? response[i].startReviews : \"-\",\n            currentReviews: response[i].currentReviews ? response[i].currentReviews : response[i].startReviews ? response[i].startReviews : \"-\",\n            addedReviews: !response[i].currentReviews ? 0 : (response[i].currentReviews - response[i].startReviews > 0) ? (response[i].currentReviews - response[i].startReviews) : \"-\",\n            startRating: response[i].startRating,\n            currentRating: response[i].currentRating ? Math.round(10*parseFloat(response[i].currentRating))/10 : response[i].startRating,\n            changeRating: ratingChange,\n            //negInt: '-',\n            gmbStatus: response[i].gmbStatus\n          };\n          if(response[i].startReviews > 0) {\n            newObj.addedReviewsPercent = response[i].currentReviews && response[i].currentReviews - response[i].startReviews > 0 ? (Math.floor(100*(response[i].currentReviews - response[i].startReviews)/response[i].startReviews)) + '%' : '-';\n          } else {\n            newObj.addedReviewsPercent = response[i].currentReviews && response[i].currentReviews - response[i].startReviews > 0 ? (Math.floor(100*(response[i].currentReviews - .9)/.9)) + '%' : '-';\n          }\n          vm.reportData.push(newObj);\n        }\n        /*ReportService.getMultiReportIntercepts().then(function(response2) {\n          for (let j=0; j<vm.reportData.length; j++) {\n            for (let i=0; i<response2.length; i++) {\n              if (vm.reportData[j].locationId == response2[i]._id) {\n                vm.reportData[j].negInt = response2[i].count;\n              }\n            }\n          }\n          var tempThing = vm.reportData;\n          vm.reportData = null;\n          vm.reportData = tempThing;\n        }, function(error2) {\n          console.log(error2);\n          EventService.$emit(\"loading\", \"hide\");\n        });*/\n        EventService.$emit(\"loading\", \"hide\");\n      }, function(error) {\n        console.log(error);\n        EventService.$emit(\"loading\", \"hide\");\n      });\n    },\n    downloadReport() {\n      var csvDownload = 'Location,' +\n        'Start Date,' +\n        'Months on Opiniion,' +\n        'Starting Reviews,' +\n        'Current Reviews,' +\n        'Reviews Added,' +\n        '% Increase,' +\n        'Starting Rating,' +\n        'Current Rating,' +\n        'Change in Rating,' +\n        //'Negative Reviews Intercepted,' +\n        'GMB Status\\n\\n';\n\n      for (let i=0; i<this.reportData.length; i++) {\n        csvDownload += this.reportData[i].name + ',' +\n          this.reportData[i].startDate + ',' +\n          this.reportData[i].months + ',' +\n          this.reportData[i].startReviews + ',' +\n          this.reportData[i].currentReviews + ',' +\n          this.reportData[i].addedReviews + ',' +\n          this.reportData[i].addedReviewsPercent + ',' +\n          this.reportData[i].startRating + ',' +\n          this.reportData[i].currentRating + ',' +\n          this.reportData[i].changeRating + ',' +\n          //this.reportData[i].negInt + ',' +\n          this.reportData[i].gmbStatus + '\\n';\n      }\n\n      var MIME_TYPE = \"text/csv\";\n      var blob = new Blob([csvDownload], {type: MIME_TYPE});\n      var csvURL = window.URL.createObjectURL(blob);\n      var tempLink = document.createElement('a');\n      document.body.append(tempLink);\n      tempLink.href = csvURL;\n      tempLink.setAttribute('download', 'report.csv');\n      tempLink.click();\n      document.body.removeChild(tempLink);\n    }\n  }\n}\n</script>\n\n<style scoped>\n  #report-header {\n    background: transparent;\n    display: flex;\n    justify-content: space-between;\n    margin-left: 2px;\n    height: 66px;\n    box-shadow: transparent;\n    color: #545454;\n  }#report-content-wrapper {\n    width: 100%;\n  }\n  .location-overview-table {\n    overflow-x: auto;\n    /* min-width: 1200px; */\n  }\n\n  #report-name-wrapper {\n    font-size: 24px;\n    padding: 20px 0 20px 20px;\n    font-weight: 500;\n  }\n\n  #report-options-wrapper {\n    font-size: 20px;\n    padding: 20px 20px 10px 0;\n    display: flex;\n    align-items: center;\n  }\n  #lo-card-wrapper,\n  #cvp-private-feedback-card-wrapper {\n    background: white;\n    margin: 20px;\n  }\n\n  .report-title {\n    width: calc(100%-20px);\n    background: white;\n    border-radius: 1px;\n    box-shadow: 0px 2px 4px rgba(0,0,0,.25);\n    padding: 5px 0;\n    margin-bottom: 10px;\n    padding: 20px;\n    font-weight: bold;\n    font-size: 24px;\n  }\n\n  .report-table {\n    background: white;\n    font-size: 14px;\n    padding: 10px 0;\n    box-shadow: 0px 2px 4px rgba(0, 0, 0, 0.25);\n    border-radius: 1px;\n    clear: both;\n  }\n\n  .report-header {\n    width: calc(100% - 20px);\n    font-weight: bold;\n    padding: 10px 10px 20px 10px;\n    border-bottom: 3px solid #efefef;\n    display: flex;\n    flex-wrap: nowrap;\n    justify-content: space-evenly;\n    font-family: FuturaPTDemi;\n    font-size: 16px;\n    margin-bottom: 0;\n  }\n\n  .report-header div,\n  .report-row div {\n    margin: auto;\n  }\n\n  .report-row {\n    width: calc(100% - 20px);\n    clear: both;\n    padding: 10px;\n    border-bottom: 1px solid #efefef;\n    height: auto;\n    display: flex;\n    flex-wrap: nowrap;\n    justify-content: space-evenly;\n    font-size: 12px;\n  }\n\n  .report-row:hover {\n    background: #dadada;\n  }\n\n  .report-header div:nth-child(1),\n  .report-row div:nth-child(1) {\n    flex: 0 0 150px;\n  }\n  .report-header div:nth-child(2),\n  .report-row div:nth-child(2) {\n    flex: 0 0 70px;\n  }\n  .report-header div:nth-child(3),\n  .report-row div:nth-child(3) {\n    flex: 0 0 70px;\n  }\n  .report-header div:nth-child(4),\n  .report-row div:nth-child(4) {\n    flex: 0 0 70px;\n  }\n  .report-header div:nth-child(5),\n  .report-row div:nth-child(5) {\n    flex: 0 0 70px;\n  }\n  .report-header div:nth-child(6),\n  .report-row div:nth-child(6) {\n    flex: 0 0 70px;\n  }\n  .report-header div:nth-child(7),\n  .report-row div:nth-child(7) {\n    flex: 0 0 70px;\n  }\n  .report-header div:nth-child(8),\n  .report-row div:nth-child(8) {\n    flex: 0 0 70px;\n  }\n  .report-header div:nth-child(9),\n  .report-row div:nth-child(9) {\n    flex: 0 0 70px;\n  }\n  .report-header div:nth-child(10),\n  .report-row div:nth-child(10) {\n    flex: 0 0 70px;\n  }\n  .report-header div:nth-child(11),\n  .report-row div:nth-child(11) {\n    flex: 0 0 50px;\n  }\n  .report-header div:nth-child(12),\n  .report-row div:nth-child(12) {\n    flex: 0 0 50px;\n  }\n\n  .report-footer {\n    display: flex;\n    flex-direction: row-reverse;\n    justify-content: space-between;\n  }\n\n  .download-container {\n    margin-top: 10px;\n    margin-right: 10px;\n    display: flex;\n    justify-content: center;\n  }\n\n  .download-container button {\n    background: #4fcbc5;\n    color: white;\n    padding: 10px 20px;\n    border-radius: 1px;\n    cursor: pointer;\n  }\n\n  .export-btn{\n    margin-top: -10px;\n    margin-left: 20px;\n    background-color: #66615B;\n    border: none\n  }\n  .export-btn:hover {\n\n    background-color: #66615B\n  }\n  .el-icon-download{\n    font-size: 20px;\n    font-weight: bold\n  }\n  .datepicks{\n    cursor: pointer;\n    padding-left: 35px;\n    background-color: #FAFAFA;\n    border: 1px solid #d8d7d7;\n    height: 18px\n  }\n#input_container {\n    display: flex;\n    justify-content: space-between;\n    position:relative;\n    width: 200px;\n    margin-left: 20px\n}\n#input_img {\n    position:absolute;\n    bottom:20px;\n    padding-left:5px; \n    width:24px;\n    height:24px;\n}\n#report-body{\n  /* width: 95%; */\n  /* max-width: 90%; */\n  /* width: 1400px; */\n  /* margin: auto; */\n  margin-left: 30px;\n  margin-top: 40px;\n}\n\n</style>\n"]}]}