{"remainingRequest":"/Users/hamzaafzal/OLD DATA/Mac Home/vueJS/OPINIION FINAL/opiniionv4/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/hamzaafzal/OLD DATA/Mac Home/vueJS/OPINIION FINAL/opiniionv4/src/components/AddLocation.vue?vue&type=script&lang=js&","dependencies":[{"path":"/Users/hamzaafzal/OLD DATA/Mac Home/vueJS/OPINIION FINAL/opiniionv4/src/components/AddLocation.vue","mtime":1581270622000},{"path":"/Users/hamzaafzal/OLD DATA/Mac Home/vueJS/OPINIION FINAL/opiniionv4/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/hamzaafzal/OLD DATA/Mac Home/vueJS/OPINIION FINAL/opiniionv4/node_modules/babel-loader/lib/index.js","mtime":499162500000},{"path":"/Users/hamzaafzal/OLD DATA/Mac Home/vueJS/OPINIION FINAL/opiniionv4/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/hamzaafzal/OLD DATA/Mac Home/vueJS/OPINIION FINAL/opiniionv4/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["AddLocation.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA4RA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"AddLocation.vue","sourceRoot":"src/components","sourcesContent":["<template>\n  <el-dialog\n    class=\"add-location-dialog\"\n    width=\"900px\"\n    :visible.sync=\"locationModalOpen\"\n    :show-close=\"false\"\n    :before-close=\"closeModal\"\n  >\n    <!-- Step Header -->\n    <el-steps :active=\"steps\" finish-status=\"success\" align-center>\n      <el-step title=\"Location Details\"></el-step>\n      <el-step title=\"Location Settings\"></el-step>\n      <el-step title=\"Rotators\"></el-step>\n    </el-steps>\n    <!-- Header -->\n    <div class=\"add-location-header\" v-if=\"steps == 0\">Location Details</div>\n    <div class=\"add-location-header\" v-else-if=\"steps == 1\">Location Settings</div>\n    <div class=\"add-location-header\" v-else-if=\"steps == 2\">Rotators</div>\n\n    <div class=\"dialog-body\">\n      <!-- Step 1 (Location Details) -->\n      <div v-if=\"steps == 0\" class=\"step-one-container\">\n        <el-form :model=\"location\" ref=\"stepOneForm\" :rules=\"stepOneRules\">\n          <el-row :gutter=\"20\">\n            <el-col :span=\"12\">\n              <label class=\"input-label\">\n                Location Name\n                <el-form-item prop=\"name\">\n                  <el-input class=\"input\" placeholder=\"Enter location name...\" v-model=\"location.name\"></el-input>\n                </el-form-item>\n              </label>\n            </el-col>\n            <el-col :span=\"12\">\n              <label class=\"input-label\">\n                Display Name\n                <el-form-item prop=\"displayName\">\n                  <el-input\n                    class=\"input\"\n                    placeholder=\"Enter display name...\"\n                    v-model=\"location.displayName\"\n                  ></el-input>\n                </el-form-item>\n              </label>\n            </el-col>\n          </el-row>\n          <el-divider></el-divider>\n          <el-row :gutter=\"20\">\n            <el-col :span=\"12\">\n              <label class=\"input-label\">\n                Street Address\n                <el-form-item prop=\"address.street1\">\n                  <el-input\n                    class=\"input\"\n                    placeholder=\"Street\"\n                    v-model=\"location.address.street1\"\n                  ></el-input>\n                </el-form-item>\n              </label>\n            </el-col>\n            <el-col :span=\"6\">\n              <label class=\"input-label\">\n                City\n                <el-form-item prop=\"address.city\">\n                  <el-input class=\"input\" placeholder=\"City\" v-model=\"location.address.city\"></el-input>\n                </el-form-item>\n              </label>\n            </el-col>\n            <el-col :span=\"6\">\n              <label class=\"input-label\">\n                State\n                <el-form-item prop=\"address.state\">\n                  <el-input class=\"input\" placeholder=\"State\" v-model=\"location.address.state\"></el-input>\n                </el-form-item>\n              </label>\n            </el-col>\n          </el-row>\n          <el-row :gutter=\"20\">\n            <el-col :span=\"12\">\n              <label class=\"input-label\" type=\"tel\">\n                Phone Number\n                <el-form-item prop=\"phone\">\n                  <el-input class=\"input\" placeholder=\"Phone Number\" v-model=\"location.phone\"></el-input>\n                </el-form-item>\n              </label>\n            </el-col>\n            <el-col :span=\"12\">\n              <label class=\"input-label\">\n                Zip Code\n                <el-form-item prop=\"address.zip\">\n                  <el-input class=\"input\" placeholder=\"Zip Code\" v-model=\"location.address.zip\"></el-input>\n                </el-form-item>\n              </label>\n            </el-col>\n          </el-row>\n          <el-divider></el-divider>\n          <el-row :gutter=\"20\">\n            <el-col :span=\"12\">\n              <label class=\"input-label\">\n                Beginning Google Rating\n                <el-form-item prop=\"beginningRating\">\n                  <el-input type=\"number\" class=\"input\" placeholder=\"Beginning Rating\" v-model=\"location.beginningRating\" :min=\"0.0\" :max=\"5.0\"></el-input>\n                </el-form-item>\n              </label>\n            </el-col>\n            <el-col :span=\"12\">\n              <label class=\"input-label\">\n                Beginning Review Total\n                <el-form-item prop=\"beginningTotals\">\n                  <el-input type=\"number\" class=\"input\" placeholder=\"Beginning Review Total\" v-model=\"location.beginningTotals\" :min=\"0\"></el-input>\n                </el-form-item>\n              </label>\n            </el-col>\n          </el-row>\n          <el-divider></el-divider>\n          <el-row :gutter=\"20\">\n            <el-col :span=\"12\">\n              <label class=\"input-label\">\n                Zoho ID\n                <el-input class=\"input\" placeholder=\"Zoho ID\" v-model=\"location.zohoId\"></el-input>\n              </label>\n            </el-col>\n            <el-col :span=\"12\">\n              <label class=\"input-label\">\n                Location Status\n                <el-select class=\"select\" v-model=\"location.status\" placeholder=\"Select\">\n                  <el-option value=\"active\" label=\"Active\"></el-option>\n                  <el-option value=\"inactive\" label=\"Inactive\"></el-option>\n                </el-select>\n              </label>\n            </el-col>\n          </el-row>\n          <el-row>\n            <el-checkbox border size=\"medium\" style=\"margin-top:10px;\" v-model=\"location.qsEnabled\" @click=\"location.qsEnabled=!location.qsEnabled\" label=\"Create Property Q's on Location Creation?\"></el-checkbox>\n          </el-row>\n        </el-form>\n      </div>\n\n      <!-- Step 2 (Location Settings) -->\n      <div v-if=\"steps == 1\">\n        <el-form :model=\"location\" ref=\"stepTwoForm\" :rules=\"stepTwoRules\">\n          <!-- <el-form-item> -->\n            <el-upload\n              list-type=\"picture\"\n              :limit=1\n              :file-list=\"fileList\"\n              :on-success=\"handleSuccess\"\n              :action=\"imageLocation\"\n              accept=\"image/*\"\n            >\n              <el-button size=\"small\" round type=\"primary\">Upload Logo</el-button>\n              <div v-if=\"!location.locationLogo\" slot=\"tip\" class=\"el-upload__tip\">No file chosen</div>\n            </el-upload>\n          <!-- </el-form-item> -->\n\n          <!-- <div v-show=\"location.locationLogo\" style=\"width:300px; height:300px;\">\n            <div :style=\"{'background-image': location.locationLogo}\"></div>\n          </div>-->\n          <!-- <div class=\"modal-image-container\" v-show=\"location.locationLogo\">\n            <div class=\"modal-image-div\" ref=\"locationImageDiv\" @click=\"encodeImageFileAsURL\" :style=\"{'background-image': location.locationLogo}\" ></div>\n          </div>-->\n          <el-row :gutter=\"20\">\n            <el-col :span=\"12\">\n              <label class=\"input-label\">\n                Button Color\n                <el-form-item prop=\"locationStyle.buttonColor\">\n                  <el-input\n                    class=\"input\"\n                    placeholder=\"Enter hex code...\"\n                    v-model=\"location.locationStyle.buttonColor\"\n                    required\n                  ></el-input>\n                </el-form-item>\n              </label>\n            </el-col>\n            <el-col :span=\"12\">\n              <div class=\"color-circle\" :style=\"{'background': location.locationStyle.buttonColor}\"></div>\n            </el-col>\n            <!-- <el-col :span=\"8\">\n              <el-color-picker v-model=\"location.locationStyle.buttonColor\"></el-color-picker>\n            </el-col>-->\n          </el-row>\n          <el-row :gutter=\"20\">\n            <el-col :span=\"12\">\n              <label class=\"input-label\">\n                Footer Color\n                <el-form-item prop=\"locationStyle.footerColor\">\n                  <el-input\n                    class=\"input\"\n                    placeholder=\"Enter hex code...\"\n                    v-model=\"location.locationStyle.footerColor\"\n                    required\n                  ></el-input>\n                </el-form-item>\n              </label>\n            </el-col>\n            <el-col :span=\"12\">\n              <div class=\"color-circle\" :style=\"{'background': location.locationStyle.footerColor}\"></div>\n              <!-- <el-color-picker v-model=\"location.locationStyle.footerColor\"></el-color-picker> -->\n            </el-col>\n          </el-row>\n          <el-divider></el-divider>\n          <el-row :gutter=\"20\">\n            <el-col :span=\"12\">\n              <label class=\"input-label\">\n                Email From Name\n                <el-form-item prop=\"emailFromName\">\n                  <el-input class=\"input\" placeholder=\"Email From Name...\" v-model=\"location.emailFromName\" required></el-input>\n                </el-form-item>\n              </label>\n            </el-col>\n            <el-col :span=\"12\">\n              <label class=\"input-label\">\n                Reply Email Address\n                <el-form-item prop=\"emailFromAddress\">\n                  <el-input class=\"input\" placeholder=\"Reply Email Address...\" v-model=\"location.emailFromAddress\"></el-input>\n                </el-form-item>\n              </label>\n            </el-col>\n          </el-row>            \n        </el-form>\n      </div>\n\n      <!-- Step 3 (Rotators) -->\n      <div v-if=\"steps == 2\">\n        <el-form :model=\"location\" ref=\"stepThreeForm\" :rules=\"stepThreeRules\">\n          <div class=\"list-item-container\">\n            <el-row class=\"row-headers\" :gutter=\"20\">\n              <el-col :span=\"11\">\n                <label>Source</label>\n              </el-col>\n              <el-col :span=\"11\">\n                <label>Url</label>\n              </el-col>\n            </el-row>\n            <el-row :gutter=\"20\" v-for=\"(rotator, index) in location.rotators\" :key=\"index\">\n              <div class=\"list-item\">\n                <el-col :span=\"11\">\n                  <el-form-item prop=\"rotator.name\">\n                    <el-select size=\"small\" v-model=\"rotator.name\" placeholder=\"Source (Ex: Google)\" required>\n                      <el-option v-for=\"option in rotatorOptions\" :key=\"option\" :value=\"option\" :label=\"option\"></el-option>\n                    </el-select>\n                  </el-form-item>\n                </el-col>\n                <el-col :span=\"11\">\n                  <el-form-item prop=\"rotator.url\">\n                    <el-input size=\"small\" v-model=\"rotator.url\" placeholder=\"Enter source URL\" required></el-input>\n                  </el-form-item>\n                </el-col>\n                <el-col :span=\"1\">\n                  <div class=\"delete-rotator fal fa-times quick-transition\" @click=\"removeRotator(index)\"></div>\n                </el-col>\n              </div>\n            </el-row>\n            <el-row :gutter=\"20\">\n              <div class=\"add-item\" @click=\"addRotator()\">\n                <el-col :span=\"20\">Add New</el-col>\n              </div>\n            </el-row>\n          </div>\n        </el-form>\n      </div>\n    </div>\n    <!-- <button class=\"modal-default-button\" v-if=\"step == 3\" @click=\"saveLocation()\"           \n          :disabled=\"!location.name || !location.displayName || !location.emailFromName ||  !location.emailFromAddress || !location.address.street1 || !location.address.city || !location.address.state || !location.address.zip || !location.phone || !(parseInt(location.beginningRating) >= 0) || !(parseInt(location.beginningTotals) >= 0) || !location.locationStyle.logo || !location.rotators[location.rotators.length -1].url || !location.rotators[location.rotators.length -1].name || !is100Percent \"\n          >\n            Save\n    </button>-->\n    <span slot=\"footer\" class=\"dialog-footer\">\n      <div>\n        <el-button type=\"text\" v-if=\"steps == 0\" @click=\"closeModal()\">Cancel</el-button>\n        <el-button type=\"text\" v-if=\"steps > 0\" @click=\"backStep()\">Back</el-button>\n      </div>\n      <el-button\n        v-if=\"steps >= 2\"\n        type=\"primary\"\n        @click=\"saveLocation()\"\n        :disabled=\"isDisabledConditions\"\n        >Confirm</el-button>\n      <el-button v-if=\"steps != 2\" type=\"primary\" @click=\"nextStep()\">Next</el-button>\n    </span>\n  </el-dialog>\n</template>\n\n<script>\nimport LocationService from \"./../services/LocationService\";\nimport SharedFunctions from \"./../services/SharedFunctions\";\nimport config from '../config';\nimport debounce from \"lodash.debounce\";\nimport { EventService } from \"../main\";\nexport default {\n  name: \"addLocation\",\n  props: [\"selectedLocation\", \"copiedLocation\"],\n  data: function() {\n    var checkBeginningRating = (rule, value, callback) => {\n      if(value < 0 || value > 5) {\n        return callback(new Error('Please enter value between 0 and 5'))\n      } else {\n        callback();\n      }\n    };\n    var checkBeginningTotal = (rule, value, callback) => {\n      if(value < 0) {\n        return callback(new Error('Please enter value greater than or equal to 0'))\n      } else {\n        callback();\n      }\n    };\n    return {\n      steps: 0,\n      serviceArray: [],\n      pageAction: \"Create New\",\n      locationModalOpen: true,\n      imageLocation: '',\n      rotatorOptions: ['Google', 'Facebook', 'Yelp', 'YellowPages', 'Apartments.com', 'ApartmentRatings.com', 'BBB', 'Other'],\n      location: {\n        name: \"\",\n        displayName: \"\",\n        address: {\n          street1: \"\",\n          city: \"\",\n          state: \"\",\n          zip: \"\"\n        },\n        zohoId: \"\",\n        phone: null,\n        status: \"active\",\n        incomingEmail: \"\",\n        emailFromName: \"\",\n        emailFromAddress: \"\",\n        locationStyle: {\n          logo: \"\",\n          buttonColor: \"#4fcbc5\",\n          footerColor: \"#545454\"\n        },\n        beginningRating: null,\n        beginningTotals: null,\n        rotators: [\n          {\n            name: \"\",\n            url: \"\",\n            rule: 100,\n            showReviewButton: true\n          }\n        ],\n        locationType: \"Generic\",\n        webhook: \"\",\n        rotatorsDisabled: false,\n        qsEnabled: false,\n        reviewsList: {\n          google: {\n            pullData: false,\n            url: \"\"\n          },\n          facebook: {\n            pullData: false,\n            url: \"\"\n          },\n          yelp: {\n            pullData: false,\n            url: \"\"\n          },\n          yellowpages: {\n            pullData: false,\n            url: \"\"\n          }\n        }\n      },\n      options: [\"Generic\", \"Retail\", \"Residential\", \"Pest Control\"],\n      locationLogo: \"\",\n      fileList: [],\n      is100Percent: true,\n      reviewURLmissing: false,\n      saving: false,\n      stepOneRules: {\n        name: [\n          { required: true, message: 'Please input a location name', trigger: 'blur' },\n        ],\n        displayName: [\n          { required: true, message: 'Please input a display name', trigger: 'blur' },\n        ],\n        phone: [\n          { required: true, message: 'Please input valid phone number (numbers only)', trigger: 'blur', min: 7, max: 10 },                    \n        ],\n        beginningRating: [\n          { required: true, message: 'Please input valid starting rating', trigger: 'blur' },\n        ],\n        beginningTotals: [\n          { required: true, message: 'Please input the beginning rating total', trigger: 'blur', },\n        ],\n        'address.street1': [\n          { required: true, message: 'Please input a street', trigger: 'blur'}          \n        ],\n        'address.city': [\n          { required: true, message: 'Please input a city', trigger: 'blur'}          \n        ],\n        'address.state': [\n          { required: true, message: 'Please input a state', trigger: 'blur'}          \n        ],\n        'address.zip': [\n          { required: true, message: 'Please input a zip code', trigger: 'blur', max: 5, min: 5}          \n        ],\n      },\n      stepTwoRules: {\n        // fileList: [\n        //   { required: false, message: 'Please upload an image', trigger: 'blur' },   \n        // ],\n        'locationStyle.buttonColor': [\n          { required: true, message: 'Please select a color', trigger: 'blur' },   \n        ],\n        'locationStyle.footerColor': [\n          { required: true, message: 'Please select a color', trigger: 'blur' },   \n        ],\n        emailFromName: [\n          { required: true, message: 'Please select the name the email will display from', trigger: 'blur' },   \n        ],\n        emailFromAddress: [\n          { required: true, message: 'Please select the email address the message will display from', trigger: 'blur' },   \n        ]\n      },\n      stepThreeRules: {\n        'rotator.name': [\n          { required: false, message: 'Please input a name', trigger: 'blur' },   \n        ],\n        'rotator.url': [\n          { required: false, message: 'Please input a url', trigger: 'blur' },   \n        ]\n      },\n    };\n  },\n  computed: {\n    isDisabledConditions: function() {\n      let location = this.location;\n      if (\n        !location.name ||\n        !location.displayName ||\n        !location.emailFromName ||\n        !location.address.street1 ||\n        !location.address.city ||\n        !location.address.state ||\n        !location.address.zip ||\n        !location.phone ||\n        !(parseInt(location.beginningRating) >= 0) ||\n        !(parseInt(location.beginningTotals) >= 0) ||\n        // !location.locationStyle.logo ||\n        !location.rotators[location.rotators.length - 1].url ||\n        !location.rotators[location.rotators.length - 1].name\n      ) {\n        return true;\n      } else {\n        return false;\n      }\n    },\n    rotatorData() {\n      if (this.location.rotators.length >= 1) {\n        return this.location.rotators.length;\n      }\n    }\n  },\n  methods: {\n    saveLocation() {\n      EventService.$emit(\"loading\", \"show\");     \n      let vm = this;\n      if(vm.saving == false) {\n        vm.saving = true;\n        LocationService.saveLocation(this.location).then(response => {\n            if (response) {                            \n              vm.closeModal();\n              EventService.$emit(\"loading\", \"hide\");\n              EventService.$emit(\"successMessage\", \"Hooray! Location Added.\");\n            } else {\n              EventService.$emit(\"loading\", \"hide\");\n              EventService.$emit( \"errorMessage\", \"Sorry, something went wrong. Please try again.\" );\n            }\n          }\n        );\n      } else {\n        EventService.$emit( \"errorMessage\", \"Sorry, something went wrong. Please try again.\" );     \n      }\n    },\n    addRotator() {\n      if (this.location.rotators.length < 4) {\n        this.location.rotators.push({ name: \"\", url: \"\" });\n      } else {\n        this.$message.error(\"Rotators are currently limited.\");\n      }\n      this.checkPercent();\n    },\n    removeRotator(index) {\n      this.location.rotators.splice(index, 1);\n    },\n    backStep() {\n      if (this.steps > 0) this.steps--;\n    },\n    nextStep() {\n      let vm = this;\n      if(vm.steps == 0) {\n        vm.$refs['stepOneForm'].validate((valid) => {\n          if(valid) {\n            vm.steps++;\n          } else {\n            return false;\n          }\n        })\n      } else if(vm.steps == 1) {\n        vm.$refs['stepTwoForm'].validate((valid) => {\n          if(valid) {\n            vm.steps++;\n          } else {\n            return false;\n          }\n        })\n      } else if(vm.steps == 2) {\n        vm.$refs['stepThreeForm'].validate((valid) => {\n          if(valid) {          \n            vm.$emit(\"closeEditor\");\n          } else {\n            return false;\n          }\n        })\n      }        \n    },\n    checkPercent() {\n      let tlr = this.location.rotators;\n      let numArr = [];\n      if (tlr.length > 1) {\n        tlr.map((e, i) => {\n          numArr.push(+e.rule);\n        });\n        let total = numArr.reduce((a, b) => a + b, 0);\n        if (total === 100) {\n          this.is100Percent = true;\n        } else {\n          this.is100Percent = false;\n        }\n      }\n    },\n    closeModal () {\n      this.$emit('closeEditor');\n      this.locationModalOpen = false;\n    },\n    encodeImageFileAsURL(event) {\n      let filesList = event.target.files[0];\n      let vm = this;\n      let reader = new FileReader();\n      reader.onloadend = function() {\n        if (reader.result.length > 500000 * 1.36) {\n          vm.showLogoSizeError = true;\n        } else {\n          vm.location.locationStyle.logo = reader.result.substring(22);\n          vm.changeImage(reader.result);\n        }\n      };\n      reader.readAsDataURL(filesList);\n    },\n    handleSuccess(response, file) {\n      let vm = this;\n      let reader = new FileReader();\n      reader.onloadend = function() {\n        if (reader.result.length > 500000 * 1.36) {\n          vm.showLogoSizeError = true;\n        } else {\n          vm.location.locationStyle.logo = reader.result.substring(22);\n        }\n      };\n      reader.readAsDataURL(file.raw);\n    }\n  },\n  created() {\n    this.imageLocation = config.baseUrl + 'location/acceptImage'\n    this.validatePhone = SharedFunctions.validatePhone;\n    if (this.selectedLocation) {\n      this.pageAction = \"Update\";\n      this.location = this.selectedLocation;\n    }\n    if (this.copiedLocation) {\n      this.pageAction = \"Copy\";\n      this.location = this.copiedLocation;\n      this.location.locationId = null;\n    }\n    // if (this.location.locationStyle.logo) {\n    //   this.changeImage(\n    //     \"data:image/png;base64,\" + this.location.locationStyle.logo\n    //   );\n    // }\n  }\n};\n</script>\n\n<style scoped>\n.el-divider {\n  margin-top: 10px;\n}\n.color-circle {\n  display: inline-block;\n  width: 40px;\n  height: 40px;\n  border-radius: 1000px;\n  box-shadow: 0px 3px 6px rgba(0, 0, 0, 0.16);\n  background: white;\n  margin-top: 19px;\n}\n.modal-image-div {\n  width: 300px;\n  height: 300px;\n  background-size: cover;\n}\n.row-headers {\n  padding-bottom: 10px;\n  font-size: 14px;\n  text-transform: uppercase;\n  font-weight: 600;\n  color: #757575;\n}\n.add-item {\n  display: flex;\n  justify-content: center;\n  align-items: